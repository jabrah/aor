        <marginalia hand="English_secretary"> <!-- FSF: anchor per damnationem? -->
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Damnatio, vt hic capitur.</marginalia_text>
                </position>
            </language>
            <translation>Damnation, as it is captured here.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Et scias, quod non negatur transitus de geniture ad mensem, uel ad diem, sine anno, &amp; ita in reliquis, cum damnatio fuerit ualida, aliter non"> 
