                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Transitus alioq[ue] ordine profligato, no[n] admitteretur //.</marginalia_text>
                </position>
            </language>
            <translation>The crossing to another deprived order was not allowed.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary"> <!-- FSF: Anchor sub terra -->
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sub terra/.</marginalia_text>
                </position>
            </language>
            <translation>Under the earth.</translation>
        </marginalia>
        
