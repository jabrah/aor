    <annotation>
        
        <marginalia hand="Italian" anchor_text="Pleiades cum Luna">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ple&iuml;ades &Moon;</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The Pleiades [with] the moon.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tauro, leone, uel aquario">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Taurus; &Leo; &Aquarius;</marginalia_text>
                    <symbol_in_text name="Taurus"/>
                    <symbol_in_text name="Leo"/>
                    <symbol_in_text name="Aquarius"/>
                </position>
            </language>
            <translation>[From] Taurus, Leo, or Aquarius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Lunam; ex regijs">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Moon; cu[m] regijs</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The moon with regal things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Solem uel Lunam in lactae uia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Sun;:&Moon; in lact[ea] via.</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The sun and the moon in a pure path.</translation> <!-- FSF: I think lactea via is the Milky Way -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="caput Algol">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Caput Algol</marginalia_text>
                </position>
            </language>
            <translation>The head of Algol</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Figurae in coele fortes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Figura[e] fortes</marginalia_text>
                </position>
            </language>
            <translation>Powerful figures.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Figurae in coele fortes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fixaru[m] Natura[e]</marginalia_text>
                </position>
            </language>
            <translation>The nature of fixed [stars].</translation>
        </marginalia>
        
