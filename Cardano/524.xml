    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="524.tif" reader="John Dee" pagination="[252v]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="in corde Lunae">
           <language ident="LA">
               <position place="left_margin" book_orientation="0">
                   <marginalia_text>Planeta[e] vel fixae in Corde &Moon; vel solis.</marginalia_text>
                   <symbol_in_text name="Moon"/>
               </position>
           </language>
           <translation>The planets are either fixed in the heart of the moon or the sun.</translation>
       </marginalia> 
        
        <marginalia hand="English_secretary" anchor_text="eclipsis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Eclipsis</marginalia_text>
                </position>
            </language>
            <translation>Eclipse.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Mars facit in ascendente">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Mars; vel &Saturn; in asc.</marginalia_text>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>Mars or Saturn in the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="amicitia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Planetarum amicitia quibus modis conciliat[ur]</marginalia_text>
                </position>
            </language>
            <translation>By which way the friendship of the planets is acquired.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Iupiter; Venus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Jupiter;. &Venus; bonitas.</marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>The benevolence of Jupiter and Venus.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="amicitia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Eruditi malor[um] morum saepissi[m]e</marginalia_text>
                </position>
            </language>
            <translation>Most often of the tought evil habits.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Trinus omnis radius, bonum affert">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>No[n] tame[n] semper vide supra fo 86, v, linea 4.</marginalia_text>
                    <internal_ref>
                        <target ref="RCP7134:192:m0001" text="86, v"/>
                    </internal_ref>
                </position>
            </language>
            <translation>Yet not always, see above, folio 86 verso, line 4.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Trinus Saturni &amp; Iouis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Saturn; &Jupiter; &Trine;</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Trine"/>
                </position>
            </language>
            <translation>The trine of Saturn and Jupiter.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="amicitia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>propter &Saturn;' impuritate[m]</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>Because of Saturn's impurity.</translation> 
        </marginalia>
        
