<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="627.tif" reader="John Dee"  pagination="304"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Sun; in loco &Moon;a[e] radicis</marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The Sun in the place of the root of the Moon.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Moon; in [septima] [da] hora[e] casus</marginalia_text> <!--JG: Unsure about the word before 'horae'-->
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The moon in the seventh...</translation> <!--JG: To be finished..-->
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Sun; &Venus; &Mercury; in [octava].</marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>The Sun, Venus, and Mercury in the eight [place].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Asce[n]dit [octava] revolutio[n]is </marginalia_text>
                </position>
            </language>
            <translation>The eight of the revolution ascends.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mercury;; o[mn]i tempore infortunatus erat.</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>Venus was infortunate in all times.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>D[omi]na as. sub [re] radijs &Sun; ad [i]nfort[unit]ate[m] &Mercury;i.</marginalia_text> <!--JG: Unsure about 're' and 'infortunitatem'-->
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>The lady of the ascendant under the [..] rays of the Sun towards the misfortune of Mercury.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Signa inter[...] et mortis hoc anno. Octava radicis, as.</marginalia_text>
                </position>
            </language>
            <translation>The signs of [...] and death in this year. The eigth of the root, [ascendant].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Moon; in [sexta] radicis</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The Moon in the sixth of the root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Jupiter; in radice et revolutione in [duodecima].</marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>Jupiter in the root and revolution in the twelfth.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Square; &Moon; et &Sun; et &Moon; in sublimi loco</marginalia_text>
                    <symbol_in_text name="Square"/>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>A square of the Moon and the Sun, and the Moon in the highest place.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>et &Saturn; d[omi]n[u]s asce[n]de[n]t[i]s elevatus in figurali tetragono &Venus; in in ascendete.</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Saturn, the lord of the ascendant, having been elevated in a square shape; Venus in [a square shape] in the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Venus; d[omi]na octava[e] in asce[n]dente [pulsata] d[omi]no asce[n]dentis</marginalia_text>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Venus, the lady of the eight in the ascendant, beaten by the lord of the ascendant.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mars; circa [septimam] radicis</marginalia_text>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>Mars around the seventh of the root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mars; cadit in &Square;o radicalis veneris.</marginalia_text>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Square"/>
                </position>
            </language>
            <translation>Mars falls in the square of Venus's root.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Aquarius; etia[m] signu[m] profectionate est, et [octavii] radicis.</marginalia_text> <!--JG: Unsure about prot/fectionate..-->
                    <symbol_in_text name="Aquarius"/>
                </position>
            </language>
            <translation>Even Aquarius... </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ascendit quadrat[u]s Loci &Moon; radicis in 8.</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
        </marginalia>
        
        
        
        
    </annotation>
