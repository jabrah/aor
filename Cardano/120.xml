    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Harum igitur Alfridariarum, si genitura diurna sit; Si autem nocturna">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vide tabulas co[mmun]es Alfridariaru[m] pro diurnis et nocturnis genituris.</marginalia_text> 
                </position>
            </language>
            <translation>See the common tables of alfridaries for the daily and nocturnal genitures.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua r[ati]o[n]e plurimi senes velle sua[m] senecta[m] transiga[n]t</marginalia_text>  
                </position>
            </language>
            <translation>For what reason many old men finish to want their old age.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary"> <!-- FSF: possibly anchor domini, generalis & proprius -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Generale[m] intelligit septennij dominu[m] propriu[m] vocat [asseptum] sociu[m].</marginalia_text> 
                </position>
            </language>
            <translation>He understands the general master of the seven years. He calls...</translation> <!--JG: Unsure--> <!-- FSF: He calls him an encircled ally; I have no idea -->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Directio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Directio.</marginalia_text>
                </position>
            </language>
            <translation>Direction.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ingenij">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ingenium.</marginalia_text>
                </position>
            </language>
            <translation>Innate quality.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="honoris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Honor.</marginalia_text>
                </position>
            </language>
            <translation>Honour.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="laborum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Labor.</marginalia_text>
                </position>
            </language>
            <translation>Labour.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>procreatio liberor[um]</marginalia_text>
                </position>
            </language>
            <translation>The begetting of children.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuiti[a]e.</marginalia_text>
                </position>
            </language>
            <translation>Riches.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>morsus</marginalia_text> 
                </position>
            </language>
            <translation>Sting.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>eius anni</marginalia_text>
                </position>
            </language>
            <translation>Of this year.</translation> 
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Alfridariarum, si genitura diurna sit, primus annus Soli, secundus Veneri, tertius Mercurio, sic'q[ue] descendendo"/>
        <underline method="pen" type="straight" language="LA" text="sit: Si autem nocturna, primus Lunae, secundus Mercurio"/>
        <underline method="pen" type="straight" language="LA" text="sic ascendendo, donec Saturnus ultimus sit, dedicatur"/>
        <underline method="pen" type="straight" language="LA" text="rem ultimi climactrici, semper sunt infortunarum"/>
        <underline method="pen" type="straight" language="LA" text="utroq[ue] subscribente iam aetate frigida &amp; sicca, maxime periculosi sint. Si tamen Saturnus in nocturna, aut Mars in diurna dominentur principalibus uitae locis, nunq[uam] melius se habebunt, quam his temporibus"/>
        <underline method="pen" type="straight" language="LA" text="comes"/>
        <underline method="pen" type="straight" language="LA" text="Cum uero ambo domini, generalis &amp; proprius"/>
        <underline method="pen" type="straight" language="LA" text="dispositi fuerint, natus bene uiuet, etiam si directio mala occurrat, &amp; quae mortem significet, poterit enim anno integro produci uita"/>
        <underline method="pen" type="straight" language="LA" text="minimu[m] aute[m] in diuitijs"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="LA" text="utroque subscribente iam aetate frigida &amp; sicca, maxime periculosi sint. Si tamen Saturnus in nocturna, aut Mars in diurna dominentur principalibus uitae locis, nunq[uam] melius se habebunt, quam his temporibus"/>
        <mark name="bracket" method="pen" place="left_margin" language="LA" text="Cum uero ambo domini, generalis &amp; proprius, bene dispositi fuerint, natus bene uiuet, etiam si directio mala occurrat, &amp; quae mortem significet, poterit enim anno integro produci uita"/>
        <mark name="section_sign" method="pen" place="intext"/>
        
