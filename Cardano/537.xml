    <page filename="537.tif" reader="John Dee" pagination="259" signature="TT iij"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Operationes planetarum nunq[uam] cessant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quando operationes planetarum cessant et q[u]omodo cessant.</marginalia_text> 
                </position>
            </language>
            <translation>When the operations of the planets cease and how they cease.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Sol geminatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Solis geminatio vel triplicatio.</marginalia_text>
                </position>
            </language>
            <translation>The doubling or tripling of the sun.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="retrogradando separatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Separatio ex retrogradatione.</marginalia_text>
                </position>
            </language>
            <translation>The separation from retrogradation.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Hoc idem in alijs domibus significat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Regula.</marginalia_text>
                </position>
            </language>
            <translation>Rule.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Cum in genitura alicuius Sol geminatur, ut aliquando contingit ex ratione aspectus, ille seditionem mouebit aduersus principe[m], aut illustris erit, &amp; hoc cu[m] in eadem hora hoc acciderit"/>
        <underline method="pen" type="straight" language="LA" text="Hoc idem in alijs domibus significat"/>
        <underline method="pen" type="straight" language="LA" text="ascendunt, nec cadunt casu aliquo"/>
        
    </annotation>
