<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="125.tif" reader="John Dee" pagination="53" signature="O"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Caput Serpentarij">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Caput serpe[n]ta.</marginalia_text> <!--JG: Unsure, one expects 'serpentarii', also because serpenta doesn't exist--> <!-- FSF: I think this is abbreviated; there is a small little dot behind serpe[n]ta, I think -->
                </position>
            </language>
            <translation>Head of the serpent.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in collo Tauri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In collo &Taurus;.</marginalia_text>
                    <symbol_in_text name="Taurus"/>
                </position>
            </language>
            <translation>In the neck of Taurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Venus cu[m] Scorpij corde">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cor &Scorpio;. &Venus;.</marginalia_text>
                    <symbol_in_text name="Scorpio"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>The heart of Scorpio [and] Venus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Biothanatos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Biothanati.</marginalia_text>
                </position>
            </language>
            <translation>Those who die a violent death.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mercurij uel Martis uel Lunae, cu[m] Sole; Iouis &amp; Veneris.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mercury;. &Mars;. &Moon; &Jupiter;. &Venus; } cu[m] &Sun;.</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>Of Mercury, Mars, the moon, Judpiter, Venis, with the sun.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mercurius; Saturni &amp; Jovis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mercury;. &Mercury; &Mars;. &Mercury; &Saturn;.</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>Of Mercury, [or] of Mercury and Mars, [or] of Mercury and Saturn.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mercurius; Saturni &amp; Jovis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Saturn; &Jupiter; cu[m] &Mercury;</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>Saturn, Jupiter with Mercury.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="repentinam mortem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Repentina mors.</marginalia_text>
                </position>
            </language>
            <translation>A sudden death.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Iouis &amp;"/>
        <underline method="pen" type="straight" language="LA" text="cum luminosiore lancis meridionalis, mortem ex sanguinis p[er]fluuio denu[n]ciat, eade[m] stella in occasu, facit hemorragia[m]"/>
        
