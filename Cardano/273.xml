        <marginalia hand="English_secretary" anchor_text="Venus fuit Alfridariae domina">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Alfridaria[e] d[omi]n[u]s.</marginalia_text>
                </position>
            </language>
            <translation>The lord of the Alfridary.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="filiorum geniturae immutant parentum fortunam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
