    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="radij foelices luminarium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Luminarium radij &Trine; et &Star;</marginalia_text>
                </position>
            </language>
            <translation>The rays of light of a trine and sextile.</translation> <!--JG: Unsure how trine and sextile fit in this sentence.-->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="uitio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Oculoru[m] viti[um].</marginalia_text>
                </position>
            </language>
            <translation>The fault of the eyes.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="natus morietur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mors.</marginalia_text>
                </position>
            </language>
            <translation>Death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Cometae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cometa[e].</marginalia_text>
                </position>
            </language>
            <translation>Comets.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Quatuor cardines geniturae, sic quatuor elementis assimilantur, ut cui Taurus ascendit, plus terra, post aer domietur, cui Leo, plus ignis post terra, ac sic de alijs">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ex quatuor eleme[n]tis vt sum[us] co[m]positi et qu[a]e qualitas potissimu[m] dominetur.</marginalia_text>
                </position>
            </language>
            <translation>From four elements, as we have been composed et which quality may be mastered above all.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Hominu[m] concordi[a]e &amp; discordi[a]e.</marginalia_text>
                </position>
            </language>
            <translation>Men's agreements and disagreements.</translation>
        </marginalia>
        
