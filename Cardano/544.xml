    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="544.tif" reader="John Dee"  pagination="[262v]" signature="[VV ij[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Filiorum &amp; parentum dilectio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>filioru[m] et pare[n]tum dilectio.</marginalia_text>
                </position>
            </language>
            <translation>The love of children and parents.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Judiciu[m] ex c[oe]li imaginibus</marginalia_text>
                </position>
            </language>
            <translation>A judgement from the appearances of the sky.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mutatio augis Solis quid significet</marginalia_text>
                </position>
            </language>
            <translation>What the mutation of the sun's apogee may signify.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Metalla subsunt planetes.</marginalia_text>
                </position>
            </language>
            <translation>Metals are underneath the planets.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Metallica">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Metallica co[e]li partibus.</marginalia_text>
                </position>
            </language>
            <translation>Metals [are] in parts of the sky.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Lapides autem ac gemmae, tum herbarum ac arborum species, proculdubio subsunt fixis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Lapides ge[m]m[a]e herbaru[m] et arboru[m] spe[ci]es fixis subsunt.</marginalia_text>
                </position>
            </language>
            <translation>Stones [and] gems, species of herbs and trees, are beneath fixed places.</translation> <!--JG: Unsure--> <!-- FSF: I think this is fine -->
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="nam si dominus filiorum ad dominum geniturae in patris natiuitate accesserit"/>
        <underline method="pen" type="straight" language="LA" text="Cancro ad Libram, bene habitabitur Borealis, &amp; Australi dominabitur &agrave; Libra ad Capricornum, Austrina habitabitur, sed Boreali"/>
        <underline method="pen" type="straight" language="LA" text="Metallica autem, ut argentum uiuum, ac sulfur, &amp; calchantum, ac reliqua huius generis, partibus coeli, non autem"/>
        <underline method="pen" type="straight" language="LA" text="proculdubio subsunt fixis, in quibusdam tamen uirtus planetarum magis apparet"/>
        
        <mark name="bracket" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin"/>
        
    </annotation>
