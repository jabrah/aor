    <page filename="067.tif" reader="John Dee"  pagination="25"/>
    <annotation>
        
        <marginalia hand="Italian" method="pen" anchor_text="Mercurius; Saturno; Sole; Marte; Venere; Jove">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Mercury; cu[m] &Saturn; &Sun; &Mars; &Venus; &Jupiter;</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>Mercury with Saturn, the Sun, Mars, Venus, [and] Jupiter.</translation>
        </marginalia>
              
        <marginalia hand="Italian" method="pen" anchor_text="Luna; Saturno">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>[&Jupiter; - deleted] &Moon; &Saturn; </marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>Moon, Saturn.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen" anchor_text="Luna; Ioue">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Moon; &Jupiter; </marginalia_text>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>Moon, Jupiter.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen" anchor_text="Luna; Marte">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Moon; &Mars; </marginalia_text>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>Moon, Mars.</translation>
        </marginalia>
        
        <marginalia hand="Italian" method="pen" anchor_text="Luna; Marte">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cor &Scorpio;.&Moon;. </marginalia_text>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Scorpio"/>
                </position>
            </language>
            <translation>Heart, Scorpio, moon.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="cu[m] Saturno significat uenena, ac"/>
        <underline method="pen" type="straight" language="LA" text="Cum Venere, &amp; Ioue, non bene sociatur"/>
        <underline method="pen" type="straight" language="LA" text="dignitates eius sunt, tabellionatus, &acirc; secretis"/>
        <underline method="pen" type="straight" language="LA" text="apparitiones igneas"/>
        <underline method="pen" type="straight" language="LA" text="non est amicus uitae"/>
        <underline method="pen" type="straight" language="LA" text="limphaticos proprie cum Saturno: est"/>
        <underline method="pen" type="straight" language="LA" text="plurimas autem cu[m] Marte ostendit infoelicitates"/>
        <underline method="pen" type="straight" language="LA" text="cingulus Orionis"/>
        
    </annotation>
