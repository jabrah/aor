    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Saturnus in duodecimo loco">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Saturn; in 6&o_super;. 12&o_super;</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>Saturn in the sixth and twelfth [place].</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="nutrietur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nutritionis jndicium.</marginalia_text>
                </position>
            </language>
            <translation>The proof of nutrition.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="revolutio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Revolutionum varij modi.</marginalia_text>
                </position>
            </language>
            <translation>Various kinds of revolutions.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hinc patet cur cuilib[et] parti, 1 annu[m], dies 5 horas 9 attribuim[us]</marginalia_text> <!--JG: Not sure whether it's 5 or 4 dies-->
                </position>
            </language>
            <translation>Here it is well known why we attribute 1 year, 5 days, and eight hour to any part.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Luna">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quando &Moon; in sua auge vel opposito</marginalia_text> <!--JG: Unsure-->
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>When the moon is in its apogee or opposition.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="revolutio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Saturn; &Jupiter; &Mars; &Venus; &Mercury; &Moon; } ecce[n]trici r[ati]o.</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>Saturn, Jupiter, Mars, Venus, Mercury, and the moon: the reason for an eccentric orbit.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Iupiter, aut Venus in medio duarum infortunarum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Jupiter; vel &Venus; obsessa a malis planetis.</marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Jupiter or Venus, having been beset by bad planets.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Mars; &Jupiter; &Saturn;. &Mars; &Venus; &Saturn;. &Mars; &Venus; &Mercury;. &Saturn; &Venus; &Mercury;.</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                    <symbol_in_text name="Jupiter"/>
                    <symbol_in_text name="Mars"/>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>Mars, Jupiter, Saturn; Mars, Venus, Saturn; Mars, Venus, Mercury; Saturn, Venus, Mercury.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fortuna[e] infortunati.</marginalia_text>
                </position>
            </language>
            <translation>Fortunate to infortunate.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>pro aere et fru[c]tib[us] [etc].</marginalia_text> <!--JG: Not sure abt 'fructibus'-->
                </position>
            </language>
            <translation>For the air and fruits, et cetera.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="addita tanta parte eclipticae, quanta[m] peragrare potest in una die naturali">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>cur adderetur illa pars?</marginalia_text>
                </position>
            </language>
            <translation>Why should that part be added?</translation>
        </marginalia>
        
