    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="directionibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Directio</marginalia_text>
                </position>
            </language>
            <translation>Direction.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mors accidit uiolenta, Mars aut Venus ascendenti">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mors violenta a &Venus; et &Mars;.</marginalia_text>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>A violent death by Venus and Mars.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cadente Marte, natus erit timidus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Audacia eni[m] a marte pendet.</marginalia_text>  
                </position>
            </language>
            <translation>For audacity weighs out from Mars.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Mars in octauo loco">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Mars; in 8</marginalia_text>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>Mars in the eigth [place].</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="proditores">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Proditores.</marginalia_text>
                </position>
            </language>
            <translation>Traitors.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Sicut superiores orientales descendunt in paruo circulo, occidentales ascendunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quando superiores ascendunt vel desce[n]du[n]t in parvo circulo.</marginalia_text>
                </position>
            </language>
            <translation>When the superior [planets] ascend or descend in a small circle.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quibus mors accidit uiolenta"/>
        <underline method="pen" type="straight" language="LA" text="Venus &amp; Luna &amp; Saturnus"/>
