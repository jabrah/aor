        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text> 
                        Scipio Africanus reus.
                    </marginalia_text>
                <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>
                Scipio Africanus as defendant
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="orationem ade&ograve; magnificam de rebus se gestis est exorsus"/>
        <underline method="pen" type="straight" language="LA" text="neq[ue] uerius laudatum esse."/>
        <underline method="pen" type="straight" language="LA" text="ab eodem animo ingenioq[ue] &agrave; quo gesta erant;"/>
        <underline method="pen" type="straight" language="LA" text="quia pro periculo"/>
        <underline method="pen" type="straight" language="LA" text="non in gloriam referebantur"/>
        <underline method="pen" type="straight" language="LA" text="uetera luxuriae crimina"/>
        <underline method="pen" type="straight" language="LA" text="Syracusanorum hybernorum"/>
        <underline method="pen" type="straight" language="LA" text="Locris Pleminianum tumultum"/>
        <underline method="pen" type="straight" language="LA" text="suspicionibus magis"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m argumentis"/>
        <underline method="pen" type="straight" language="LA" text="pecuniae captae reum accusarunt"/>
        <underline method="pen" type="straight" language="LA" text="Filium captum"/>
        <underline method="pen" type="straight" language="LA" text="sine precio Romana bellumq[ue] esset"/>
        <underline method="pen" type="straight" language="LA" text="Antiocho cultum"/>
        <underline method="pen" type="straight" language="LA" text="dictatorem eum"/>
        <underline method="pen" type="straight" language="LA" text="non legatum"/>
        <underline method="pen" type="straight" language="LA" text="ad aliam rem e&ograve; profectum"/>
        <underline method="pen" type="straight" language="LA" text="id quod Hispaniae, Galliae, Siciliae, Africae iampridem persuasum esset"/>
        <underline method="pen" type="straight" language="LA" text="Graeciae Asiaeq[ue] &amp; omnibus ad orientem uersis regibus gentibusq[ue] appareret"/>
        <underline method="pen" type="straight" language="LA" text="unum hominem"/>
        <underline method="pen" type="straight" language="LA" text="caput colummenq[ue] imperij Romani esse"/>
        <underline method="pen" type="straight" language="LA" text="umbra Scipionis ciuitatem"/>
        <underline method="pen" type="straight" language="LA" text="dominam orbis terrarum lat&ecirc;re"/>
        <underline method="pen" type="straight" language="LA" text="nutus eius pro decretis patru[m]"/>
        <underline method="pen" type="straight" language="LA" text="pro populi iussis esse."/>
        <underline method="pen" type="straight" language="LA" text="Infamia intactum"/>
        <underline method="pen" type="straight" language="LA" text="inuidia qua possunt"/>
        <underline method="pen" type="straight" language="LA" text="citatus reus magno"/>
        <underline method="pen" type="straight" language="LA" text="agmine amicorum clientiumq[ue] per mediam"/>
        <underline method="pen" type="straight" language="LA" text="ad Rostra subijt"/>
        <underline method="pen" type="straight" language="LA" text="silentioq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="Hoc"/>
        <underline method="pen" type="straight" language="LA" text="die tribuni"/>
        <underline method="pen" type="straight" language="LA" text="cum Annibale &amp; Carthaginiensibus"/>
        <underline method="pen" type="straight" language="LA" text="Africa bene ac foeliciter pugnaui"/>
        <underline method="pen" type="straight" language="LA" text="litibus &amp; iurgijs superseder i aequum"/>
        <underline method="pen" type="straight" language="LA" text="ego hinc extempl&ograve; in Capitolium ad Iouem"/>
        <underline method="pen" type="straight" language="LA" text="salutandos ibo"/>
        <underline method="pen" type="straight" language="LA" text="gratias agam, qu&ograve;d mihi &amp; hoc ipso die, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="egregi&egrave; reip. gerendae mentem facult atemq[ue] dederu[n]t"/>
        <underline method="pen" type="straight" language="LA" text="uestr&ucirc;m quoq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="se uniuersa co[n]cio auertitite mecum"/>
        <underline method="pen" type="straight" language="LA" text="mei similes principes habeatis"/>
        <underline method="pen" type="straight" language="LA" text="ab annis septemdecim ad senectutem"/>
        <underline method="pen" type="straight" language="LA" text="semper uos aetate[m] mea[m] honoribus uestris anteistis"/>
        <underline method="pen" type="straight" language="LA" text="uestros honores rebus"/>
        <underline method="pen" type="straight" language="LA" text="gerendis pr[a]ecessi"/>
        <underline method="pen" type="straight" language="LA" text="Capitolium ascendit"/>
        <underline method="pen" type="straight" language="LA" text="secuta Scipionem"/>
        <underline method="pen" type="straight" language="LA" text="postrem&ograve; scribae uiatoresq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="tribunos relinquerent"/>
        <underline method="pen" type="straight" language="LA" text="ijs praeter seruilem comitatu[m], &amp; praeconem, qui"/>
        <underline method="pen" type="straight" language="LA" text="Scipio non in Capitolio"/>
        <underline method="pen" type="straight" language="LA" text="totam urbem"/>
        <underline method="pen" type="straight" language="LA" text="omnia templa de&ucirc;m"/>
        <underline method="pen" type="straight" language="LA" text="Celebratior is prop&egrave; dies fauore hominu[m], &amp; aestimatione uerae"/>
        <underline method="pen" type="straight" language="LA" text="magnitudinis eius fuit"/>
        <underline method="pen" type="straight" language="LA" text="triumphans de Syphace rege, &amp; Carthaginiensibus"/>
        <underline method="pen" type="straight" language="LA" text="Hic speciosus ultimus dies P. Scipioni illuxit"/>
        <underline method="pen" type="straight" language="LA" text="cum inuidiam &amp; certamina cum tribunis"/>
        <underline method="pen" type="straight" language="LA" text="die longiore producta"/>
        <underline method="pen" type="straight" language="LA" text="Literniu[m] concessit"/>
        <underline method="pen" type="straight" language="LA" text="ne ad causam dicendam"/>
        <underline method="pen" type="straight" language="LA" text="maior animus &amp; natura erat, ac maiori fortunae assuetus"/>
        <underline method="pen" type="straight" language="LA" text="ut reus esse sciret, &amp; summitttere se in humilitatem causam dicentium"/>
        <underline method="pen" type="straight" language="LA" text="citariq[ue] absens est"/>
        <underline method="pen" type="straight" language="LA" text="L. Scipio morbu[m] causae esse"/>
        <underline method="pen" type="straight" language="LA" text="excusationem"/>
        <underline method="pen" type="straight" language="LA" text="qui diem dixera[n]t, non acciperent"/>
        <underline method="pen" type="straight" language="LA" text="ab eadem superbia non uenire ad causam dicendam"/>
        <underline method="pen" type="straight" language="LA" text="iudicium"/>
        <underline method="pen" type="straight" language="LA" text="tribunos pl. &amp; concionem"/>
        <underline method="pen" type="straight" language="LA" text="quibus ius de se dicendae sente[n]tiae &amp; libertatem"/>
        <underline method="pen" type="straight" language="LA" text="his comitatus"/>
        <underline method="pen" type="straight" language="LA" text="uelut captos trahens, triumphum de PR"/>
        <underline method="pen" type="straight" language="LA" text="secessionemq[ue] eo die in Capitolium"/>
        <underline method="pen" type="straight" language="LA" text="temeritatis illius mercedem"/>
        <underline method="pen" type="straight" language="LA" text="quo duce &amp; autore nos reliquistis"/>
        <underline method="pen" type="straight" language="LA" text="eo ipsi relicti estis."/>
        <underline method="pen" type="straight" language="LA" text="ad quem"/>
        <underline method="pen" type="straight" language="LA" text="exercitum &amp; classem habente[m] tribunos pleb is aedilemq[ue] in Siciliam"/>
        <underline method="pen" type="straight" language="LA" text="qui prehe[n]derent eum, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="eum priuatum ex uilla sua extrahendum ad causam"/>
        <underline method="pen" type="straight" language="LA" text="non audeamus"/>
        <underline method="pen" type="straight" language="LA" text="Tribuni pl. appellati ab L. Scipione"/>
        <underline method="pen" type="straight" language="LA" text="decreueru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="morbi causa excusaretur"/>
        <underline method="pen" type="straight" language="LA" text="sibi placere accipi eam"/>
        <underline method="pen" type="straight" language="LA" text="diemq[ue] &agrave; collegis produci"/>
        <underline method="pen" type="straight" language="LA" text="Tribunus ple."/>
        <underline method="pen" type="straight" language="LA" text="Tib. Sempronius Gracchus"/>
        <underline method="pen" type="straight" language="LA" text="cui inimicitiae cum P. Scipione"/>
        <underline method="pen" type="straight" language="LA" text="uetuisset nomen suum decreto collegaru[m] ascribi"/>
        <underline method="pen" type="straight" language="LA" text="L. Scipio excuset morbum esse"/>
        <underline method="pen" type="straight" language="LA" text="id sibi uideri, se P. Scipionem"/>
        <underline method="pen" type="straight" language="LA" text="accusari non passurum"/>
        <underline method="pen" type="straight" language="LA" text="se appellet, auxilio ei futurum ne causam dicat"/>
        <underline method="pen" type="straight" language="LA" text="id sastigium rebus gestis"/>
        
