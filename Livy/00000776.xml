        <underline method="pen" type="straight" language="LA" text="Attalum"/>
        <underline method="pen" type="straight" language="LA" text="regis Eumenis fratrem legatu[m] uenisse"/>
        <underline method="pen" type="straight" language="LA" text="ipsum Eumenem uenisse"/>
        <underline method="pen" type="straight" language="LA" text="Eumenes igitur ut Romam uenit, exceptus cum tanto honore,"/>
        <underline method="pen" type="straight" language="LA" text="meritis tantum eius"/>
        <underline method="pen" type="straight" language="LA" text="sed beneficijs etiam suis, ingentia quae in"/>
        <underline method="pen" type="straight" language="LA" text="Causam ueniendi sibi"/>
        <underline method="pen" type="straight" language="LA" text="praeter cupiditatem uisendi deos, hominesq[ue] quorum"/>
        <underline method="pen" type="straight" language="LA" text="supra quam ne optare quidem"/>
        <underline method="pen" type="straight" language="LA" text="Persei conatis obuiam iret"/>
        <underline method="pen" type="straight" language="LA" text="Persea haereditarium"/>
        <underline method="pen" type="straight" language="LA" text="Quae cum corporis robore ac uiribus uigeat"/>
        <underline method="pen" type="straight" language="LA" text="animum esse inueteratum diutina arte atque usu belli."/>
        <underline method="pen" type="straight" language="LA" text="Romanis quoque bellis, non finitimis"/>
        <underline method="pen" type="straight" language="LA" text="expeditiones multas, uarias que."/>
        <underline method="pen" type="straight" language="LA" text="uereri maiestatem eius"/>
        <underline method="pen" type="straight" language="LA" text="His eum fultum societatibus atque amicitijs"/>
        <underline method="pen" type="straight" language="LA" text="eos domesticos apparatus belli habere, ut externis non egeat"/>
        <underline method="pen" type="straight" language="LA" text="XXX milia peditum"/>
        <underline method="pen" type="straight" language="LA" text="V millia equitum"/>
        <underline method="pen" type="straight" language="LA" text="in decem"/>
        <underline method="pen" type="straight" language="LA" text="annos frumentum praeparare, ut"/>
        <underline method="pen" type="straight" language="LA" text="pecuniam tantam"/>
        <underline method="pen" type="straight" language="LA" text="decem millia mercenariorum"/>
        <underline method="pen" type="straight" language="LA" text="stipendium in totidem annos praeparatum"/>
        <underline method="pen" type="straight" language="LA" text="praeter annuum"/>
        <underline method="pen" type="straight" language="LA" text="metallis regijs capiat uectigal."/>
        <underline method="pen" type="straight" language="LA" text="Arma uel tribus tantum"/>
        <underline method="pen" type="straight" language="LA" text="Iuuentutem, ut iam"/>

