        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Privatarum rationum respectus semper officit consiliis publicis.
                        Oeconomica argumenta vincunt politica. Maximus semper consiliorum Elenchus.
                    </marginalia_text>
                </position>
            </language>
            <translation>Regard for personal considerations always hinders public counsels.
                Domestic-economical arguments triumph over political. Always the strongest argument against counsels.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="negare ultra decipi"/>
        <underline method="pen" type="straight" language="LA" text="ni praestaretur fides publica"/>
        <underline method="pen" type="straight" language="LA" text="libetertatem unicuiq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="patria ciuibusq[ue], non pro dominis pugnent"/>
        <underline method="pen" type="straight" language="LA" text="intra parietes curiae ferociter"/>
        <underline method="pen" type="straight" language="LA" text="prius"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m ultima experirentur, senatum iteru[m] consulere"/>
        <underline method="pen" type="straight" language="LA" text="acerrimi cuiusq[ue] arbitrio"/>
        <underline method="pen" type="straight" language="LA" text="placet, rem agemus"/>
        <underline method="pen" type="straight" language="LA" text="nihilo plus sanitatis in curia, qu&agrave;m in foro esse"/>
        <underline method="pen" type="straight" language="LA" text="dictatore[m], &agrave; quo prouocatio no[n] est, creemus"/>
        <underline method="pen" type="straight" language="LA" text="hic, quo nunc omnia ardent, conticescet furor"/>
        <underline method="pen" type="straight" language="LA" text="mihi lictore[m], qui sciet ius"/>
        <underline method="pen" type="straight" language="LA" text="de tergo uitaq[ue] sua penes unum illum esse, cuius maiestatem uiolauit"/>
        <underline method="pen" type="straight" language="LA" text="medium maxim&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="moderatum utroq[ue] consilium Virginij habebatur"/>
        <underline method="pen" type="straight" language="LA" text="factione, respectu[m]q[ue] rerum priuatarum"/>
        <underline method="pen" type="straight" language="LA" text="quae semper afficere, officientq[ue] publicis consilijs"/>
        <underline method="pen" type="straight" language="LA" text="imperium suo uehemens mansueto"/>
        
