        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Strategema versatum, atq[ue] pestilens: ingentis momenti facinus: siquidem victor / sapuisset.
                    </marginalia_text>
                </position>
            </language>
            <translation>A skilled and unwholesome ploy: a deed of great importance, if, at any rate, the conqueror would have had good sense.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Iustum est bellum Samnites"/>
        <underline method="pen" type="straight" language="LA" text="quibus necessarium"/>
        <underline method="pen" type="straight" language="LA" text="pia arma, quibus nulla nisi in armis relinquitur spes"/>
        <underline method="pen" type="straight" language="LA" text="exercitu educto circa Caudium castra"/>
        <underline method="pen" type="straight" language="LA" text="potest occultissim&egrave; locat"/>
        <underline method="pen" type="straight" language="LA" text="milites decem pastorum habitu mittit"/>
        <underline method="pen" type="straight" language="LA" text="pecoraq[ue] diuersos alium alibi"/>
        <underline method="pen" type="straight" language="LA" text="ubi inciderint in praedatores, ut idem omnibus sermo constet"/>
        <underline method="pen" type="straight" language="LA" text="Luceriam omnibus"/>
        <underline method="pen" type="straight" language="LA" text="etiam rumor ant&egrave; de industria"/>
        <underline method="pen" type="straight" language="LA" text="fidem auxere captiui"/>
        <underline method="pen" type="straight" language="LA" text="maxim&egrave;, qu&ograve;d sermo inter omnes"/>
        <underline method="pen" type="straight" language="LA" text="Duae ad Luceriam ferebant uiae"/>
        <underline method="pen" type="straight" language="LA" text="quanto tutior, tanto fer&egrave; lo[n]gior"/>
        <underline method="pen" type="straight" language="LA" text="altera per Furculas Caudinas"/>
        <underline method="pen" type="straight" language="LA" text="breuior"/>
        <underline method="pen" type="straight" language="LA" text="cum fraus hostilis apparuisset"/>
        <underline method="pen" type="straight" language="LA" text="praesidium etiam in summo saltu conspicitur"/>
        <underline method="pen" type="straight" language="LA" text="citati inde"/>
        <underline method="pen" type="straight" language="LA" text="pergunt repetere uiam"/>
        <underline method="pen" type="straight" language="LA" text="quoq[ue] clausam sua obice, armisq[ue] inueniunt"/>
        <underline method="pen" type="straight" language="LA" text="stupor que omnium animos"/>
        <underline method="pen" type="straight" language="LA" text="intuentesq[ue] alij alios, cum alterum quisque"/>
        <underline method="pen" type="straight" language="LA" text="magis mentis compote[m]"/>
        <underline method="pen" type="straight" language="LA" text="quanquam ludibrio fore munientes perditis rebus, ac spe omni adempta"/>
        <underline method="pen" type="straight" language="LA" text="ne culpam malis adderent"/>
        <underline method="pen" type="straight" language="LA" text="pro se quisque"/>
        <underline method="pen" type="straight" language="LA" text="ad muniendum uersi"/>
        <underline method="pen" type="straight" language="LA" text="castra propter"/>
        <underline method="pen" type="straight" language="LA" text="sua ipse opera, laboremq[ue] irritum, praeterquam"/>
        <underline method="pen" type="straight" language="LA" text="cum miserabili confessione eludentes"/>
        <underline method="pen" type="straight" language="LA" text="quando nec consilio nec auxilio locus esset"/>
        <underline method="pen" type="straight" language="LA" text="militesq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="uersi, opem, quam uix dij"/>
        <underline method="pen" type="straight" language="LA" text="immortales ferre poterant, ab ducibus exposcunt"/>
        <underline method="pen" type="straight" language="LA" text="Querentes magis, qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="cum pro ingenio quisque fremerent"/>
        <underline method="pen" type="straight" language="LA" text="mod&ograve; ad hostem"/>
        <underline method="pen" type="straight" language="LA" text="omnia aequa ac plana erunt Romano in perfidum Samnitem pugnanti"/>
        <underline method="pen" type="straight" language="LA" text="Qu&ograve;, aut qu&agrave; eamus"/>
        
