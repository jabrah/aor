    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="Nebula"/>
        <underline method="pen" type="straight" language="LA" text="uelut latebra insidiarum freti"/>
        <underline method="pen" type="straight" language="LA" text="Improuiso oppressis, nec animi satis ad resistendum, nec uirium fuit"/>
        <underline method="pen" type="straight" language="LA" text="captum qu[a]estoriu[m], quaestorq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="co[n]clamatu[m] inde ad arma"/>
        <underline method="pen" type="straight" language="LA" text="dum satis aptatis"/>
        <underline method="pen" type="straight" language="LA" text="in ordines"/>
        <underline method="pen" type="straight" language="LA" text="clamore magis"/>
        <underline method="pen" type="straight" language="LA" text="expulsi ne extra uallu[m]"/>
        <underline method="pen" type="straight" language="LA" text="castra deinde sua oppugnaturi"/>
        <underline method="pen" type="straight" language="LA" text="impulsos semel terrore eodem agunt, quo coeperunt"/>
        <underline method="pen" type="straight" language="LA" text="expellu[n]t extra"/>
        <underline method="pen" type="straight" language="LA" text="quia turbida lux metum circ&agrave; insidiarum faciebat"/>
        <underline method="pen" type="straight" language="LA" text="non ausi, libertatis"/>
        <underline method="pen" type="straight" language="LA" text="Animos inde Samnitibus non infelix audacia auxit"/>
        <underline method="pen" type="straight" language="LA" text="mod&ograve; proferre inde castra"/>
        <underline method="pen" type="straight" language="LA" text="ne pabulari quidem"/>
        <underline method="pen" type="straight" language="LA" text="perlata Romam"/>
        <underline method="pen" type="straight" language="LA" text="aedem Victoriae, quam"/>
        <underline method="pen" type="straight" language="LA" text="dedicauit"/>
        <underline method="pen" type="straight" language="LA" text="diffisi duobus exercitib[us] resisti"/>
        <underline method="pen" type="straight" language="LA" text="Coss. ad uastandos agros, urbesq[ue] oppugnandas discedunt"/>
        <underline method="pen" type="straight" language="LA" text="Posthumius"/>
        <underline method="pen" type="straight" language="LA" text="ui prim&ograve;, atq[ue] impetu"/>
        <underline method="pen" type="straight" language="LA" text="opere ac uineis demum"/>
        <underline method="pen" type="straight" language="LA" text="ut sile[n]tiu[m] uastu[m]"/>
        <underline method="pen" type="straight" language="LA" text="auidu[m] inuade[n]di deserta moenia milite[m]"/>
        <underline method="pen" type="straight" language="LA" text="ne qua[m] occulta[m] in fraude[m] incautus rueret"/>
        <underline method="pen" type="straight" language="LA" text="circu[m]equitare moenia, atq[ue] explorare omnia iubet"/>
        <underline method="pen" type="straight" language="LA" text="ijs uestigia nocturn[a]e hostiu[m] fugae, adequita[n]t"/>
        <underline method="pen" type="straight" language="LA" text="solitudine"/>
        <underline method="pen" type="straight" language="LA" text="uestigijs fug[a]e, ac strage reru[m] in"/>
        <underline method="pen" type="straight" language="LA" text="quinq[ue] equites iubet intrare urbe[m] &amp; modicu[m] spaciu[m]"/>
        <underline method="pen" type="straight" language="LA" text="tres manere"/>
        <underline method="pen" type="straight" language="LA" text="duos explorata ad se referre"/>
        <underline method="pen" type="straight" language="LA" text="progressos, unde in omnes partes circu[m]spectus"/>
        <underline method="pen" type="straight" language="LA" text="cohortes expeditas in urbe[m]"/>
        <underline method="pen" type="straight" language="LA" text="paucos graues aetate, aut inualidos"/>
        <underline method="pen" type="straight" language="LA" text="co[m]muni"/>
        <underline method="pen" type="straight" language="LA" text="urbes co[n]cisse fuga[m], suos"/>
        <underline method="pen" type="straight" language="LA" text="prima uigilia p[er]fectos, credere ea[n]de[m] "/>
        <underline method="pen" type="straight" language="LA" text="urbibu[us] solitudine[m] inue[n]turos"/>
        <underline method="pen" type="straight" language="LA" text="captiuoru[m] fides extitit"/>
        <underline method="pen" type="straight" language="LA" text="desertis oppidis"/>
        
