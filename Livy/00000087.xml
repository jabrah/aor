        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ca[e]so, a lustie and dowtie fellow. a terrible killcowe in his kinde.
                    </marginalia_text>
                    <person name="Caeso Quinctius"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
