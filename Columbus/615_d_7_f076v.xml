       <underline method="pen" type="straight" language="IT" text="Nostro Signore non permetterebbe mai"/>
        <underline method="pen" type="straight" language="IT" text="cosa di tanta essaltatione della sua Chiesa"/>
        <underline method="pen" type="straight" language="IT" text="al fin con tante contrariet&agrave;"/>
        <underline method="pen" type="straight" language="IT" text="per la qual cosa io scrissi in vna pergamena"/>
        <underline method="pen" type="straight" language="IT" text="quella breuit&agrave;, che il tempo ricercaua"/>
        <underline method="pen" type="straight" language="IT" text="sciaua scoperte quelle terre"/>
        <underline method="pen" type="straight" language="IT" text="messe"/>
        <underline method="pen" type="straight" language="IT" text="quanti d&igrave;"/>
