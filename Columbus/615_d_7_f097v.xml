        <underline method="pen" type="straight" language="IT" text="chiamarono S. Martino"/>
        <underline method="pen" type="straight" language="IT" text="cauauano pezzi di corallo"/>
        <underline method="pen" type="straight" language="IT" text="per andare &agrave; soccorrer quelli, ch'egli haueua"/>
        <underline method="pen" type="straight" language="IT" text="che si prendesse"/>
        <underline method="pen" type="straight" language="IT" text="per saper"/>
        <underline method="pen" type="straight" language="IT" text="quattro donne"/>
        <underline method="pen" type="straight" language="IT" text="tre fanciulli"/>
        <underline method="pen" type="straight" language="IT" text="che la donna pa&sharp-S;&ograve;"/>
        <underline method="pen" type="straight" language="IT" text="targa dall'vn lato all'altro"/>
        <underline method="pen" type="straight" language="IT" text="tiraua di molte freccie"/>
        <underline method="pen" type="straight" language="IT" text="fosse stato in terra"/>
        <underline method="pen" type="double" language="IT" text="e poi castrati"/>
        <underline method="pen" type="straight" language="IT" text="ingrassino"/>
        <underline method="pen" type="double" language="IT" text="siano piu saporiti"/>
        
        
        
