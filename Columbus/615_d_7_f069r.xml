        <underline method="pen" type="straight" language="IT" text="piu dolce e mansueto del mondo, allegro, e sempre accompagnato da"/>
        <underline method="pen" type="straight" language="IT" text="ma per&ograve; credano le Altezze"/>
        <underline method="pen" type="straight" language="IT" text="che hanno costumi molto lodeuoli"/>
        <underline method="pen" type="straight" language="IT" text="&egrave; seruito con grande Maest&agrave;"/>
        <underline method="pen" type="straight" language="IT" text="continente, che porge gran diletto"/>
        <underline method="pen" type="straight" language="IT" text="la memoria di questo"/>
        <underline method="pen" type="straight" language="IT" text="il desiderio di sapere ogni cosa"/>
        <underline method="pen" type="straight" language="IT" text="la causa, e l'effetto del"/>
        <underline method="pen" type="straight" language="IT" text="Re principale"/>
        <underline method="pen" type="straight" language="IT" text="offerendogli liberalmente tutto quello, che del suo li piacesse riceuere ; &amp; dicendo, che gi&agrave; haueua donate tre case a' Christiani"/>
        <underline method="pen" type="straight" language="IT" text="mettessero tutto quello, che della naue"/>
        <underline method="pen" type="double" language="IT" text="cauassero"/>
        <underline method="pen" type="straight" language="IT" text="foglie d'oro"/>
        <underline method="pen" type="double" language="IT" text="sonagli"/>
        
        
        
        
        
        
        
        
        
        
        
        
