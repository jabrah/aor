        <underline method="pen" type="double" language="IT" text="castigo"/>
        <underline method="pen" type="straight" language="IT" text="mento sarebbe bastato per fermar la gente"/>
        <underline method="pen" type="double" language="IT" text="a scorrere i luoghi"/>
