    <annotation>
        
        <marginalia hand="Italian" anchor_text="I quali, intesa la buona relatione, che questi gli fecero, tutti con le lor Canoe uennero a' nauigli a barattar di quelle cose">
            <language ident ="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Policie, for credit noynning</marginalia_text> 
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="che nelle altre Isole, prima scoperte">
            <language ident ="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Note</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="auuelenata per le freccie, la qual que sti non usano, ma i Caribi">
            <language ident ="IT">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>I Caribi soli hanno le frecie avelanate</marginalia_text>
                </position>
            </language>
            <translation>The Kalinago just have poisoned arrows.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="panni ben tessuti">
            <language ident ="IT">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Panni ben tessuti</marginalia_text>
                </position>
            </language>
            <translation>Well woven cloths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="La gente">
            <language ident ="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Note the people</marginalia_text>
                </position>
            </language>
        </marginalia>
        
