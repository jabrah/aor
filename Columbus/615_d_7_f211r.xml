        <underline method="pen" type="straight" language="IT" text="volte sparar qualche pezzo di artiglieria: al qual"/>
        <underline method="pen" type="straight" language="IT" text="haueano paura del suo romore, perche in effetto pensauano, che sol fossero tuoni per cagionare spauento"/>
        <underline method="pen" type="straight" language="IT" text="hauessero tanta superbia, ne sprezzassero i Christiani"/>
        <underline method="pen" type="straight" language="IT" text="fece loro conoscere, che quella burla era cosi fulmine, come tuono"/>
        <underline method="pen" type="straight" language="IT" text="paese la piu disposta, che fino"/>
        <underline method="pen" type="straight" language="IT" text="no alti, &amp; asciutti"/>
        <underline method="pen" type="straight" language="IT" text="alcuna go[n]fiezza"/>
        <underline method="pen" type="straight" language="IT" text="musco del mondo"/>
        <underline method="pen" type="straight" language="IT" text="tanto carnefici, &amp; crudeli, che prendono vn'huomo"/>
        
