        <underline method="pen" type="straight" language="IT" text="Manicauter"/>
        <underline method="pen" type="straight" language="IT" text="tenea vn suo figliuolo, &amp; vn nepote appresso di"/>
        <underline method="pen" type="straight" language="IT" text="percioche peso non hebbero mai."/>
        <underline method="pen" type="straight" language="IT" text="cosi diuisi i"/>
        
