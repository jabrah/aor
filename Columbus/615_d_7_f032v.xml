    <annotation>
        
        <underline method="pen" type="straight" language="IT" text="Ma, percioche la cosa douea"/>
        <underline method="pen" type="double" language="IT" text="trattarsi pi&ugrave; con fondamento di dottrina, che"/>
        <underline method="pen" type="double" language="IT" text="parole, o fauori"/>
        <underline method="pen" type="straight" language="IT" text="intendere, che gli auuenisse quel, che in Portogallo"/>
        <underline method="pen" type="straight" language="IT" text="auuenne, &amp; gli rubassero la benedittione"/>
        <underline method="pen" type="double" language="IT" text="non era verisimile"/>
        
        
