        <underline method="pen" type="straight" language="IT" text="occhi, &amp; con le orecchie"/>
        <underline method="pen" type="straight" language="IT" text="Duchi"/>
        <underline method="pen" type="straight" language="IT" text="fatti i nostri"/>
        <underline method="pen" type="double" language="IT" text="sedere"/>
        <underline method="pen" type="straight" language="IT" text="venuano poi a baciar loro i piedi, &amp; le mani"/>
