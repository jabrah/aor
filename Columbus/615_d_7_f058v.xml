        <underline method="pen" type="straight" language="IT" text="pregandoli molto a voler"/>
        <underline method="pen" type="straight" language="IT" text="rimanere in quel luogo appresso di loro"/>
        <underline method="pen" type="straight" language="IT" text="si riposassero per cinque"/>
        <underline method="pen" type="straight" language="IT" text="i due Indiani"/>
        <underline method="pen" type="double" language="IT" text="menati"/>
        <underline method="pen" type="double" language="IT" text="diceuano lor"/>
        <underline method="pen" type="straight" language="IT" text="gran ben de' Christiani"/>
        <underline method="pen" type="straight" language="IT" text="e riuerenza baciauano lore parimente i piedi, &amp; le mani, come cosa sacra, offerendo loro quello"/>
