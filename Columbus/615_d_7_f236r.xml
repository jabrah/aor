    <annotation>
        
        <underline method="pen" type="straight" language="IT" text="breue guarirono Christiani, &amp; gl Indiani"/>
        <underline method="pen" type="straight" language="IT" text="gente di poca fatica, per coltiuar gran campagne"/>
        <underline method="pen" type="straight" language="IT" text="unamo pi&ugrave; in vn di, che essi non mangiano in XX"/>
        <underline method="pen" type="double" language="IT" text="inancato"/>
        <underline method="pen" type="straight" language="IT" text="desiderio delle"/>
        <underline method="pen" type="straight" language="IT" text="quasi il consiglio de solleuati, poi che vedeano, che"/>
        <underline method="pen" type="straight" language="IT" text="de nostri era contra noi, non"/>
        <underline method="pen" type="straight" language="IT" text="volendole tor loro per forza, facea bisogno, che la maggior"/>
        <underline method="pen" type="straight" language="IT" text="noi smontassimo in atto di guerra"/>
        
    </annotation>
