    <annotation>
        
        <marginalia hand="Italian" anchor_text="Denn was in oder ob der erden ist m&u_ring;&sharp-S; alles dem himmel z&u_ring;">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>A Centro o[mn]ia contendu[n]t [a]d Circumfere[n]tia[m]</marginalia_text>
                </position>
            </language>
            <translation>Everything strives from the centre for the circumference.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="das solcher wasser gengen ein abschneiden geschicht">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cataracta[e] Thermaru[m] no[n] numq[am] interclusa[e]</marginalia_text>
                </position>
            </language>
            <translation>The cataracts of the baths are always blockaded.</translation> <!-- FSF: to be added -->
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="nicht warm wasser cataracten vnderligen"/>
        <underline method="pen" type="straight" language="DE" text="vnden auff au&sharp-S; der erden m&u_ring;&sharp-S; die werme ghen"/>
        <underline method="pen" type="straight" language="DE" text="dem Centro der erden der samen darau&sharp-S; wachssen die cataracten"/>
        <underline method="pen" type="straight" language="DE" text="in"/>
        <underline method="pen" type="straight" language="DE" text="erden"/>
        <underline method="pen" type="double" language="DE" text="alles dem himmel z&u_ring;"/>
        <underline method="pen" type="straight" language="DE" text="der cataracten"/>
        <underline method="pen" type="straight" language="DE" text="keine sommer in den selbigen"/>
        <underline method="pen" type="double" language="DE" text="abschneiden"/>
        <underline method="pen" type="double" language="DE" text="den erdbidem"/>
        <underline method="pen" type="double" language="DE" text="verfallen"/>
        
