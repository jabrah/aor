        <underline method="pen" type="straight" language="LA" text="Esse, sed vt valeas, multa dolenda feres"/>
        <underline method="pen" type="straight" language="LA" text="Vt corpus redimas, ferrum patieris, &amp; ignes"/>
        <underline method="pen" type="straight" language="LA" text="Vt valeas animo, quicquam tolerare negabis"/>
        <underline method="pen" type="straight" language="LA" text="At pretium pars haec corpore maius habet"/>
        <underline method="pen" type="straight" language="LA" text="Et labor est vnus tempora prima pati"/>
