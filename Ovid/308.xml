    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="Uenturae memores iam nunc estote senectae"/>
        <underline method="pen" type="straight" language="LA" text="Sic nullum vobis tempus abibit iners"/>
        <underline method="pen" type="straight" language="LA" text="Dum licet, &amp; veros etiam nunc editis"/>
        <underline method="pen" type="straight" language="LA" text="Ludite, eunt anni more fluentis aquae"/>
        <underline method="pen" type="straight" language="LA" text="Nec quae praeterijt, rursum reuocabitur vnda"/>
        <underline method="pen" type="straight" language="LA" text="Nec quae praeterijt hora, redire potest"/>
        <underline method="pen" type="straight" language="LA" text="Utendum est aetate. cito pede labitur"/>
        <underline method="pen" type="straight" language="LA" text="Nec bona tam sequitur, qu&agrave;m bona prima"/>
        <underline method="pen" type="straight" language="LA" text="laxantur corpora"/>
        
