        <marginalia hand="Italian" anchor_text="hoc est no[n] significat veritate[m] v[e]l falsitate[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Definitio no[n] significat veritatem vel falsitatem.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A definition does not signify the truth or falsehood.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mathematic[us] vtit[ur] etia[m] peticionib[us] q[ua]s diximus demonstrabiles [et] no[n] p[ro]babiles">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Petitiones demo[n]strabiles no[n] p[ro]babiles
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Demonstratable, but not proveable petitions.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="phisice">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Physic&egrave;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Physically.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Euclidis sententia alia est. quam aristotelis, in istis omnibus
                    </marginalia_text>
                    <person name="Euclid"/>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>
                Euclid's opinion is different from that of Aristotle, in all of these [aspects].
            </translation>
        </marginalia>
        
