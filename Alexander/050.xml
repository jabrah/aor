        <underline method="pen" type="straight" language="LA" text="co[m]mune scie[n]tia[m] q[uae] est methaphisica"/>
        <underline method="pen" type="straight" language="LA" text="hab[et] methaphisica ad om[n]es scie[n]tias qua[n]tu[m] ad hoc q[uod] demo[n]strat p[ri]ncipia co[m]munia"/>
        <underline method="pen" type="straight" language="LA" text="Quare co[n]dudit q[uod] demo[n]stratio no[n] erit ex p[ri]ncipijs extraneis nec ex co[m]munibus. sed ex p[ro]prijs q[uo] dictum est q[uod] no[n]"/>
        <underline method="pen" type="straight" language="LA" text="descendu[n]t in mechanicas v[e]l in machinatiuas"/>
        <underline method="pen" type="straight" language="LA" text="me[n]surantes co[n]tine[n]tia[m] dolioru[m]"/>
        <underline method="pen" type="straight" language="LA" text=" sunt de triangulis"/>
        <underline method="pen" type="straight" language="LA" text="Et hic simplicit[er] loque[n]do no[n] est descensus de genere in gen[us] diuersum. sed in ide[m] [et] subalt[er]nu[m]. p[ri]ncipia e[ni]m"/>
        <underline method="pen" type="straight" language="LA" text="simplicit[er] extranea p[ri]ncipijs"/>
        <underline method="pen" type="straight" language="LA" text="illa pri[n]cipia sint resolubilia in p[ri]ma vera immediata"/>
        <underline method="pen" type="straight" language="LA" text="in q[ua]libet ergo scie[n]tia demo[n]stratiua accipit[ur] sine deminstratio[n]e quid significe[n]t p[er] nome[n] diffinitione[m] v[e]l expositione[m]. [et]"/>
        <underline method="pen" type="straight" language="LA" text="vt passio[n]is esse. est inesse subiecto. [et] hoc"/>
        
