    <page filename="075.tif" reader="John Dee" signature="[Fiij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian">   <!-- DO: Can anchor some of the text between 'opinione[m] ... lume[n] extingueret[ur]' -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quorumda[m] opinio dice[n]tiu[m] &Moon;a[e] vna[m] partem esse tenebrosam, altera[m] luminosa[m] Alioru[m] dicentiu[m] .... fieri ob luminis dies extinctione[m].  <!-- DO: The missing word might be 'eclipsim' (and then a moon), an alternative form of the accusative? -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The opinion of some saying that one part of the moon is dark, the other bright. <!-- FSF: to be completed -->  <!-- DO: '[...] That of others saying that a lunar eclipse happens because of an extinction of light' Not sure how to fit in 'dies', wondering if it might be something else... -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mediu[m] accide[n]tale">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mediu[m] accide[n]tale
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An accidental medium.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mediu[m] extrinsecu[m] [et] accide[n]tale">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mediu[m] extrinsecu[m] et accide[n]tale
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An extrinsic and accidental medium.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Causa eclipsis &Moon;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The reason for the eclipse of the moon.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="q[ua]tuor sunt cause">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quatuor causa[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Four reasons.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="textus est corrupt[us]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Textus corruptus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A corrupted text.
            </translation>
        </marginalia>        
        
        <underline method="pen" type="straight" language="LA" text="mediu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Si aut[em] no[n] sciat[ur] p[er] mediu[m] p[ro]priu[m] [et] essentiale. s[ed] p[er] mediu[m] extrinsecu[m] [et] accde[n]tale. possum[us]"/>
        <underline method="pen" type="straight" language="LA" text="s[e]c[un]d[u]m veritate[m] [et] co[m]mune[m] scola[m] mathematicoru[m] vel vtru[m]"/>
        <underline method="pen" type="straight" language="LA" text="vel vtru[m] hoc fiat p[ro]pter extinctione[m]"/>
        <underline method="pen" type="straight" language="LA" text="q[ue] sibi obuiat aliq[uo]d humidu[m] v[e]l q[ue] p[rae]occuparet[ur]"/>
        <underline method="pen" type="straight" language="LA" text="seq[ui]tur q[uod] mediu[m] demo[n]stratio[n]is sit causa"/>
        <underline method="pen" type="straight" language="LA" text="hoc etia[m] c[om]me[n]tatores"/>
        <underline method="pen" type="straight" language="LA" text="[et] inductiuas demo[n]stratio[n]es ponu[n]t et demo[n]strant"/>
        
        
