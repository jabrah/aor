    <page filename="047.tif" reader="John Dee" signature="[D[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Nam vbicu[m]q[ue] dat[ur] maius et min[us] no[n] semp[er] dat[ur] eq[ua]le">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vbi dato maiori et minori datur a[e]q[ua]le
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Where given to the greater or lesser, the equal is given.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="co[m]munia dicunt[ur] extranea">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Co[mmun]ia, extranea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Common, external.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cusa">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cusa cardin.
                    </marginalia_text>
                    <person name="Nicholas of Cusa"/>
                </position>
            </language>
            <translation>
                Cardinal of Cusa.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sit nota nu[m]eris r[ati]onalib[us]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Circumfere[n]tia no[n] est nota numeris r[ati]onalibus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The circumference is not notes in rational numbers.     <!-- DO: 'noted' -->
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="sumpsit maiore[m] ex co[m]munib[us] [et] no[n] p[ro]ximis"/>
        <underline method="pen" type="straight" language="LA" text="specie q[uo] sunt"/>
        <underline method="pen" type="straight" language="LA" text="Sed nunq[uam] dat[ur] vacca maior petro [et] minor ergo eq[ua]lis cu[m] vacca [et] petrus differu[n]t specie"/>
        <underline method="pen" type="straight" language="LA" text="q[uo] differu[n]t specie sed c[on]ueniu[n]t solu[m] genere"/>
        <underline method="pen" type="straight" language="LA" text="co[n]tinuo q[ua]re"/>
        <underline method="pen" type="straight" language="LA" text="vadit in aliud gen[us]"/>
        <underline method="pen" type="straight" language="LA" text="accide[n]s"/>
        <underline method="pen" type="straight" language="LA" text="ex extraneis"/>
        <underline method="pen" type="straight" language="LA" text="q[uo] co[m]munia dicunt[ur] extranea quia se extendunt in extraneu[m] genus subiecti"/>
        <underline method="pen" type="straight" language="LA" text="cusa et multi qui seducti sunt huiusmo[d]i"/>
        <underline method="pen" type="straight" language="LA" text="Minor transce[n]dit genus [et] sumit[ur]"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] sit nota nu[m]eris r[ati]onalib[us] no[n] sequit[ur]"/>
        
