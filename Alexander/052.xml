        <underline method="pen" type="straight" language="LA" text="Na[m] par [et] i[m]par q[ua]dratus [et] cubicus sunt passiones ip[s]ius nu[m]eri q[ua]s supponit arismetric[us]"/>
        <underline method="pen" type="straight" language="LA" text="irrationalis [et] curuu[m] seu reflexu[m]"/>
        <underline method="pen" type="straight" language="LA" text="p[ro]ut terminat[ur] visibili. q[ua]re geomet[er] demo[n]strat reflexu[m]. quia est. [et] p[er]spectiu[us] p[ro]pter quid"/>
        <underline method="pen" type="straight" language="LA" text="tria[n]gulo q[ue] est [et] quid"/>
        <underline method="pen" type="straight" language="LA" text="S[e]c[un]do p[ro]pria[m] passione[m] subiecti"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] co[n]clusio in vna demo[n]stratione potest esse p[ri]ncipiu[m] i[m]mediatu[m] in alia vt si sit c[on]clusio inte[n]ta p[ro]banda q[uod] om[n]is trianguli angulus"/>
        <underline method="pen" type="straight" language="LA" text="ergo p[ri]ncipia p[ro]pria possunt esse conclus[i]o[n]es demo[n]strationu[m]. ideo"/>
        <underline method="pen" type="straight" language="LA" text="quia p[er] huiusmo[d]i p[ri]ncipia co[m]munia [et] cu[m] p[ri]ncipijs que demo[n]strant[ur]. i[d est] cum p[ri]ncipijs p[ro]prijs demonstrat p[ro]prias passiones"/>
        
