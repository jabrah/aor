        <underline method="pen" type="straight" language="LA" text="est no[n] causam vt causa[m] ponere"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] hoc p[ri]mu[m] [et] pri[n]cipalit[er] i[n] silogismis c[on]ti[n]git q[ui] su[n]t ad impossibile in quibus p[ri]mu[m] supponit[ur] falsum vt iam in"/>
   
        
