        <underline method="pen" type="straight" language="LA" text="ita sunt singula p[ro]bata ex se ipsis"/>
        <underline method="pen" type="straight" language="LA" text="principio est ide[m] ad p[ro]batione[m] sui ip[s]ius sumere. statim"/>
        <underline method="pen" type="straight" language="LA" text="q[ua]re solus mathematic[us] infert peti[ti]o[ni]s in p[ri]ncipio"/> <!-- FSF: not sure about petitionis -->
