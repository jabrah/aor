        <marginalia hand="Italian" anchor_text="de nouo discere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        An discere sit recordari?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Isn't learning to remember?
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quem pithagorici dixeru[n]t numeru[m] alteratio[n]is sicut postea dicet[ur]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pythagorici de b&iacute;nario
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pythagoreans about binary numbers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Respo[n]de q[uam]uis sit c[om]me[n]surabilis hoc t[ame]n e[st] i[n] pote[n]tia [et] no[n] lo[n]gitudine">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[m]me[n]surab[ilis] pote[n]tia lo[n]git[udine] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Commensurable potence in longitude.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="surda est quia non scitur a nobis nec a natura [et] hoc ex causis tibi demonstratis in arte algebre">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Surdi numeri Algebra[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Indistinct numbers of Algebra.
            </translation>
        </marginalia>
        
