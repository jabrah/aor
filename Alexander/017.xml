        <underline method="pen" type="straight" language="LA" text="Sunt aut[em] tange[n]cia eor[um] quor[um] vltima sunt simul et p[ri]ma [et] neutri[us] per se. vt dixim[us]"/>
        <underline method="pen" type="straight" language="LA" text="reple[n]dis corporib[us]"/>
        <underline method="pen" type="straight" language="LA" text="p[er]manentia[m] et positione[m] no[n] habeat in p[ar]tibus que[m]admodu[m] numer[us] poci[us] [et] ordine[m]. postea"/>
        <underline method="pen" type="straight" language="LA" text="tanq[uam] eunde[m] replentes s[e]c[un]d[u]m"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mune[m] t[er]minu[m] corp[or]is"/>
        <underline method="pen" type="straight" language="LA" text="Sic loci particule que sunt particule corp[or]is locati co[n]tinuant[ur]"/>
        <underline method="pen" type="straight" language="LA" text="mediante puncto"/>
        <underline method="pen" type="straight" language="LA" text="loci"/>
        <underline method="pen" type="straight" language="LA" text="distantie"/>
        <underline method="pen" type="straight" language="LA" text="intersecant tres linee dyametrales ad vigintiquatuor rectos angulos et s[e]c[un]d[u]m totidem sup[er]ficies"/>
        <underline method="pen" type="straight" language="LA" text="spaciu[m] circu[m]stans q[uo]dlibet punctu[m] in sublimi localiter"/>
        <underline method="pen" type="straight" language="LA" text="cubos solu[m] replere locum quare locu[m] trib[us] dimensio[n]ibus"/>
