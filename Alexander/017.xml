    <annotation>
        
        <marginalia hand="Italian" anchor_text="om[n]e copila[n]s p[er] modu[m] co[n]tinuatio[n]is indiuisibile est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omne copulans p[er] modu[m] co[n]tinuationis, indiuisibile e[st].
                    </marginalia_text>
                </position>
            </language>
            <!-- FSF: I am not sure about corpulans -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="linee se tangu[n]t in punctis indiuisibilibus et sup[er]ficies lineis et corp[or]a sup[er]ficiebus et t[em]p[or]a instantaneis, et loca reple[n]dis corporib[us"> <!-- FSF: not sure about tempora -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Nota, loca se tangere mutuo p[er] reple[n]da corpora
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Note that the place is in return touching the body that is to be completed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qui[n]q[ue] regularia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quinq[ue] regularia corpora
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Five regular bodies
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tempus esse q[ua]ntitate[m] co[n]tinua[m] q[uo]d quide[m] methaphisici est">
