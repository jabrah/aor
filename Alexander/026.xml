    <annotation>
        
        <marginalia hand="Italian" anchor_text="Co[m]me[n]surabile">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Comme[n]surabile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Commensurable.
            </translation>
        </marginalia>        
        
        <marginalia hand="Italian" anchor_text="dyameter est co[m]mensurabilis coste">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        D&iacute;ametru[m] esse inco[m]me[n]surabile costa[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The diameter is incommensurable with the side.  
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nona[m] quinti geometrie">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        9a 5i
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ninth of a fifth.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="duo nu[m]eri obtine[n]tes p[ro]portione[m] dupla[m] nequaq[uam] possunt esse
            co[n]tra se inuice[m] vt nu[m]erus q[ua]dratus ad quadratu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quadratos numeros non posse esse ad invice[m] in dupla p[ro]portio[n]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Square numbers cannot be constant in the proportion to each other.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="q[ua]dratus q[ua]drato duplus relinquit[ur]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quadrati numeri no[n] possunt esse in dupla proportione
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Square numbers cannot be in double proportion.
            </translation>
        </marginalia>
        
