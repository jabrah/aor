        <underline method="pen" type="straight" language="LA" text="Altera aut[em] q[uae] sunt passiones et p[re]dicata no[n] est accipere q[ue] sunt"/>
        <underline method="pen" type="straight" language="LA" text="esse est inesse"/>
        <underline method="pen" type="straight" language="LA" text="quia illa supponunt alia vero demo[n]strant sicut tu[n]c passio demo[n]strat[ur] de subiecto. Est"/>
        <underline method="pen" type="straight" language="LA" text="Sed co[n]clusionu[m] dicunt[ur] p[ro]xima"/>
        <underline method="pen" type="straight" language="LA" text="q[uo]d quide[m] p[ri]ncipiu[m] exuit[ur] p[ri]ma principia scie[n]tie demo[n]strando"/>
        <underline method="pen" type="straight" language="LA" text="Quare n[on] om[n]es co[n]clusio[n]es mathematicales possunt ostendi immediais principijs talis scie[n]tie s[ed] solu[m]"/>
        <underline method="pen" type="straight" language="LA" text="q[uae] principijs sunt p[ro]xima vt triangulu[m]"/>
        <underline method="pen" type="straight" language="LA" text="[et] sic p[ri]ncipia immediata c[on]clusionu[m] sunt p[ri]ncipiata"/>
        <underline method="pen" type="straight" language="LA" text="Na[m] demo[n]strator vtit[ur] p[ri]nciijs  p[ro]prijs p[er] se co[m]munibus v[er]o s[e]c[un]d[u]m analogia[m] hoc est s[e]c[un]d[u]m p[ro]portione[m] ad eam cui subiecto
            p[ro]prio c[on]uenit"/>
        <underline method="pen" type="straight" language="LA" text="p[ro]pria [et] inco[m]plexa q[uae] sine"/>
        <underline method="pen" type="straight" language="LA" text="Hec e[ni]m [et] talia subiecta no[n] recipiu[n]t esse quo sunt in seip[s]is. sed recipiu[n]t hoc esse. q[uo]d habent ex hoc q[uod] passiones insunt eis"/>
        
