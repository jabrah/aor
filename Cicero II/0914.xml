        <underline method="pen" type="straight" language="LA" text="prim&iacute;sque versutum"/>
        <underline method="pen" type="straight" language="LA" text="conatus perditos vindicauit"/>
        <underline method="pen" type="straight" language="LA" text="naturae regula metiamur"/>
        <underline method="pen" type="straight" language="LA" text="repugnare naturae"/>
        <underline method="pen" type="straight" language="LA" text="discrepantiam conferre debemus. Atque"/>
        <underline method="pen" type="straight" language="LA" text="eorum vita, &amp; mores fuerant"/>
        <underline method="pen" type="straight" language="LA" text="ipse perpetua constantia roborauisset"/>
        <underline method="pen" type="straight" language="LA" text="in illo errore diuturno"/>
        <underline method="pen" type="straight" language="LA" text="Quae contemplantes, expe[n]dere oportebit quid quisque habeat sui, e&aacute;que moderari, nec velle experiri qu&agrave;m se aliena
             deceant. Id"/>
        <underline method="pen" type="straight" language="LA" text="Ergo histrio hoc videbit in scena, quod non videbit sapiens vir in vita? Ad quas igitur res aptissimi erimus, in iis potissimu[m] elaborabimus. Sin"/>
        
