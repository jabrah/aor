        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua[e] est conclusio ona</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>omne infinitu[m] est.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pra[e]missa[e].</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ennius.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dij inhil ignorat.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Eve[n]tura, nescire melius q[uam] scire.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>No[n] o[mn]es, Deos fatintur.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Multa dij da[n]t, qua[e] nos accipere non possimus.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex falsis veru[m] signi no[n] p[otes]t</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cratippi r[ati]o</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[n]tra cratippi [p]roem.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text=""> <!-- M.B. Greek marginalia -->
