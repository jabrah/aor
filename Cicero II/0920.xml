        <marginalia hand="Italian" anchor_text="Princ&eacute;psque omnium virtutum est illa sapientia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapientia o[mn][iu]m virtutu[m] p[ri]nceps</marginalia_text>
                </position>
            </language>
            <translation>Wisdom is the chief of all virtues.</translation>
        </marginalia>
        
