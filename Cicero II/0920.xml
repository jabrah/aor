        </marginalia>
        
        <marginalia hand="Italian" anchor_text="offici&iacute;sque scientiae praeponenda esse officia iustitiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>officia, studiaq[ue] scie[n]tia[e], su[n]t iustitia[e] postponenda. </marginalia_text>
                </position>
            </language>
            <translation>Duties, scholar's knowledge, there justice is postponed.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rerum cognitione versara">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Rerum cognitioin dediti quatu[m] in rip? profecerunt.</marginalia_text>  <!-- M.B. Not sure of the penultimate word. -->
