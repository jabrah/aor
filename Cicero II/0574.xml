        <underline method="pen" type="straight" language="LA" text="internosci"/>
        <underline method="pen" type="straight" language="LA" text="faciunt pueriliter"/>
        <underline method="pen" type="straight" language="LA" text="iudicantur sublato, reliqua se nega[n]t tollere: vt"/>
        <underline method="pen" type="straight" language="LA" text="improbabilem"/>
        <underline method="pen" type="straight" language="LA" text="tibi erit reuertendum"/>
        <underline method="pen" type="straight" language="LA" text="quia proprium in communi signo notari no[n]"/>
        <underline method="pen" type="straight" language="LA" text="Simili in errore versantur, quum conuicti"/>
        <underline method="pen" type="straight" language="LA" text="coacti, perspicua &agrave; perceptis volunt distinguere, &amp; conantur ostendere"/>
        <underline method="pen" type="straight" language="LA" text="perspicue dixeris"/>
        <underline method="pen" type="straight" language="LA" text="vere inanit&eacute;rve"/>
        <underline method="pen" type="straight" language="LA" text="Et hoc illud his vsu venire solet, vt quicquid dixerint, &agrave; quibusdam interrogentur"/>
        <underline method="pen" type="straight" language="LA" text="nec asseuerare sine"/>
        <underline method="pen" type="straight" language="LA" text="quod cuique occurrit, &amp; primo quasi aspectu probabile videtur, id co[n]firmatur: quid eo leuius"/>
        <underline method="pen" type="straight" language="LA" text="circunspectione aliqua, &amp; accurata consyderatione"/>
        <underline method="pen" type="straight" language="LA" text="omnibus abrogatur fides"/>
        <underline method="pen" type="straight" language="LA" text="quamproxime"/>
        <underline method="pen" type="straight" language="LA" text="veri: quo obscuro &amp; oppresso, quod tandem verum sibi videbuntur attingere? Quid autem tam absurde dici potest, qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="rei signum aut argumentum, &amp; ea re id sequor"/>
        <underline method="pen" type="straight" language="LA" text="sunt labefactare volet, facile etia[m] absentibus nobis veritas se ipsa defendet"/>
        <underline method="pen" type="straight" language="LA" text="quo non latus locus sit, sed paulo"/>
        <underline method="pen" type="straight" language="LA" text="inter inanimu[m] &amp; animal hoc maxime"/>
        <underline method="pen" type="straight" language="LA" text="ponderibus impositis deprimi, sic animu[m] perspicuis cedere. Nam"/>
        <underline method="pen" type="straight" language="LA" text="assentitur"/>
        
        
