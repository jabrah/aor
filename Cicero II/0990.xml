    <annotation>

        <marginalia hand="Italian" anchor_text="[gene]ra motus"> <!-- M.B. Rest of anchor appears on 0989.tif 'diuinis duo genera motus' -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuinis Caialiby duo in sunt genera motus</marginalia_text>
                </position>
            </language>
            <translation>The divine [??] two kinds of emotions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quae infixa caelo non mounetur loco">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>fixa sydera</marginalia_text>
                </position>
            </language>
            <translation>The immovable stars.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>De planetis loq[uen]tur et &Jupiter; ut vulgo dicunt passionibus</marginalia_text> <!-- M.B. Not certain. -->
                </position>
            </language>
            <translation>To speak of the planets and Jupiter in order for [??] </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="δαίμουας">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>δαίμ[ο]υας</marginalia_text> <!-- M.B. μ[ο] ligature.  -->
                </position>
            </language>
            <translation>Daemon.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Oceanum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Oceanus.</marginalia_text>
                </position>
            </language>
            <translation>Ocean.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Deus qui omnia genuit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Deoru[m] duplex genus</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of gods.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vos qui deorum satu ori estis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dei or[ati]o ad creaturam sunt ca[e]listis.</marginalia_text>
                </position>
            </language>
            <translation>The speech of the gods on creating the heavens.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quasi in currum vniuersitatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[e]lu[m], currus vniversitate</marginalia_text>
                </position>
            </language>
            <translation>Heaven, chariot of the universe.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="in antiquam partem"/>
        <underline method="pen" type="straight" language="LA" text="ex quo genere ea sunt fidere, quae infixa caelo non mouentur loco, quae sunt animantia, e&aacute;que diuina, ob e&aacute;mque causam"/>
        <underline method="pen" type="straight" language="LA" text="vt supr&agrave;"/>
        <underline method="pen" type="straight" language="LA" text="quae traiecto axi"/>
        <underline method="pen" type="straight" language="LA" text="generatores suos"/>
        <underline method="pen" type="straight" language="LA" text="quanquam nec argumentis"/>
        <underline method="pen" type="straight" language="LA" text="deorum satu orti estis"/>
        <underline method="pen" type="straight" language="LA" text="&amp; indissolubiles non potestis"/>
        <underline method="pen" type="straight" language="LA" text="Vt igitur mortal conditione genere[n]tur vos"/>
        <underline method="pen" type="straight" language="LA" text="illa gignatis,"/>
        <underline method="pen" type="double" language="LA" text="imitemin&iacute;que vim meam qua me &amp; in vestro ortu"/>
        <underline method="pen" type="straight" language="LA" text="vsum esse meministis"/>
        <underline method="pen" type="straight" language="LA" text="erit immortale partem attexitote mortalem: ita orientur animantes quos"/>
        <underline method="pen" type="straight" language="LA" text="issdam, sed &agrave; diis secundum sumebat atque etiam tertium"/>
        <underline method="pen" type="straight" language="LA" text="atque ita quasi in currum vniuersitatis imposuit"/>
       
