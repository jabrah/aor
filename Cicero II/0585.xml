        <underline method="pen" type="straight" language="LA" text="Mecu[m] vero quid habent litium, qui ipsorum disciplinam sequor"/>
        <underline method="pen" type="double" language="LA" text="minam"/>
        <underline method="pen" type="straight" language="LA" text="Diogenes reddet"/>
        <underline method="pen" type="straight" language="LA" text="totu[m] tortuosum genus disputandi relinquamus, ostendam&uacute;sque qui simus, iam explicata tota Carneadis sententia, Antiochi ista corruent vniuersa"/>
        <underline method="pen" type="straight" language="LA" text="qui vsque ad senectutem"/>
        <underline method="pen" type="straight" language="LA" text="etenim contra naturam"/>
        <underline method="pen" type="straight" language="LA" text="omnis vitae ea, quam Luculle co[m]memorabas, euersio"/>
        <underline method="pen" type="straight" language="LA" text="sit probabilitati illi contrarium: vtetur"/>
        <underline method="pen" type="straight" language="LA" text="se ex sente[n]tia nauigaturum? qu&icirc; potest"/>
        <underline method="pen" type="straight" language="LA" text="facili&oacute;rque erit vt albam"/>
        <underline method="pen" type="straight" language="LA" text="est &egrave; saxo sculptus, aut &egrave; robore dolatus: habet corpus, habet animu[m], mouetur me[n]te, mouetur sensibus, vt ei vera multa videantur"/>
        <underline method="pen" type="straight" language="LA" text="Vt taceam: co[n]clusio ipsa loquitur nihil"/>
        <underline method="pen" type="straight" language="LA" text="quiuis arbitratu suo reprehe[n]dat, quod"/>
        <underline method="pen" type="straight" language="LA" text="Sunt enim mihi nota propterea, qu&ograve;d earum ipsaru[m] rerum"/>
        
        
        
