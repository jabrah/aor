        <underline method="pen" type="straight" language="LA" text="horum errore susceptum esse, partim supersitione multa fallendo"/>
        <underline method="pen" type="straight" language="LA" text="quieto &amp; libero animo esse poteris, vt ad rem gerendam non superstionem habeas, sed rationem ducem? It&aacute;ne"/>
        <underline method="pen" type="straight" language="LA" text="abruptio corrigiae"/>
        <underline method="pen" type="straight" language="LA" text="quod micare"/>
        <underline method="pen" type="straight" language="LA" text="quod talos"/>
        <underline method="pen" type="straight" language="LA" text="quod tesseras"/>
        <underline method="pen" type="straight" language="LA" text="res est inuenta fallacii, aut ad quaestum, aut ad supersitionem, aut ad errorem"/>
        <underline method="pen" type="straight" language="LA" text="Nihil est, inquiunt, quod deus efficere non possit."/>
        <underline method="pen" type="straight" language="LA" text="ne omnia cum supersitiosa solicitudine &amp; miseria credere[n]t. Sed"/>
        <underline method="pen" type="straight" language="LA" text="vita itam co[m]munis"/>
        <underline method="pen" type="straight" language="LA" text="etiamnum"/>
        <underline method="pen" type="straight" language="LA" text="plan&egrave; refrixerunt"/>
        <underline method="pen" type="straight" language="LA" text="hominum facile princeps, sic opinatur"/>
        <underline method="pen" type="straight" language="LA" text="in notatione cuiusque vitae ex natali die, minime esse credendum. Nominat"/>
        <underline method="pen" type="straight" language="LA" text="hoc praedictionis"/>
        <underline method="pen" type="straight" language="LA" text="excellens in astrologia"/>
        <underline method="pen" type="straight" language="LA" text="natalitia praedicta defendunt"/>
        <underline method="pen" type="straight" language="LA" text="Vim quandam esse aiunt signifero in orve qui Graece"/>
