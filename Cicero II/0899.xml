        <underline method="pen" type="straight" language="LA" text="abundare oportet praeceptis"/>
        <underline method="pen" type="straight" language="LA" text="faciendum, vt par sis in vtriusque orationis facultate"/>
        <underline method="pen" type="straight" language="LA" text="principe huius aetatis philosophoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Latina[m] efficies"/>
        <underline method="pen" type="straight" language="LA" text="nostris pleniore[m]"/>
        <underline method="pen" type="straight" language="LA" text="Vis enim dice[n]di maior est in illis: sed hoc quoque colendum est &amp; aequabile &amp; temperatum orationis genus"/>
        <underline method="pen" type="straight" language="LA" text="in hoc numero haberi"/>
        <underline method="pen" type="straight" language="LA" text="subtilis, orator parum veheme[n]s, dulcis tamen, vt Theophrasti discipulu[m] possis agnoscere"/>
        <underline method="pen" type="straight" language="LA" text="volui, quod &amp; aetati tuae esset aptissimu[m], authoritati meae"/>
        <underline method="pen" type="straight" language="LA" text="latissim&egrave; patere"/>
        <underline method="pen" type="straight" language="LA" text="tradita ab illis &amp; praecepta"/>
        <underline method="pen" type="straight" language="LA" text="in e&oacute;que cole[n]do sita vitae est honestas omnis, &amp; negligendo turpitudo"/>
        <underline method="pen" type="straight" language="LA" text="bonu[m] sic"/>
        <underline method="pen" type="straight" language="LA" text="quanquam ita sint in promptu, vt res disputatione no[n] egeat, sunt"/>
        <underline method="pen" type="straight" language="LA" text="consentanea"/>
        <underline method="pen" type="straight" language="LA" text="est ea"/>
        
