    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="abiecta omni cunctatione adipiscendi"/>
        <underline method="pen" type="straight" language="LA" text="minus multa patent in eorum vita quae fortuna feriat, &amp; quo"/>
        <underline method="pen" type="straight" language="LA" text="Ad rem gerendam aute[m] qui accedit, caueat ne id modo consideret, qu&agrave;m illa res honesta sit, sed etia[m] vt habeat 
            efficiendi facultate[m]. In quo"/>
        <underline method="pen" type="straight" language="LA" text="enim bella saepe quaesiuerunt propter"/>
        <underline method="pen" type="straight" language="LA" text="vrbanae maiores clarior&eacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="no[n] minus praeclarum"/>
        <underline method="pen" type="straight" language="LA" text="quanquam imperium"/>
        <underline method="pen" type="straight" language="LA" text="dilatatum putatur"/>
        <underline method="pen" type="straight" language="LA" text="causas &amp; parentiores"/>
        <underline method="pen" type="straight" language="LA" text="neque pueris nobis"/>
        <underline method="pen" type="straight" language="LA" text="Parua enim sunt foris arma, nisi est consilium domi"/>
        <underline method="pen" type="straight" language="LA" text="res no[n] solum ex domestica est ratione, sed attingit etia[m]"/>
        <underline method="pen" type="straight" language="LA" text="solere ad inuidis &amp; improbis audito"/>
        <underline method="pen" type="straight" language="LA" text="nostra celerier de manibus audacissimoru[m] ciuium delapsa arma ipsa ceciderunt"/>
        <underline method="pen" type="straight" language="LA" text="Marce fili"/>
        <underline method="pen" type="straight" language="LA" text="abundans bellicis laudibus"/>
        <underline method="pen" type="straight" language="LA" text="triumphu[m] deportaturum fuisse"/>
        <underline method="pen" type="straight" language="LA" text="his operae studi&iacute;que ponendu[m] est. Omnino"/>
        <underline method="pen" type="straight" language="LA" text="Exercendu[m] tamen corpus, &amp; ita efficiendum est, vt obedire consilio ration&iacute;que possit in exequendis negociis, 
            &amp; in labore tolerando. Honestum"/>
        
