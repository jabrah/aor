        <underline method="pen" type="straight" language="LA" text="quos qui ratione rexerit, iuste vixerit: qui autem his se dederit, iniuste. Atque"/>
        <underline method="pen" type="straight" language="LA" text="honeste curriculum viunedi &agrave; natura"/>
        <underline method="pen" type="straight" language="LA" text="ad illud astrum quocum aptus fuerit, reuertetur"/>
        <underline method="pen" type="straight" language="LA" text="eueniet, quu[m] illa quae ex igni, anima, aqua, atque terra turbule[n]ta &amp; rationis expertia insederint, ratione depulerit, 
            &amp; ad primam atque optimam affectione[m] animi peruenerint"/>
        <underline method="pen" type="straight" language="LA" text="esset reliquum ex humano animo quod deberet"/>
        <underline method="pen" type="straight" language="LA" text="ipse benefactus, sua"/>
        <underline method="pen" type="straight" language="LA" text="composuit, constanter"/>
        <underline method="pen" type="straight" language="LA" text="sui, particulas ignis &amp; terrae, &amp; aquae, &amp; animae &agrave; mundo, quas surus redderent, mutuabantur, e&aacute;sque"/>
