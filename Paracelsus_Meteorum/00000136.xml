        <marginalia hand="Italian" anchor_text="steine von Himmel ; Metal ; Praesagium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Presagiu[m] ex Metallis et Lapidibus cade[n]tibus
                    </marginalia_text>
                </position>
            </language>
            <translation>A sense of foreboding from metal and fallen rocks.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        fulminus lapis
                    </marginalia_text>
                </position>
            </language>
            <translation>Crushed stone.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="ist sondern ein seltzames"/>
        <underline method="pen" type="double" language="DE" text="Stral der kalt"/>
        <underline method="pen" type="straight" language="DE" text="ist nichts als"/>
        <underline method="pen" type="double" language="DE" text="ha[e]rter sandt"/>
        <underline method="pen" type="straight" language="DE" text="So nun die natur"/>
        <underline method="pen" type="double" language="DE" text="kalten Stral"/>
        <underline method="pen" type="straight" language="DE" text="ein stein geb&uuml;rt"/>
        <underline method="pen" type="double" language="DE" text="wol mo[e]glich"/>
        <underline method="pen" type="straight" language="DE" text="steine falle[n]"/>
        <underline method="pen" type="straight" language="DE" text="ohne Strals in die Erden"/>
        <underline method="pen" type="straight" language="LA" text="Praesagia"/>
        <underline method="pen" type="straight" language="DE" text="seindt"/>
        <underline method="pen" type="straight" language="DE" text="denu Sam haben sie wol von kalter"/>
        <underline method="pen" type="straight" language="LA" text="influxion generationis"/>
        <underline method="pen" type="straight" language="DE" text="dieselbige Materien nit zum resoluiere[n] koin[n]en lassen"/>
        <underline method="pen" type="straight" language="DE" text="als dann so die zeit kompt der stunde fallen lassen in die Erden"/>
        <underline method="pen" type="straight" language="DE" text="sie seindt fix bleiben auff Erden liegen"/>
        
