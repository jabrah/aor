                    <marginalia_text>Nota exe[m]plu[m].</marginalia_text> 
                </position>
            </language>
            <translation>Note the exame</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vulcanus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>in o[mn]ibus Elementis</marginalia_text> 
                </position>
            </language>
            <translation>[Vulcan is] in all elements.</translation>
        </marginalia>
        
