<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="00000033.tif" reader="John Dee" pagination="10"  signature="Cij[r]"/> 
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>De corpore spiritu, vita. Anima Rerum [omnium]</marginalia_text> <!--JG: Unsure about 'omnium'-->
                </position>
            </language>
            <translation>On the body [and] on the spirit, life, and soul of all things.</translation>
        </marginalia>
        
        <marginalia hand="Italian"  anchor_text="aber der Mensch ist ist eine Seele ist ein Geist vnd ist klein">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Homo anima est spiritusque exiguus.</marginalia_text>
                </position>
            </language>
            <translation>Man is a soul, and a mind, and small.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Elementum purum corpuletum et feculetum</marginalia_text>
                </position>
            </language>
            <translation>Element - pure - physical and filthy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="die Erden tregt frucht nicht sie aber das Element in ir das gibt die fr&uuml;chte">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Fructus terrae ex vita spiritueq[ue] terrae, no[n] terrae corpore.</marginalia_text>
                </position>
            </language>
            <translation>The fruit of the earth is from the life and spirit of the earth, not from the body of the earth.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Elementu[m] et Corpus elementare } differunt.</marginalia_text>
                </position>
            </language>
            <translation>An element and the elementary body differ.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="Einjeglichs Element ist vo[n] des Menschen"/>
        <underline method="pen" type="straight" language="DE" text="was Element"/>
        <underline method="pen" type="double" language="DE" text="was nicht"/>
        <underline method="pen" type="straight" language="DE" text="ist eine Seele"/>
        <underline method="pen" type="straight" language="DE" text="ist ein Geist"/>
        <underline method="pen" type="double" language="DE" text="vnt ist klein"/>
        <underline method="pen" type="straight" language="DE" text="Corpus ein"/>
        <underline method="pen" type="straight" language="DE" text="Mensch im Corpus"/>
        <underline method="pen" type="straight" language="DE" text="die Seele ohne das Corpus nicht is sondern"/>
        <underline method="pen" type="straight" language="DE" text="kleine"/>
        <underline method="pen" type="straight" language="DE" text="das Element Terrae ist im selbigen Corpus, vnd ist vnsichtbar"/>
        <underline method="pen" type="straight" language="DE" text="Element ist in im ein kleiner Geist gegen"/>
        <underline method="pen" type="straight" language="DE" text="solcher gr&ouml;sse so der"/>
        <underline method="pen" type="straight" language="DE" text="Mensch"/>
        <underline method="pen" type="straight" language="DE" text="der Geist redt"/>
        <underline method="pen" type="double" language="DE" text="die Erden tregt"/>
        <underline method="pen" type="straight" language="DE" text="das Element in ir"/>
        
    </annotation>
</transcription>
