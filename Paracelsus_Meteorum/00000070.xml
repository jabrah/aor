<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="00000070.tif" reader="John Dee" pagination="[28[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="ist warm vn[d] trucken">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Calidus siccus.</marginalia_text>
                </position>
            </language>
            <translation>Warm [and] dry.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Euro vnd seinen mitgeselle[n]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Euri collaterales.</marginalia_text>
                </position>
            </language>
            <translation>Collateral east winds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="kaldt vnd feucht">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Frigidus humidus</marginalia_text>
                </position>
            </language>
            <translation>Cold [and] humid.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="die Natur des Windes materien">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Venti Materia in tribus primis.</marginalia_text>
                </position>
            </language>
            <translation>The matter of the wind in three primes.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="gehet der Windt durch den lufft vber das Meer vnd"/>
        <underline method="pen" type="straight" language="DE" text="langer vbung vnd weither reise sich selbst verzert"/>
        <underline method="pen" type="double" language="DE" text="kein Materia"/>
        <underline method="pen" type="straight" language="DE" text="ist warm vn[d] trucken"/>
        <underline method="pen" type="double" language="DE" text="den dreien Ersten ist"/>
        <underline method="pen" type="straight" language="DE" text="zu sein vnd nicht wider sein natur"/>
        <underline method="pen" type="straight" language="DE" text="mitgeselle[n] kein andere natur vn[d] operation erfunden alle warm vnd trucken"/>
        <underline method="pen" type="straight" language="DE" text="kaldt vnd feucht"/>
        <underline method="pen" type="straight" language="DE" text="der Nidergang ist der Complexion wie der auffgang aber"/>
        <underline method="pen" type="straight" language="DE" text="Natur des Windes materien im Nidergang"/>
        <underline method="pen" type="straight" language="DE" text="geschaffen kalt vnd feuchte"/>
        
        <mark name="vertical_bar" method="pen" place="intext"/>
        
        <mark name="scribble" method="pen" place="tail"/>
        
    </annotation>
