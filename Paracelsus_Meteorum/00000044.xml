    <annotation>
        
        <marginalia hand="Italian" anchor_text="Biren; Apffel; N&uuml;ffe; Rege[n]; Schnee; Hagel">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Vt pira, poma, Cerasa su[n]t Arboru[m] fructus. Ita nix, grando, pluvia, stellarum sunt fructus.</marginalia_text>
                </position>
            </language>
            <translation>As pears, apples and cherries are fruits of the trees, thus snow, hail and rain are fruits of the stars.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="eigenschafft standen auch die"/>
        <underline method="pen" type="straight" language="DE" text="Rege[n]"/>
        <underline method="pen" type="straight" language="DE" text="Schnee"/>
        <underline method="pen" type="straight" language="DE" text="Hagel"/>
        <underline method="pen" type="straight" language="DE" text="die hitze so"/>
        <underline method="pen" type="straight" language="DE" text="Wie war dann sehent von den Etnischen fewer von den heissen Wassern"/>
        <underline method="pen" type="straight" language="DE" text="solche Natur auch"/>
        <underline method="pen" type="straight" language="DE" text="solcher K&auml;ldte auch begabet seindt vnd inen eingeleibt"/>
        <underline method="pen" type="straight" language="DE" text="Also seindt auch anziehende Sternen im Firmament"/>
        
