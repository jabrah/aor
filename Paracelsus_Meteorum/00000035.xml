    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="00000035.tif" reader="John Dee" pagination="11" signature="Ciij"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Also seindt alle fr&uuml;chte seinem Element gleich mit dem Corpus vnd Spiritum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Regula.</marginalia_text>
                </position>
            </language>
            <translation>Rule.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Es ist aber ein vnderscheidt zwischen dem nat&uuml;rlichen Geist vnd vnnat&uuml;rlichen der Nat&uuml;rlich ist Corporalisch vn[d] nat&uuml;rlich eingeleibt in ein besonder Corpus der vnnatt&uuml;rliche Geist ist gar ohn ein Corpus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>NOTA.</marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Der Buxus hat ein geschmack derselbige geschmack ist sin Spiritus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Spiritus Buxi ex gustu sentitur.</marginalia_text>
                </position>
            </language>
            <translation>The spirit of the boxweed is perceived by taste.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Also im Corallen ist die farbe der geist das ander ein Corpus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Color rubeus Coralloru[m], est eoru[m] spiritus.</marginalia_text>
                </position>
            </language>
            <translation>The red color of corals is their spirit.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Frustus quid.</marginalia_text>
                </position>
            </language>
            <translation>What is the fruit.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Es ist aber ein vnderscheidt zwischen dem nat&uuml;rlichen Geist vnd vnnat&uuml;rlichen">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Spiritus</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>- natural - innatur.</marginalia_text>
                </position>
            </language>
            <translation>Spirit. Natural. Innatural.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="die R&ouml;tte der Corallen">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Corrallorum Rubedo.</marginalia_text>
                </position>
            </language>
            <translation>The redness of the corals.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Corpus ist aber so gar allein des Geists also das nichts fauls oder grobs mehr da ist">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Corpus spirituale.</marginalia_text>
                </position>
            </language>
            <translation>The spiritual body.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="Corpus, vn[d] im Corpus den Spiritum"/>
        <underline method="pen" type="triple" language="DE" text="Spiritus ist das rechte"/>
        <underline method="pen" type="triple" language="DE" text="ist nichts"/>
        <underline method="pen" type="double" language="DE" text="sein Spiritus"/>
        <underline method="pen" type="double" language="DE" text="Seel"/>
        <underline method="pen" type="straight" language="DE" text="Geist ist der"/>
        <underline method="pen" type="triple" language="DE" text="farbe desr geist"/>
        <underline method="pen" type="straight" language="DE" text="Also seindt alle fr&uuml;chte seinem Element gleich mit dem Corpus vnd Spiritum"/>
        <underline method="pen" type="straight" language="DE" text="die der Spiritus ist"/>
        <underline method="pen" type="straight" language="DE" text="nat&uuml;rlichen Geist"/>
        <underline method="pen" type="straight" language="DE" text="vnd unnar&uuml;rlichen"/>
        <underline method="pen" type="double" language="DE" text="Nat&uuml;rlichen ist Corporalisch"/>
        <underline method="pen" type="straight" language="DE" text="der vnnatt&uuml;rliche Geist ist gar ohn ein Corpus"/>
        <underline method="pen" type="double" language="DE" text="R&ouml;tte ein Corpus"/>
        <underline method="pen" type="straight" language="DE" text="ist aber so gar allein des"/>
        <underline method="pen" type="triple" language="DE" text="nichts fauls oder grobs mehr da ist"/>
        <underline method="pen" type="triple" language="DE" text="Geist der fr&uuml;chten"/>
        <underline method="pen" type="straight" language="DE" text="greifflich noch sichtlich"/>
        <underline method="pen" type="straight" language="DE" text="ein Corpus sonder"/>
        <underline method="pen" type="double" language="DE" text="im selbigen"/>
        
        <mark name="vertical_bar" method="pen" place="left_margin"/>
        
        <mark name="bracket" method="pen" place="right_margin" language="DE" text="Vnd dieweil der Geist der fr&uuml;chten nicht greifflich noch sichtlich ist darum[m] hat im die Natur ein Corpus sonder geben im selbigen zu sein"/>
        
    </annotation>
