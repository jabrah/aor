    <annotation>
        
        <marginalia hand="Italian" anchor_text="Also ist auch der Himmel ein Corpus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Coeli corpus q[u]ale</marginalia_text>
                </position>
            </language>
            <translation>What body of the heaven.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="farbe[n] des Himmels">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Colores Elementoru[m].</marginalia_text>
                </position>
            </language>
            <translation>The colours of the elements.</translation>
        </marginalia>
        
        <marginalia hand="Italian" marginalia_id="00000020_0001" internal_ref="00000020_u0005" anchor_text="Miner">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Mineraru[m] colores.</marginalia_text>
                </position>
            </language>
            <translation>The colours of the minerals.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Erden">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Terra.</marginalia_text>
                </position>
            </language>
            <translation>Earth.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="mit den andern gar keine vermischung hat"/>
        <underline method="pen" type="double" language="DE" text="den Rauch"/>
        <underline method="pen" type="straight" language="DE" text="was von jm wachst ist auch also seind"/>
        <underline method="pen" type="straight" language="DE" text="Also die Sonne die ist ein Corpus wie gemelt ist der Mon vnd alle Sternen"/>
        <underline method="pen" type="straight" language="DE" text="ohne Complexion"/>
        <underline method="pen" type="double" language="DE" text="vnd ist das" id="00000020_u0001" internal_ref="00000020_u0003"/>
        <underline method="pen" type="double" language="DE" text="au&sharp-S; dem das fewr" id="00000020_u0002" internal_ref="00000020_u0004"/>
        <underline method="pen" type="straight" language="DE" text="wachst vnd entspringet"/>
        <underline method="pen" type="straight" language="DE" text="Sternen die au&sharp-S; dem Himmel jr natur vnd wesen nemen"/>
        <underline method="pen" type="straight" language="DE" text="ein Element des fewers" internal_ref="00000020_u0001" id="00000020_u0003"/>
        <underline method="pen" type="straight" language="DE" text="das fewer thun sol das thut der Himmel der es gibt vnd"/>
        <underline method="pen" type="double" language="DE" text="au&sharp-S; jm wachst" id="00000020_u0004" internal_ref="00000020_u0002"/>
        <underline method="pen" type="straight" language="DE" text="schwartz"/>
        <underline method="pen" type="straight" language="DE" text="ist ein Miner" id="00000020_u0005"/>
        
