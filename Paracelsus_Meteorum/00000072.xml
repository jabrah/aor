    <page filename="00000072.tif" reader="John Dee" pagination="[29[v]]" signature="[H[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="vo[n] dene[n] entspringe[n] alle Wind">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Aliquot Millena[e] stella[e] Ventoru[m] causa.</marginalia_text>
                </position>
            </language>
            <translation>Several thousand stars are the cause of the winds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Prima materia der dreye[n] Ersten">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Tria prima, materia ventorum</marginalia_text>
                </position>
            </language>
            <translation>The three primes, matter of the winds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="auch Winde werde[n] vom andern Sterne[n] der">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nota</marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="etlich tausent"/>
        <underline method="pen" type="straight" language="DE" text="dere[n] kein zal"/>
        <underline method="pen" type="straight" language="DE" text="vo[n] dene[n] entspringe[n] alle Wind"/>
        <underline method="pen" type="straight" language="DE" text="auch Winde werde[n] vom andern Sterne[n] der"/>
        <underline method="pen" type="double" language="DE" text="Regen"/>
        <underline method="pen" type="double" language="DE" text="Schaueren"/>
        <underline method="pen" type="double" language="DE" text="nicht in Windzirckel gerechnet"/>
        <underline method="pen" type="double" language="DE" text="die vier theil"/>
        <underline method="pen" type="straight" language="DE" text="vom eusserste[n] des Him[m]els"/>
        <underline method="pen" type="straight" language="DE" text="das dieselbigen Sternen in innen habben zugeberen die Winde"/>
        <underline method="pen" type="straight" language="DE" text="auch mit solchen Sternen zu verstehen"/>
        <underline method="pen" type="straight" language="DE" text="seind die M&uuml;tter"/>
        <underline method="pen" type="straight" language="DE" text="Winde"/>
        <underline method="pen" type="straight" language="DE" text="Zephyro. Wie nun die Natur der dreien Ersten ist"/>
        <underline method="pen" type="straight" language="DE" text="also ist jre Frucht vnd"/>
        
    </annotation>
