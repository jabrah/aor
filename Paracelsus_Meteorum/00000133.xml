    <page filename="00000133.tif" reader="John Dee" pagination="60"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Sonst kan kein Metal an sich zogen werden">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Dubito an solum ferru[m] attrahi possitu[s].
                    </marginalia_text>
                </position>
            </language>
            <translation>I am uncertain whether [it is] possible that metal is attracted by the sun.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="portz">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        [po]rtione
                    </marginalia_text>
                </position>
            </language>
            <translation>Part.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sternen seindt die Magnetische arth haben">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Magnetica stellaru[m] vis.
                    </marginalia_text>
                </position>
            </language>
            <translation>The magnetic force of stars.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Stern Magnes">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Stella magnetica
                    </marginalia_text>
                </position>
            </language>
            <translation>Magnetic star.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="das thunt andere Sternen in andere wege"/>
        <underline method="pen" type="straight" language="DE" text="Sonst kan kein Metal an sich zogen werden"/>
        <underline method="pen" type="straight" language="DE" text="ein attraction"/>
        <underline method="pen" type="straight" language="DE" text="Berg Magnes"/>
        <underline method="pen" type="straight" language="DE" text="auffein mal an eim st&uuml;cke"/>
        <underline method="pen" type="straight" language="DE" text="klaubt zusammen mit der portz"/>
        <underline method="pen" type="straight" language="DE" text="biss er viel zu wegen"/>
        <underline method="pen" type="double" language="DE" text="in kleiner gestalt"/>
        <underline method="pen" type="double" language="DE" text="na[e]chsten enden"/>
        <underline method="pen" type="double" language="DE" text="zu jm in sein Mansion"/>
        <underline method="pen" type="double" language="DE" text="Radiert ers"/>
        <underline method="pen" type="double" language="DE" text="schmeltzets wie"/>
        <underline method="pen" type="straight" language="DE" text="der Magnet"/>
        <underline method="pen" type="straight" language="DE" text="von jm widerfallen"/>
        <underline method="pen" type="straight" language="DE" text="Alcool"/>
        <underline method="pen" type="double" language="DE" text="corradiert in"/>
        <underline method="pen" type="straight" language="DE" text="Alcool"/>
        <underline method="pen" type="straight" language="DE" text="Salis colligiert"/>
        
        <mark name="plus_sign" method="pen" place="left_margin"/>
        
    </annotation>
