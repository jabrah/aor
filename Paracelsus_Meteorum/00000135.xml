        <marginalia hand="Italian" anchor_text="Borras">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Borax
                    </marginalia_text>
                    <person name="Boreas"/>
                </position>
            </language>
            <translation>Boreas.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Eisen das wider herab fallt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ferru[m] ex ca[e]lo cadens
                    </marginalia_text>
                </position>
            </language>
            <translation>Iron falling from the sky.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Wo solch Eysen ligt oder gefunden wirdt kan nicht sein es muss besser sein dann ander Eysen: dann der Etherische Borras hats geleutert vnd gradiert.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ferru[m] &agrave; ca[e]lo repurgatum, excelle[n]tissimu[m] est.
                    </marginalia_text>
                </position>
            </language>
            <translation>Iron cleansed by the sky is very excellent.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pennates">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pennates
                    </marginalia_text>
                </position>
            </language>
            <translation>Comets.</translation> <!--KS: Literally, 'pennates' refers to elemental spirits that used comets as weapons.-->
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="wider herab fallt"/>
        <underline method="pen" type="straight" language="DE" text="dan[n] es ist nicht fir"/>
        <underline method="pen" type="double" language="DE" text="transmutiert in ein"/>
        <underline method="pen" type="straight" language="LA" text="Eruginem"/>
        <underline method="pen" type="straight" language="DE" text="den zeuchts an sich"/>
        <underline method="pen" type="double" language="DE" text="derselbige"/>
        <underline method="pen" type="straight" language="DE" text="jm diss bereidt"/>
        <underline method="pen" type="double" language="DE" text="hat. So"/>
        <underline method="pen" type="double" language="DE" text="es zu viel ist"/>
        <underline method="pen" type="straight" language="DE" text="Borras"/>
        <underline method="pen" type="straight" language="DE" text="flissent mache[n] in ein Mass"/>
        <underline method="pen" type="straight" language="LA" text="Corpus"/>
        <underline method="pen" type="straight" language="DE" text="so mags das"/>
        <underline method="pen" type="straight" language="LA" text="Firmament"/>
        <underline method="pen" type="straight" language="DE" text="nimmer trage[n] vnd lests fallen"/>
        <underline method="pen" type="straight" language="DE" text="solch Eysen"/>
        <underline method="pen" type="straight" language="DE" text="sein es muss besser sein dann ander Eysen"/>
