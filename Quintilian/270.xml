        <marginalia hand="Italian" anchor_text="Oratori satis est dicere: C&ugrave;m argentum legauerit omne, percuniam quoq[ue] legauit, quae est in argento.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Consilium</marginalia_text>
                </position>
            </language>
            <translation>Advice.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="oratione syllogismo quidem aliquandi utim nefas non duco, ita constare totam, aut cert&egrave; confertam esse aggre&sharp-S;ionu[m] 
            &amp; enthymematum stipatione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>orationes no[n] debent e&sharp-S;e reserta[e] syllogi[smo] enthyme.</marginalia_text>
                </position>
            </language>
            <translation>Speeches do not have to be bound together with syllogism or enthyme.</translation>
        </marginalia>
        
