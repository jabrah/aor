                </position>
            </language>
            <translation>Dialectical.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nobis ad alioru[m] iudicia comnpone[n]da est oratio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Oratores.</marginalia_text>
                </position>
            </language>
            <translation>Orators.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eloquentia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Eloque[n]tia</marginalia_text>
                </position>
            </language>
            <translation>Eloquence.</translation>
