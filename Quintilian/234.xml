        <marginalia hand="Italian" anchor_text="Diuisio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuisio</marginalia_text>
                </position>
            </language>
            <translation>Division.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Itaque quodcunq[ue] proprium deerot, soluet finitionem, non utiq[ue] quodcunq[ue], erit, confirmabit."/>
        <underline method="pen" type="straight" language="LA" text="Quod autem proprium non erit, differens"/>
        <underline method="pen" type="straight" language="LA" text="Animal, genus: mortale, species: terrenum, uel bipes, differens"/>
        <underline method="pen" type="straight" language="LA" text="&amp; ijs quae aliquid sunt, subijcit"/>
        <underline method="pen" type="straight" language="LA" text="in manum non conuenerit"/>
        <underline method="pen" type="straight" language="LA" text="Partes incertas esse"/>
        <underline method="pen" type="straight" language="LA" text="quibus constet respub"/>
        <underline method="pen" type="straight" language="LA" text="utitur, quia scribens ad Trebatium, ex iure ea ducere maluit: ego apertiora posui. Propria"/>
        
