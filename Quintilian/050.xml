    <annotation>
        
        <marginalia hand="Italian" anchor_text="Frigidiora his alia, ut quicquid, c quartam haberet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quidcquid</marginalia_text>
                </position>
            </language>
            <translation>Whatever.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Et quotidie, non cotidie, ut sit quot diebus.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quotidie.</marginalia_text>
                </position>
            </language>
            <translation>Daily.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quaeri solet, in scribendo praepostiones sonum quem iunctae efficiunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua[e]stio de prepositionum sono.</marginalia_text>
                </position>
            </language>
            <translation>The question of the sound of prepositions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Est etiam in diuidendis uerbis obseruatio, mediam literam consonantem priori, an sequenti syllabae adiungas.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>.Oberuatio.</marginalia_text>
                </position>
            </language>
            <translation>Observing.</translation> <!-- FSF: observation? -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nam k quidem in nullis uerbis utendum puto, nisi quae significat, etiam ut sola ponatur. Hoc e&ograve; non omisi, qu&ograve;d quidam eam, quoties a
             sequatur, neccesariam credunt: cum sit c litera, quae ad omnes uocales uim suam perferat.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>K &amp; C,</marginalia_text>
                </position>
            </language>
            <translation>[The letters] K and C.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Et Latinis ueteribus d plurimis in uerbis ultimam adiectam, quod manifestum est etiam ex columna rostrata, quae est *D. iulio in foro posita.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>.D.</marginalia_text>
                </position>
            </language>
            <translation>[The letter] D.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Interin g quoq[que]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>G.</marginalia_text>
                </position>
            </language>
            <translation>[The letter] G.</translation>
        </marginalia>
        
