        <numeral place="intext" language="LA" text="quo">3</numeral>
        <numeral place="intext" language="LA" text="locis">4</numeral>
        <numeral place="intext" language="LA" text="an">5</numeral>
        <numeral place="intext" language="LA" text="nocturna">6</numeral>
        
        
