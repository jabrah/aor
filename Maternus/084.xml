    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="084.tif" reader="John Dee" pagination="61" signature="f"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="locus fortunae partiliter">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pars fortuna[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A part of fortune.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pollincotres, aut funerarios">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pollinctores, curratores funera et vnctor cadauerum sunt.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                There are undertaker, supervisors of a funeral and anointer of corpse.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Et si in nocturna genitura &Sun; &amp; &Saturn;, in septimo loco co[n]stitutis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vix p[otes]t &Sun; in nocturna genitura in hoc loco esse
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The sun can hardly be in this place in the nocturnal constellation.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="archigallos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Archigalli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Chief priest.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in sexto loco">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Sexti loci malitia vt frangi p[otes]t, docuit supra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                        Above it taught how the badness of the sixth place can be removed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aut cornicularii, aut co[m]mentarienses">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        cornicularij commentarie[n]ses
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Assistants, officials.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in duodecimo loco">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        In duodecimo loco.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In twelfth place.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quid malorum decretu[m] fuerit, mitigatur"/>
        <underline method="pen" type="straight" language="LA" text="naturam maleuolae stellae, qu[a]e cum ipso fuerit collocata, indicit uitia"/>
        <underline method="pen" type="straight" language="LA" text="fuerit in domo"/>
        <underline method="pen" type="straight" language="LA" text="uel altitudine"/>
        <underline method="pen" type="straight" language="LA" text="domo &Jupiter;"/>
        <underline method="pen" type="straight" language="LA" text="Facient etiam ut ipsi sibi manibus suis inferant mortem, aut captiuos facient hos eosdem, aut exules"/>
        <underline method="pen" type="straight" language="LA" text="uelint difficilium literarum noticiam habere"/>
        <underline method="pen" type="straight" language="LA" text="pollinctores"/>
        <underline method="pen" type="straight" language="LA" text="nocturna genitura &Sun; &amp; &Saturn;, in septimo loco"/>
        <underline method="pen" type="straight" language="LA" text="in quibus se duplex natura coniungat"/>
        <underline method="pen" type="straight" language="LA" text="archigallos faciet, &amp; qui uirilia propriis sibi"/>
        <underline method="pen" type="straight" language="LA" text="MC, fuerit inuenta, iis omnibus in sexto"/>
        <underline method="pen" type="straight" language="LA" text="uel aquaru[m] distributores. Quod"/>
        <underline method="pen" type="straight" language="LA" text="aut cornicularii"/>
        <underline method="pen" type="straight" language="LA" text="aut co[m]mentarienses, quibus"/>
        <underline method="pen" type="straight" language="LA" text="nihil quidem eorum facient, quae"/>
        <underline method="pen" type="straight" language="LA" text="Sed hae omnes"/>
        
