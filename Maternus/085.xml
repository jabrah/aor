<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="085.tif" reader="John Dee" pagination="62" signature="[f[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="dabit ex igne &amp; ferro artem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &Sun; &Mars; arte[m] significa[n]t <!-- M.B. I read '&Sun; &Mars; vt arte[m] significa[n]t.' -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The sun and Mars indicate skill.
            </translation>
        </marginalia>
                
        <marginalia hand="Italian" anchor_text="nondum soluta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        oppositio no[n]du[m] soluta. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A not yet open opposition.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="caput malitiosis humoribus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Caput malis humoribus anestum <!-- M.B. I think the last word might be 'oner[a]tum' as in the printed text (humoribus onerant).  -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The head in bad humours.  <!-- M.B. The last word of the marginalia is omitted from this translation. If above is correct it would be something like 'The head oppressed by bad humours.'-->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        secundu[m] differe[n]tias signor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                According to the differences of signs.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cantationibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        cantationes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Incantations.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fabricatores deorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        fabricatores deorum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The fabricators of the gods. <!-- M.B. I would translate this simply as 'The makers of the gods.' -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="remotus &agrave; patria morietur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mors extra patria <!-- M.B. I read 'patriam'. -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The death outside the native land. <!-- M.B. I woud phrase this as 'Death outside of the fatherland.' -->
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="dabit ex igne &amp; ferro artem"/>
        <underline method="pen" type="straight" language="LA" text="nondum soluta coniunctione, sed"/>
        <underline method="pen" type="straight" language="LA" text="facient caducos, &amp; lunaticos"/>
        <underline method="pen" type="straight" language="LA" text="alienos humores"/>
        <underline method="pen" type="straight" language="LA" text="amputant corporis, sed haec secundum differentias signorum"/>
        <underline method="pen" type="straight" language="LA" text="tales erunt, ut mederi, &amp; curati possint"/>
        <underline method="pen" type="straight" language="LA" text="aut cantationibus compescet"/>
        <underline method="pen" type="straight" language="LA" text="biothanata morte"/>
        <underline method="pen" type="straight" language="LA" text="fabricatores deorum"/>
        <underline method="pen" type="straight" language="LA" text="sed &amp; ipsis, &amp; patri eius uarias uitae mutationes, multiplic&eacute;sq[ue]  decernit"/>
        <underline method="pen" type="straight" language="LA" text="remotus &agrave; patria morietur"/>
        <underline method="pen" type="straight" language="LA" text="diurna genitura"/>
        
    </annotation>
</transcription>