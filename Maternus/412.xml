        <underline method="pen" type="straight" language="LA" text="praedicare"/>
        <underline method="pen" type="straight" language="LA" text="Sollicitare umbras"/>
        <underline method="pen" type="straight" language="LA" text="Omnia conando docilis solertia uicit"/>
        
