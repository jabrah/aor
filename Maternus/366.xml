    <annotation>
        
        <marginalia hand="Italian" anchor_text="Quartae autem anni sunt solius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quartae ha[e] notanda[e] veniunt 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                These notable four come.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Singularis admonitio
                    </marginalia_text>
                </position>
            </language>
            <translation>A remarkable advice.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dominus 11 ascendentis, uel 11 Lunae, aut 11 partis fortunae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Nota istas domas vndecimas tres. 
                    </marginalia_text>
                </position>
            </language>
            <translation>Note that there are three of these eleventh houses
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Videtur hinc repugnare 7a 
                    </marginalia_text>
                </position>
            </language>
            <translation>Hence he seems to disagree with the seventh.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Angulos asce[n]detis dicit quonia[m] respectu alior[um] planetor[um] anguli etia[m] sunt.
                    </marginalia_text>
                </position>
            </language>
            <translation>He says the angles of the ascendant becuase the are angles with respect to the other planets.  
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de ascensione in descensionem &amp; econuerso">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mutatio de asce[n]sione ad descensione[m] et co[n]tra 
                    </marginalia_text>
                </position>
            </language>
            <translation> A mutation of the ascension towards descension and contrary.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="planetae masculini sunt qui agunt, feminini uero sunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                      Planeta[e] masc. fa[e]mi.
                    </marginalia_text>
                </position>
            </language>
            <translation>Masculine and feminine planets.
            </translation>
        </marginalia>

        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Hinc accepit cardanus sua[m] distinctione[m] significatoris secundu[m] substa[n]tia[m] et secundu[m] accidens. 
                    </marginalia_text>
                    <person name="Girolamo Cardano"/>
                </position>
            </language>
            <translation>Hence Cardano accepts his distinctions according to the substance and accident of the signifier. 
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="anni sunt"/>
        <underline method="pen" type="straight" language="LA" text="naturam consideres, in quo coiniuncti suerint: utrum sit ex eorum similitudine, nec ne, si enim fuerit ex eorum similitudine, co[n]forabis eorum significationem."/>
        <underline method="pen" type="straight" language="LA" text="dominus 11 ascendentis, uel 11 Lunae, aut 11 partis fortunae"/>
        <underline method="pen" type="straight" language="LA" text="&amp; in eodem"/>
        <underline method="pen" type="straight" language="LA" text="sed &amp; septimi circuli"/>
        <underline method="pen" type="straight" language="LA" text="&amp; duae infortunae in angulis"/>
        <underline method="pen" type="straight" language="LA" text="poterit esse nisi &agrave; Marte"/>
        <underline method="pen" type="straight" language="LA" text="super eos de ascensione in descensionem"/> 
        <underline method="pen" type="straight" language="LA" text="mutantur de una triplicitate"/>
        <underline method="pen" type="straight" language="LA" text="ponderosorum, planetae masculini sunt qui agunt, feminini uero sunt"/>
        <underline method="pen" type="straight" language="LA" text="Aspice ascensionem planetae, &amp; gradum exaltationis, gradum quoq[ue] sui casus, nam haec sunt quae significant homines &amp; eorum opera."/>
        <underline method="pen" type="straight" language="LA" text="aget in eum, id est, uincet alteru[m] ille cuius signum est masculinum, &amp; in masculino signo"/>
        <underline method="pen" type="straight" language="LA" text="Saturno in latitudine"/>
        <underline method="pen" type="straight" language="LA" text="&amp; hoc no[n] fallit"/>
        <underline method="pen" type="straight" language="LA" text="Ioui in latitudine"/>
        <underline method="pen" type="straight" language="LA" text="Iupiter &amp; Venus in eodem gradu cum Sole fuerint."/>
        
