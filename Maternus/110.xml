    <page filename="110.tif" reader="John Dee" pagination="87" signature="h 2"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        domin[us] loci &Venus; et &Moon;
                    </marginalia_text>
                    <symbol_in_text name="Venus"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>
                The Lord of the place, Venus and the moon.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Summi oratores. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest orators.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Si uero &Moon; sic fuerit collocata, ut ad nullum feratur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &Moon; cursu vacua.
                    </marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>
                The empty course of the moon.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="si &Venus; uel &Moon;, in &Saturn; uel &Mars; domo"/>
        <underline method="pen" type="straight" language="LA" text="sine opportuno &Jupiter;"/>
        <underline method="pen" type="straight" language="LA" text="sit'q[ue] luminibus plena"/>
        <underline method="pen" type="straight" language="LA" text="opportunis geniturae"/>
        <underline method="pen" type="straight" language="LA" text="in signis"/>
        <underline method="pen" type="straight" language="LA" text="Si uero deficiens"/>
        <underline method="pen" type="straight" language="LA" text="eru[n]t autem parentibus in infinitum inferiores. Corpus"/>
        <underline method="pen" type="straight" language="LA" text="si per uacuu[m] currens"/>
        <underline method="pen" type="straight" language="LA" text="Sed cum animum, &amp; corpus graui"/>
        <underline method="pen" type="straight" language="LA" text="&amp; hac atq[ue] illac, misera peregrinatione pertraxerit, tunc multorum bonorum, &amp; maximae felicitatis decernit augmenta, ut qua[n]tum primae"/>
        
        <symbol name="Saturn" place="right_margin"/>
        <symbol name="Mars" place="right_margin"/>
        
