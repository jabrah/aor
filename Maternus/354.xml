    <annotation>
        
        <marginalia hand="Italian" anchor_text="uirtus enim planetae in tribus consideratur gradibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Virtus planeta[e] in tribus consideratur gradibus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Virtue of planets is considered in three degrees.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="separationibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Applicatio. Separatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Application. Separation.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Eandem in impediendo uim habet dominus 2, quam dominus 8. Similiter dominus , eandem quam dominus 12"/>
        <underline method="pen" type="straight" language="LA" text="Nisi beneuolus planeta prohibeat, quodcunq[ue] dat Mars, reuocat"/>
        <underline method="pen" type="straight" language="LA" text="signo, uirtus enim planetae in tribus consideratur gradibus scilicet in quo est, &amp; in praecedenti, ac succedenti"/>
        <underline method="pen" type="straight" language="LA" text="Fiunt in mundo multa incommoda, quando erit in uno mense utriusq[ue] luminaris eclipsis, &amp; praecipue in his locis, in quibus est significatio eorum specialis"/>
        <underline method="pen" type="straight" language="LA" text="Si Mars fuerit almutaz in natiuitate, &amp; no[n] fuerit iunctus beneuolis, significat quod natus incendium patietur"/>
        <underline method="pen" type="straight" language="LA" text="cum Mars super Saturnum fuerit eleuatus"/>
        <underline method="pen" type="straight" language="LA" text="Si pars coniugii uiri"/>
        
