                    <symbol_in_text name="Conjunction"/>
                </position>
            </language>
            <translation>
                The moon in conjunction with planets.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
