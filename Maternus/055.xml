    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="055.tif" reader="John Dee" pagination="32" signature="[c 4[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Quintus [...] interdum in diametro huius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quinta[m] interdu[m] esse in oposito medij ca[e]li vt aute[m] dixit cap. 8
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The fifth is sometimes in opposition to the middle of the sky, as also said in chapter 8.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="loci ipsius malitia subleuatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Vt sexti loci malitia mitigetur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Because the sixth place is softened by ill will.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="uiolenta cum horosc. societate">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        violenta societali
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By social violence.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Qua r[ati]o[n]e &Moon; sola in 8a domo gaudeat.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For which reason only the moon is in the eighth house.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="fuerit horosc. uites suas per residuas pars 30 extendit"/>
        <underline method="pen" type="straight" language="LA" text="quod nulla cum horosc. radiatione coniungitur, nec aliquo miscetur aspectu"/>
        <underline method="pen" type="straight" language="LA" text="&amp; amicis"/>
        <underline method="pen" type="straight" language="LA" text="locus, cum horosc. maxima societate coniungitur"/>
        <underline method="pen" type="straight" language="LA" text="filioru[m] numerus, &amp; generis aequalitas"/>
        <underline method="pen" type="straight" language="LA" text="locus est &Venus;. Sed"/>
        <underline method="pen" type="straight" language="LA" text="&amp; interdum in diametro huius signi MC pars"/>
        <underline method="pen" type="straight" language="LA" text="causam uitii, ac ualetudinis"/>
        <underline method="pen" type="straight" language="LA" text="sed interdum loci ipsius malitia subleuatur, si in eo loco stella collocata, aliae stellae in decimo ab horosc. loco inuentae, prospera"/>
        <underline method="pen" type="straight" language="LA" text="qualitatem, &amp; quantitatem quaeramus nuptiarum"/>
        <underline method="pen" type="straight" language="LA" text="mortis q[ua]litas inuenit[ur]"/>
        <underline method="pen" type="straight" language="LA" text="genitura, cresce[n]s &Moon; fuerit inuenta, &amp; ea[m] sic posita[m], sine societate, uel raditio[n]e maleuolaru[m] stellaru[m], &Jupiter; hexagono, uel trigono tespexerit"/>
        
