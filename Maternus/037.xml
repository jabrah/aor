    <page filename="037.tif" reader="John Dee" pagination="14" signature="[b[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="generis humani">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Qua[m] fuit humano generi vtile varias facere esse formas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How it is useful to humankind to bring various froms into existence. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="totius orbis Imperator">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Totius orbis Imperator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ruler over the entire planet.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Genitus apud Tharsum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                According to Tharsus begotten.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="aeuum obliquus signorum ordo co[n]uertitur"/>
        <underline method="pen" type="straight" language="LA" text="ut ad harum stellarum cursum, omne auimantium genus, perpetua sibi generis propagatione succedat"/>
        <underline method="pen" type="straight" language="LA" text="quae nigros, ad uariandas hominum formas, stellarum plurimum potestas operatur, &amp; unicuiq[ue] homini"/>
        <underline method="pen" type="straight" language="LA" text="maxima uultuum dissimilitudo uariaret"/>
        <underline method="pen" type="straight" language="LA" text="comprobatur, Zonarum quide[m] esse, quod nigri, uel candidi sunt stelllarum uero, quod in illa uarietate coloris, dissimiliu[m] formarum uarietatibus corporantur"/>
        <underline method="pen" type="straight" language="LA" text="ac totius orbis Imperator"/>
        <underline method="pen" type="double" language="LA" text="Constantinus"/>
        <underline method="pen" type="straight" language="LA" text="scilicet maximus, Diui Co[n]stantini filius"/>
        <underline method="pen" type="straight" language="LA" text="decepit, apud Tharsum genitus"/>
        <underline method="pen" type="straight" language="LA" text="aetatis gradu imperii gubernacula"/>
        
