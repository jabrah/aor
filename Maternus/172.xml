    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="172.tif" reader="John Dee" pagination="149" signature="n 3"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Consiliu[m] rectum qua r[ati]o[n]e in precedentibus versari oporteat.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By which reason a right council ought to dwell.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Nec enim semper ingressus &Venus; loca &Jupiter;, decernit uxores">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ingressus Veneris loca &Jupiter; no[n] semp[er] vxores decernit.
                    </marginalia_text>
                    <symbol_in_text name="Jupiter"/>
                </position>
            </language>
            <translation>
                The entry of Venus into the place of Jupiter does not always determine the consorts. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ex signorum anaphoris prouecti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ex signoru[m] anaphoris provecti.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Advanced from the ascencion of the signs.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nec debes ex malo loco sperare discrimen, cum eu[m] salutare sydus aspexerit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Malus locus no[n] nocet cu[m] eu[m] salutare sydus aspexerit &amp;c
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A bad place does not do any harm when a beneficial star observes it, etc. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="stellaru[m] mixturas"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mixtura[e] stellar[um] in lib. sexto tradet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In book six he conveys the mixtures of stars.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="unius stellae cursu, totam geniturae"/>
        <underline method="pen" type="straight" language="LA" text="duodecim locoru[m] potestatibus"/>
        <underline method="pen" type="straight" language="LA" text="quae stela cum horosc. qua"/>
        <underline method="pen" type="straight" language="LA" text="Nec enim semper ingressus &Venus; loca &Jupiter;, decernit uxores, nec ingressus filioru[m] locu[m] filios tribuit"/>
        <underline method="pen" type="double" language="LA" text="sed illis h[a]ec reddit"/>
        <underline method="pen" type="straight" language="LA" text="q[ui]bus uxores, uel filios prioris substa[n]tia geniturae decreuerit"/>
        <underline method="pen" type="straight" language="LA" text="tunc ex signorum anaphoris prouecti, &amp; temporum"/>
        <underline method="pen" type="straight" language="LA" text="nec debes ex malo loco sperare discrimen, cum eu[m] salutare sydus aspexerit, sicut maleuolarum etiam stellaru[m] minaces radii, ex loci opportunitate franguntur"/>
        
