        <underline method="pen" type="straight" language="LA" text="malitia infortunarum, cum in aliqua dignitatum suarum fuerit, &amp; econuerso."/>
        <underline method="pen" type="straight" language="LA" text="impedimentum, cum fuerit in locis peregrinis"/>
        <underline method="pen" type="straight" language="LA" text="Omnis res quae festinanter fit, &amp; festa[n]ter destruiturm &amp; quae reitera[n]t, est in significatione Martis"/>
        <underline method="pen" type="straight" language="LA" text="Ea quae accidunt in hoc seculo, sciuntur &amp; inuestigantur"/>
        <underline method="pen" type="double" language="LA" text="ex magna"/>
        <underline method="pen" type="straight" language="LA" text="fortitudine superioris significatoris, &amp; ex sua eleuatione, &amp; si ea non fuerit, exquiritur &agrave; planeta, ad"/>
        <underline method="pen" type="straight" language="LA" text="iuerint omnes alii planetae"/>
        <underline method="pen" type="straight" language="LA" text="unicuiq[ue] gradui"/>
        <underline method="pen" type="double" language="LA" text="da annu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Cum aliquis planera dominabitur alicui anno, ex annis mundi, in ipso gradu"/>
        <underline method="pen" type="straight" language="LA" text="exaltionis apparebit eius significatio, &amp; eleuabitur rex in regione illa, &amp; in climate"/>
        <underline method="pen" type="double" language="LA" text="quo "/>
        <underline method="pen" type="straight" language="LA" text="ab ipso significabitur"/>
        <underline method="pen" type="straight" language="LA" text="Cum fuerit utrunque luminarium in eodem gradu, suae exaltionis in suo dominio, liberum ab infortuniis erit natus totius mundi rex,
             &amp; semen eius haereditabit"/>
        <underline method="pen" type="straight" language="LA" text="eius, obtinebit' q[ue] multo tempore"/>
        
