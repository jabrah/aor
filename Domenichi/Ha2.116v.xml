	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						The intention of Eutrapelus, to write, &amp; speake better,
						then euer anie man
						did write, or speake. The final intention of
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Eudromus, to knowe, &amp; do more in sound valu, then euer
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						anie ma[n] knew, or did.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Plus gallo granu[m] hordei,
						qu&agrave;m gemma Orientis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quem vides in vna, virtute deficere, 
						senties deficere in alijs. Sic fer&egrave; in arte.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quis, quid, vbi, quibus auxilijs, cur, quomod&ograve;, quando: }
						Sic sapiens vires ponderat ante suas.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sed qua[e] homines arant, nauigant,
						a[e]dificant; virtuti, et Arti parent omnia.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Artificum princeps.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Qui nouit Domenicu[m], et Gandinu[m]; Philostrati Apolloniu[m], et Juliani Ca[e]sarem; nouit arcana mundi.
						Pra[e]sertim cum Apiani Cosmologo; Bodini Politico, Durandi pragmatico.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Pra[e]stat alijs Eunapius: Eunapio Eutrapelus: vnus
						omnibus magis exquisitus. Sed fluentissimo stylo,
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						eodemq[ue] facillimo.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
