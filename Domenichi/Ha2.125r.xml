	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						All theise external showes ar but rude: nothing fine
						but nimble
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						performance: with cumlie grace.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						(Il Principe del Pontano: ma non del Macchiauelli. Alfonso, non	
					</marginalia_text>
					<person name="Giovanni Pontano"/>
					<person name="Machiavelli"/>
					<person name="Alfonso II of Naples"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Cesare Borgia.)
					</marginalia_text>
					<person name="Cesare Borgia"/>
				</position>
			</language>
			<translation>
				The prince of Pontano, but not the one of Machiavelli. Alfonso II, not Cesare Borgia.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nondum satis ben&egrave;, si quisq[uam] meli&ugrave;s. Nondu[m] optim&egrave;,
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						si quis potest, poteritue meli&ugrave;s.
					</marginalia_text>
				</position>
			</language>
			<translation>
				It is not yet good enough if anyone else is better. 
				It is not yet the best if anyone else can, or will, do better.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( &Ocirc; veramente gallantissimo.
						&Ocirc; puramente brauissimo del mondo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				O truly the most gallant.
				O purely the bravest of the world.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nulla nubecula, in artis, et Virtutis Sole, Eutrapelo. 
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				For Eutrapelus, no cloudiness in the arts with the sun of virtue.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Omnium la[e]tissimus ad hominem:
						sed omnium grauissimus ad rem, /
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Eutrapelus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Eutrapelus, the most cheerful of all towards each man,
				but of all men, the most serious of all towards the task at hand.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						{ Cert&egrave; in Artis, et Virtutis facie, nulla est
						Nubecula: et obscurus est vultus, qui no[n] est illustris.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Undoubtedly, with the appearance of art and virtue, nothing is cloudy.
				And whoever is not bright has a shady countenance.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Singula in vigore: omnia in perfectione. Aut se spernit Eutrapelus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Singular matters in force; all things to perfection.
				Otherwise Eutrapelus spurns himself.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Whose sense, or reason so fine or
						neate, as it should?
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
