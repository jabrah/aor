		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Ludus, iocusq[ue], omne studium practicum.
					</marginalia_text>
				</position>
			</language>
			<translation>A game and a joke, all practical persuit.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
