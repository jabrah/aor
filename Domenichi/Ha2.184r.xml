	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.186r.tif" signature="[M8r]" pagination="191" reader="Harvey"/>	<!-- ABO 188right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						As action; so discourse woold be quick, not rash.
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Not hastie but speedie. 
						) Euer to the point: &amp; as [the] wind conducts.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Euer for the Kinge, &amp; the present State.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						The onlie Parlament-man for regal /
						Statutes.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						All popular, &amp;
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						&amp; Oligarquick Lawes must be reformed.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						( Contentamentot, ricchissima Cosa del mondo. ) 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Sermo datur cunctis; animi sapientia paucis. ( lepidus, not leuis. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Ante&agrave; ita solebam meipsum acuere: Fact mod&ograve; ipsum legere
						Demosthenem: aut Ecc&egrave; audit Cicero. Nunc grauior mihi
						cautio incumbit: Adest, audit, iudicat rex Jacobus.
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						Orationis forma est
						opus plus qua[m] Attica,
						ubi talis iudicat
						plus qua[m] Areopagita.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
