	<page filename="Ha2.179v.tif" signature="[M3v]" pagination="182" reader="Harvey"/>	<!-- ABO 184left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Seu legitur, seu scribitur: siue etiam agitur, siue
						dicitur: summa, summarum est ha[e]c: Non =
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						= Non numero, sed pondere constat dignitias: et
						pauca egregia, plus qu&agrave;m mille
						Laudabilia.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						In sermone,
						odi tautologian: in actione, amo tautopraxian.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Ad punctum Semper.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Nihil aptius Autotechnia.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						&Ocirc; diuina thaumaturga, Fides: omnium miraculoru[m] radix pra[e]potentissima. Ferrei Seculi, rubes magnetica.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Imaginare quotidi&egrave; pra[e]sentem pugnam; candemq[ue] asperrima[m]
						contra robustissimos hostes, atq[ue] ade&ograve; peritissimos.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Dominatur Hector, nisi domat Achilles. Capit
						Dauidem Absolon, nisi capitur &agrave; Chusaio Achitophel.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
