<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.172v.tif" signature="[L4v]" pagination="168" reader="Harvey"/>	<!-- ABO 177left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						A cordial minde, a souerain preseruatiue
						of Witt, &amp; strength.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Machiauellus gratias agat Ca[e]sari Borgia[e],
						eiusq[ue] patri Alexandro Sexto: qui tum
						vulpinandi artem tradebant Roma[e].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Ironia, a present restoratiue at
						all oppositions, or afflictions.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						The most Cordial spirit in the world: &amp; the most radical resolution
						of all men. But teuer with an amiable candour, as gratious as
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						maybe.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Mundus regitur vel perito, et profundo: vel callido, et subtili.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
