	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Puerile est, incipere, nec perficere.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Si adsint libri, promptissim&egrave; consulendi: si =
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						= absint, contemnendi.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Manie can Ciceronise:
						none Ca[e]sarise).
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						| Academici isti, statim Scriptores laudant, aut
						vituperant:
						in artium ipsarum operibus non insistunt:						
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						( non versant pragmateias; non conferunt arcana;
						non faciunt periculum in experimentis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Ars, Virtus; et inde Foelicitas; gnoma[e] gnomarum; apophthegmata apophthegmatu[m];
						emblemata emblematum; postrem&ograve; Historia[e] Historiarum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Qu&agrave;m turpe, atq[ue] stultum est, tantum temporis, studij, opera[e]q[ue] perdere,
						quantum abijciunt, qui incipiunt, nec perficiunt.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

