	<page filename="Ha2.106v.tif" signature="[C2v]" pagination="36" reader="Harvey"/>	<!-- ABO 111left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Animus est omnia. Omnia habet, qui seipsum habet absolut&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The mind is everything. He who has himself absolutely has everything.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						{ Epictetus: Mimi: Italica prouerbia: selecta
						   Plutarchi, Laertij apophthegmata: epigrammatum argutissima.	
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Mimi"/>
					<person name="Plutarch"/>
					<person name="Diogenes Laertius"/>
				</position>
			</language>
			<translation>
				Epictetus, mimes, Italian proverbs, select aphorisms of Plutarch and Laertius, the most clever elements of epigrams. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						ridicolo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Ridiculous.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Pauca fertilissima: sed ad unguem.
					</marginalia_text>
				</position>
			</language>
			<translation>
				There are few most fertile matters, but they are learned by heart.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Totus Mundus in parua Mappa.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The entire world in a small map.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Omnis Inuentio, nihil; sine Critica Epicteti,
						aut Publij. Prosunt Certa, et seria.
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Publius Syrus"/> <!-- Probably Publius Syrus given the reference to him on the next page. --> 
				</position>
			</language>
			<translation>
				In all matters, invention is nothing without the criticism of Epictetus or Publius Syrus.
				Certain and serious things are useful.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Eudromus, euer cheerfull, &amp; most liuelie: alwais industrious, &amp;
						most nimble. Of all men, the most effectual; &amp; the least vaine.
						Neuer regarding their common matters of brawles; but onlie
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						prosecuting his proper actions to the purpose.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Vir ager, semiuir. Tanti est valetudo, et tam instantis momenti.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The wild man is a half-man. Soundness of health is of such great and immediate importance.
			</translation>
		</marginalia>
		
