	<page filename="Ha2.097v.tif" signature="[Bv]" pagination="18" reader="Harvey"/>	<!-- ABO 102left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Veteranus cum desinit exercere, tyro est.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The veteran, when he gives up, is a novice.
			</translation>
		</marginalia>		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vesperi, qu&agrave;m man&egrave; validior, Hic exercitatus.		
					</marginalia_text>
				</position>
			</language>
			<translation>
				Being trained in this place is more powerful in the evening than in the morning.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Stratagema in malam uxorem.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A strategy against a bad wife.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						profunda techna.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A profound skill.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Virtus non senescit, aut languescit.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Virtue neither grows old nor languishes.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Cautela abundans non nocet.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Abundant caution doesn't hurt.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Non autores, sed artes: Vnici studiu[m].
						Non libri, sed scientia[e].
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Non doctrina, sed facultas.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Not authors, but arts: the only thing to study.
				Not books, but knowledge.
				Not instruction, but ability.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						It is not bookes, that makes the skillfull man, but the knowledg of bookes: &amp; the
						memorie of knowledg: &amp; the practis of Memorie, both in Words, &amp; in deeds.
						He deserues to be esteemed the most cunning man,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						that can best negotiate his Lerning, 						
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="90">
					<marginalia_text>
						viua voce, &amp; viuo opere.
					</marginalia_text>
				</position>
			</language>
			<translation>
				with a living voice and living works.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Nemo Suam ipsius Naturam, artem, industriam Satis extendit.
						Quotidi&egrave; frigemus calidissimi.
					</marginalia_text>
				</position>
			</language>
			<translation>
				
			</translation>
		</marginalia>

    </annotation>
