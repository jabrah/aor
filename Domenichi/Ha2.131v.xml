	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Nemo vnq[uam] satis potuit: aut seri&ograve; voluit, quantum
						potuit. Satis ben&egrave; nullus vnq[uam] dixit,
						aut fecit.
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Vt pra[e]stantiori adhuc
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						opus Sit Natura, arte, exercitatione:
						omnin&oacute;q[ue] industria absoluta.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						s[upr]a, 7.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						perit&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nemo la[e]ditur, a Boiardo,
						nisi &agrave; Seipso. Si factum excellenter.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Fato Prudentia maior.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Alij acuere possunt; non possunt la[e]dere.
						Virescit vulnere virtus. Vincit, qui patitur.
						Omnes actiones, passionesq[ue] Nuga[e], pra[e]ter vnu[m]
						Hoc. Proprium quarto modo, Meum Hoc.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						A thousand toyes, not worth a flie, or a flea.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Eutrapelus takes all in gree; nothing in dugen. Shrewd fewllowes may
						be bold to play vpon simple men. Wee pore sowles
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						must be content to carrie coles.
						Misopogon.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
