<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.131v.tif" signature="[F3v]" pagination="86" reader="Harvey"/>	<!-- ABO 136left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Negotium ostendit virum. Et turpe est egregio pragmatico, 
						&agrave; quoq[uam] aduersario
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Superari: aut etiam quemvis
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						a[e]mulum non Superare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Qualis Fortij scopus,
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						omnes pra[e]teritos,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						pra[e]sentes, futuros antecellere, Singulari dignitate.)
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Forte desiderium, et instantissima industria non fallunt in operando.
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Pra[e]sertim cum peritissimo
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						CurSu,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						aut astutissima formula procedendi.
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Qua[e]
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						cert&egrave; dedecet egregiu[m] mundi pragmaticum;
						nisi subit&ograve; emergat omnium subtilissima,
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						et Singulorum profundissima.
						Vulgares sunt,
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						qui non Sapiunt supr&agrave; captum communem: aut non
						semper habent proprium aliquid extraordinariu[m], ac Singulare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						The weightiest discourses, or negociations in the world, light
						with Eutrapelus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Diuino ingenio omnia fluunt sua
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						sponte.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
