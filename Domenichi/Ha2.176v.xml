	<page filename="Ha2.176v.tif" signature="[L8v]" pagination="176" reader="Harvey"/>	<!-- ABO 181left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Qui potest dicere, aut facere meli&ugrave;s; et tamen dicit,
						facitve pei&ugrave;s; Is plan&egrave; fatuus est.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Dulcissimus hominum in Conuersatione:
						acerrimus virorum in actione.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Posthac nihil in libris,
						aut operibus, non summ&egrave; potens, aut
						vald&egrave; efficax. Scholasticarum,
						practicarumq[ue] facultatu[m] ualentissima[e].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Omnium viuidissim&egrave;: semper prudentissim&egrave;:
						et qua[m] fieri potest, efficaciss[im]e.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						In conuersando, Euschemosyne: in negociando, Entelechia: in vtroq[ue],
						Ironia. Sed in omnibus ars celata: et semper inaffectata affectatio.
						Mera flamma sine fumo: et pura actio sine passione.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Of the finest discourses, or worthiest histories in the World; not the tenth part
						is absolutely sound, or sharp. ( No one, for effectual soundnes, &amp; sharpnes,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						equiualent to Ca[e]sar.)
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
