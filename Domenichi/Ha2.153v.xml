	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vir non est, qui non est vir omnium horarum; et apud
						omnes homines vir, quantoscu[m]q[ue]
						magnates; aut principes.
						
						<!-- CG: Missing text added. -->
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quid prosunt omnes artes, aut omnia
						Ornamenta, nisi viro? Nihil habet, qui habet omnia, nisi
						seipsum habeat.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Plus adhuc scientia[e] in esse: plus, plusq[ue] facultatis.
						Ante omnia, et super Singula; Summa[e] Mnemonica[e].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Habere omnes Artes, philosophias, professiones; summa deniq[ue] arcana, et stratagemata;
						mea est singularis professio. Et carum praxi, plurimum omnium
						posse in Mundo, meus vnicus scopus. Nihil alioru[m] studioru[m],
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						aut actionum, huic repugnet scopo.
						Nuga[e], omnia mundi
						oprtata, pra[e] meo vnius pra[e]stantissimo scopo.
						Intima omniu[m] scientiarum 
						practica percallere;
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						eademq[ue] in mundo acerrim&egrave; extendere: nihil vanum, aut inefficax ad mittere:
						mera operandi, negociandiq[ue] puncta prosequi: mea adhuc solius industria.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
