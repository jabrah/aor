	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.100v.tif" signature="[B4v]" pagination="24" reader="Harvey"/>	<!-- ABO 105left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Ars Natur&acirc;, et Exercitatione solidescit.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						As merrie as Sir Thomas Moroe:
						as youthfull as Sir Francis Brian.
					</marginalia_text>
					<person name="Thomas More"/>
					<person name="Francis Bryan"/>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Plurimu[m] valent
						Coma Occasionis, et cuiusq[ue] ansa dexterrima.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Stultus
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						est sapiens,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						cu[m]
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						in bonam
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						partem consulit
						Omnia.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ ( Sol in Scorpione, Sol ).
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Art, or not art,
						Nature, &amp; Exercise will hold their owne.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Magis flagrans Opifex, qu&agrave;m ipse Vulcanus. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						All harsh crudities I disgest with a iest. I beare with
						Him: but beare Him in minde.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Quoti&egrave;s ipse obseruaui, potentiorem esse, Natura[e] et Exercitationis
						Valorem, qu&agrave;m Artis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
