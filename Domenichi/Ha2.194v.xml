	<page filename="Ha2.194v.tif" signature="[O2v]" pagination="212" reader="Harvey"/>	<!-- ABO 199left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Valour goes neuer backward, butt allwais forward.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Holla:
						Hora[e] olim aurea[e]: nunc adamantina[e].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Artes olim ingenua[e]: hodi&egrave; qua[e]stuosa[e]. Mundus
						chremasticus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ a[e]tas mechanica.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Examen ingenioru[m] ponit omnia in dia[e]ta: sed
						necessaria etiam Ars, et Exercitatio.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Dia[e]ta pura, et calida; parua, et cita: Axiophili altera Natura. Mea semper aurea
						dia[e]ta, et adamantina + Pa[e]dia + Parum, sed purum. <!-- CG: Unsure how to register the insertion of Pa[e]dia, if at all necessary. -->
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						All&agrave; sottile, non all&agrave; grossa.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Desine scribere: et facil&egrave; potes, qua[e] vis. Perge quotidi&egrave; vel legere; vel agere; vel etia[m]
						Cantillare: et foelix es. Sed percurrenda, et expedienda omnia diuinit&ugrave;s.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
