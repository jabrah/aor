	<page filename="Ha2.192v.tif" signature="[N8v]" pagination="208" reader="Harvey"/>	<!-- ABO 197left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						&Egrave; viuo fonte Natura[e], artis, exercitationis efficacissima[e]. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nulla egregia virtus, nisi coacta.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quisq[ue] compos, fert ben&egrave; quodq[ue] malu[m]. Axiophilus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Simplices sunt Academici, qui
						poti&ugrave;s ambiunt
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						esse uerbosi philologi, polyhistores, Critici, qu&agrave;m
						actuosi technologi, philosophi, professores.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Promptissim&egrave; singula et dicere, et facere, et perficere, ingeniosum est et quiddam
						diuinum. Qu&ograve; etiam dexteri&ugrave;s, suaq[ue] sponte fluenti&ugrave;s, e&ograve; ca[e]lestius, et
						quasi &egrave; Natura[e] sinu. Balbutire autem in loquendo, aut in agendo deficere
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						aliquo modo:
						vel infantis est, uel hominis plan&egrave; rudis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						<!-- Continued from facing page, left margin, Ha2.193r -->
						= Quia possunt? quantutm valent. Cui priuata[e], aut publica[e] facultati conducunt?
						Iam enim Artes aut in Commoditatum numero reponuntur; aut nullo sunt
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						pra[e]dicamento.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
