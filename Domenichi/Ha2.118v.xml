	<page filename="Ha2.118v.tif" signature="[D6v]" pagination="60" reader="Harvey"/>	<!-- ABO 123left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Thomaso Moro, risoluto, e faceto.
						come Luciano, &ograve; Martiale.
					</marginalia_text>
					<person name="Thomas More"/>
					<person name="Lucian"/>
					<person name="Martial"/>
				</position>
			</language>
			<translation>
				Thomas More was as resolute and witty as Lucian or Martial.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Non curo, qua[e] non procuro. )
					</marginalia_text>
				</position>
			</language>
			<translation>
				I do not care for what I cannot manage.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Ha[e]c oportet prosequi: illa non negligere.
					</marginalia_text>
				</position>
			</language>
			<translation>
				It is necessary to follow these matters and not neglect those other ones.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Hodi&egrave; meis Artibus: Cras tuis )
						Discursibus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Today with my skills, tomorrow with your discourses.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Plurimi vbiq[ue] Scholastici: paucissimi vsq[ue] Technopractici.
						Academici termini sine pragmaticis operibus inanes.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Everywhere there are many scholastics and very few pragmatic skilled men. <!-- CG: Odd neologism --> 
				Academic terms without pragmatic activities are worthless.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Scitum est, quemq[ue] parare sua[e] Hypothesi necessaria.
					</marginalia_text>
				</position>
			</language>
			<translation>
				It is best for one to prepare the necessary components of his proposition. 
			</translation>
		</marginalia>
		
