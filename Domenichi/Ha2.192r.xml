	<page filename="Ha2.192r.tif" signature="[N8r]" pagination="207" reader="Harvey"/>	<!-- ABO 196right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quasi virgula diuina suppeditata omnia.
					</marginalia_text>
				</position>
			</language>
			<translation>As if everything has been made available by a divine rod.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Tota Ethica, quatuor aurea uerba:
						et omnium Historiarum summa
						adamantina.)
					</marginalia_text>
				</position>
			</language>
			<translation>All ethics consists of four golden words: and the highest incorruptable form of all histories.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Prouidus est plenus: improuidus exstat egenus.
					</marginalia_text>
				</position>
			</language>
			<translation>The prudent man is full: the man lacking prudence appears beggarly.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Magna[e] cura[e] magna merces.
					</marginalia_text>
				</position>
			</language>
			<translation>A great reward for a great cure.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>s[upr]a /</marginalia_text>
				</position>
			</language>
			<translation>[See] above.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Qui in Summis nauigat, in nummis anchorat.
					</marginalia_text>
				</position>
			</language>
			<translation>He, who navigates in the highest spheres, anchors in money.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						In mundo Summus Rex est hoc tempore Nummus.
					</marginalia_text>
				</position>
			</language>
			<translation>In this time, money is the highest king in the world.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Omnia leui pollice: quasi procurante Deo.
					</marginalia_text>
				</position>
			</language>
			<translation>Everything by a quick thumb: as if by a managing God.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Vnicus Sua[e] domus, patria[e], Mundi Oeconomus.
					</marginalia_text>
				</position>
			</language>
			<translation>The unique home of his fatherland, the treasurer of the world.</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						In qua[e]rendo scrupulosus: in inueniendo curiosus: in prosequendo acer: in
						exequendo peremptorius. Cursus solidus
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						pra[e]cis&egrave; obseruandus.
					</marginalia_text>
				</position>
			</language>
			<translation>Exact in seeking, careful in finding, sharp in following through, decisive in carrying out. 
				A firm course must be observed precisely.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Maxim&egrave; ab omnibus laudatur, qui sua omnia dicta, scripta, facta expedit /
						facillim&egrave;. Cui nullus Labor est labor: sed ludus, iocusq[ue], maxima operum
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						humanoru[m]. Vir est, qui omnia
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						putat,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						facitq[ue] leuissima, qua[e]
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						alijs
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						uidentur mult&ograve; grauiss[im]a.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He, who provides all his sayings, writings, and deeds most easily, is praised most greatly by all;
				he, for whom no work is work, but for whom the greatest works of men are but a game and a joke;
				he, who settles everything and renders most lightly the things which seem to others most heavy, he is a man.</translation>
		</marginalia>
		
