	<page filename="Ha2.194r.tif" signature="O2[r]" pagination="211" reader="Harvey"/>	<!-- ABO 198right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Lentum est, dabit: dat, hoc quoq[ue] est lentum: dedit.
						Seneca in Hercule furente.
					</marginalia_text>
					<person name="Seneca"/>
					<book title="Hercules Furens"/>
				</position>
			</language>
			<translation>"Shall pay is slow – he pays; that, too, is slow – he has paid." [trans. Frank Miller, 1917]. Seneca in 'Hercules furens'.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Non lenta Herculis via, vt Bacchi.
					</marginalia_text>
					<person name="Hercules"/>
					<person name="Bacchus"/>
				</position>
			</language>
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Lentezza, vilt&agrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>Hercules' life is not sluggish, as is Bacchus'.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Fredericus 2. Dania[e]
					</marginalia_text>
					<person name="Frederick II of Denmark"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Rex.
						&Egrave; principe, priuatus:
						&egrave; priuato, Princeps.
						Vtrunq[ue] decor&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>Frederick II, king of Denmark. Out of a prince, a private citizen; out of a private citizen, a prince. Both honorable.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Pater hodierni Regis adolescentis, Christiani 4.
						Cert&egrave; egregius vir, et magnus Rex.
					</marginalia_text>
				</position>
			</language>
			<translation>The father of today's adolescent king, Christian 4.
			Clearly a famous man and a great king.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Theise great Lords, little actours: but pretty men for a time. A short floorish, &amp; Godnight.
						<!-- CG: Missing text added. -->
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Fac audire Demosthenem, Homeru[m]:
						Legere Ciceronem, aut Virgilium:
						et singula accuratissim&egrave;.
					</marginalia_text>
					<person name="Demosthenes"/>
					<person name="Homer"/>
					<person name="Cicero"/>
					<person name="Virgil"/>
				</position>
			</language>
			<translation>Do listen to Demosthenes and Homer. Do read Cicero, or Virgil: and singular authors most accurately.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Audit Deus: leget
						Rex: iudicabit Criticus
						Mundus.
					</marginalia_text>
				</position>
			</language>
			<translation>God listens; the king will read; the world-critic will judge.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Nec in astra lent&acirc; veniet, ut Baccus, vi&acirc;. Juno de Hercule.
					</marginalia_text>
					<person name="Bacchus"/>
					<person name="Juno"/>
					<person name="Hercules"/>
				</position>
			</language>
			<translation>He does not come into the skies in a sluggish way, as Bacchus. Juno on Hercules.</translation>
		</marginalia>
		
