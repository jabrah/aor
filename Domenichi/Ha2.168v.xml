	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Scopus { Ordinarius, { 	Politicus. Pragmaticus.
									Polemicus.
							  Extraordinarius { Mathematicus. thaumaturgicus.
							  		    Apodemicus.	
					</marginalia_text>
				</position>
			</language>
			<translation>Goal. Ordinary. Politician. Pragmatist. Polemicist. Extraordinary. Mathematician. Thaumaturgist. Emigrant. </translation> <!--JG: Not sure abt 'apodemicus'-->
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Singula strict&egrave;,
						energetic&egrave;, absolut&egrave;. Vt nihil efficacius,
						aut potentius.
					</marginalia_text>
				</position>
			</language>
			<translation>Singular things, strictly, energetically, absolutely. As nothing is more effective and more powerful.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Homo bulla.)
					</marginalia_text>
					<X-ref person="Marcus Terentius Varro" book_title="Rerum Rusticarum Libri Tres" language="LA" text="Homo bulla"/> <!--JG: Could also tag Erasmus' Adagia, from which Harvey probably got this expression...-->
				</position>
			</language>
			<translation>'Man is a bubble'.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Vir palma.
					</marginalia_text>
				</position>
			</language>
			<translation>Man is a palm.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Qua[e] calent, valent: et qua[e] coguntur,
						pra[e]pollent. Vis inferenda cuiq[ue] parti.
					</marginalia_text>
				</position>
			</language>
			<translation>The things which are hot, prevail: and the things which are collected, are very strong. To which part strength must be inflicted.</translation> <!--JG: Unsure-->
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Instrumentum Eudromi catholicum; expeditissima facultas
						Artium, Philosophia[e], Professionum: quatenus operantur in
						Mundi negocijs subtilissim&egrave;.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
			<translation>Eudromus' universal instrument; the most expedient faculty of arts, philosophy, and professions: seeing that they work most subtle in the business of the world.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Quotidiana a[e]mulatio excellentissimoru[m] artificium, et pra[e]stantissimorum operum in Esse.
					</marginalia_text>
				</position>
			</language>
			<translation>The daily emulation of the most excellent professions and of the most outstanding works in being.</translation>
		</marginalia>
		
