	<page filename="Ha2.109v.tif" signature="[C4v]" pagination="40" reader="Harvey"/>	<!-- ABO 113left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Mens cuiusq[ue] est quisq[ue]. Mentis magnitudo,
						pro virtutum, artiumq[ue] quantitate.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) fortuna, ludibrium philosophis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Virile apophthegma, et argutum epigramma.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						risoluto, s[upr]a, i[nfr]a.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Resolute, above, below.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Tutto vno.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nihil molestat philosophu[m], aut turbat virum.
						Epictetus non curat, qua[e] non sunt in sua potestate.
					</marginalia_text>
					<person name="Epictetus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Si Cynismus per se tantus: quant&ograve; maior, efficacissimis
						artibus instructus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Orthopoedia statim ediscit, et operatur, qua[e] sunt cuiusq[ue] artis vtilissima.
						Non magis Cebetis, aut Epicteti Orthopoedia, qu&agrave;m Axiophili: min&ugrave;s etia[m],
						qu&agrave;m Eudromi.
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Cebes"/>
					<person name="Axiophilus"/>
					<person name="Eudromus"/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Orthopa[e]dia Virtus: sed peritior Orthopa[e]dia, qu&ograve; Arte excultior.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
