	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quicquid tibi opponitur: tu statim redi ad rem.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whatever is set against you, immediately go back to the matter. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						intacta contumelia: tanq[uam] nihil tale.
					</marginalia_text>
				</position>
			</language>
			<translation>
				An unanswered affront, just as if such a thing were nothing.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						{ Virtus per se, A per se A:
						per accidens, quid non?
					</marginalia_text>
				</position>
			</language>
			<translation>
				Virtue by itself, A by itself A:
				by accident, why not?
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Omnia seri&ograve;,
					</marginalia_text>
				</position>
			</language>
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						quoth Remington:
					</marginalia_text>
					<person name="Richard Remington"/>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Omnia ioco,
					</marginalia_text>
				</position>
			</language>
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						quoth J. )
					</marginalia_text>					
				</position>
			</language>
			<translation>
				"All things seriously," quoth Remington.
				"All things jokingly," quoth I.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Ille
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						contradicit inuid&egrave;: tu ride, et sine interruptione redi
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						ad punctu[m].
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Spernendo, optim&egrave; maxim&egrave; confutatur aduersarius.
					</marginalia_text>
				</position>
			</language>
			<translation>
				In envy he contradicts you. You, laugh, and without interruption return to the point.
				Through scorn, an adversary is refuted most and best.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						a brace of fillips.
						{ Tut, poogh. A fiddle stick. A fig. A fillip. bawe. bawewawe owt owt.
						troogh. fie fie. fah. blurt. a tuck. (
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						gather togither all your needles, &amp; bodkins.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Virgilius olim Sirenophoenix: nuper Bartasius hodi&egrave;
					</marginalia_text>
					<person name="Virgil"/>
					<person name="Guillaume Du Bartas"/>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						vnicus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Formerly Virgil was the Sireno-phoenix; in these days, Bartas is the only one.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Vt priuatio: sic Calumnia non est
						in meis pra[e]dicamentis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Like privation, calumny is thus not one of my qualities.
			</translation>
		</marginalia>
		
