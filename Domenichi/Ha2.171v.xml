	<page filename="Ha2.171v.tif" signature="[L3v]" pagination="166" reader="Harvey"/>	<!-- ABO 176left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vita[e] vitalis quotidiana restauratio.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Hebes est, qui non est subtilior Scoto.
						Rudis est, qui non est callidior Durando.
						Simplex est Politicus, qui non est
						Bodino, Lipsioq[ue] magis compositus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Dia[e]ta Cordialis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						The most precious &amp; souerain thing in the world, next diuine Witt; is a mans
						Natural heate, &amp; radicall humour: which abooue allthings deserues to be mightily
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						preserued w[i]th all the Cordials, and
						restoratiues vnder the Sun.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Moderata Venus iuueni prodest: viro nec prodest, nec obest:
						seni nocet maxim&egrave;. Omnis luxuria senectutum reddit inualida[m],
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						eneruatam, infirma[m], a[e]gram, 
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						decocta[m],
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						effectam, exangue[m],
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="270">
					<marginalia_text>
						exhaustam, vetula[m], decrepita[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
