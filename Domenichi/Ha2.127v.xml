	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Qu&agrave;m diu iocabatur &AElig;sopus, successum est ei
						foeliciter: cum primu[m]
					</marginalia_text>
					<person name="Aesop"/>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						seri&ograve; agebat, et sua[e] est oblitus ingeniosa[e]
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Ironia[e], statim perijt miserrim&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Aut
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						non erant proucandi
						Delphici sacerdotes:
						aut ita tractandi, vt poste&agrave; agebat Socrates
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						cum sophistis quos laudans
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						rearguebat; redarguens laudabant.
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Probo arguta apophthegmata, dicteria, problemata:
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						epigrammata, satyras: sed conditas salibus,
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						et iocis:
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						quorum est in omni stylo,
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						et sermone
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						perpetuus vsus. Nec vlla ars
						pra[e]gnantior,
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						aut frequentior in humana vita[e], qu&agrave;m ha[e]c ciuilium
						Iocorum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Idea in manu.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Doctoris est docere: aliorum discere. Illius superare: horum submitti. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Plurimu[m] possunt planeta[e]: sed plus Elementa: quia magis
