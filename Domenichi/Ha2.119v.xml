	<page filename="Ha2.119v.tif" signature="[D7v]" pagination="62" reader="Harvey"/>	<!-- ABO 124left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quid potest plus qu&agrave;m alij? Mundi qua[e]stio
						et intimus prudentissimoru[m] hominu[m] respectus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				What can one do more than others? The question and innermost
				consideration of the most prudent men.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) Quors&ugrave;m tot artes, arcana, stratagemata?
					</marginalia_text>
				</position>
			</language>
			<translation>
				To what purpose all of the arts, secrets, and stratagems?
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Summa summarum est, plus actu posse, qu&agrave;m alij.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The summa of summas is to be able to do more in action than others.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Vt Paulus: sic Apollonius: sic Maximus erat duo,
						Orator, et Thaumaturgus.
						Erant omnes vnus, id est, Mercurius.
					</marginalia_text>
					<person name="St. Paul"/>
					<person name="Apollonius of Tyana"/>
					<person name="Valerius Maximus"/>
					<person name="Mercury"/>
				</position>
			</language>
			<translation>
				Just as St. Paul, so too Apollonius of Tyana; so too Valerius Maximus,
				who was two, both orator and thaumaturge.
				They were all one, that is, Mercury.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Mercurij pra[e]cipua mnemosyna; ala[e] virga
						angui potens; [doli], furta; diuina Eloquentia.
					</marginalia_text>
					<person name="Mercury"/>
				</position>
			</language>
			<translation>
				The particular symbols of Mercury: the mighty rod with the winged serpent,
				trickery, theft; divine Eloquence.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Mihi textus: tibi glossa. Mihi operatio: tibi speculatio.
						Non curo arguere, sed operari.
					</marginalia_text>
				</position>
			</language>
			<translation>
				For me, the text; for you, the gloss. For me the work, for you the speculation.
				I do not care to argue, but to act.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Maximus vnus perplacuit Juliano: dictis oraculu[m]; factis
					</marginalia_text>
					<person name="Julian"/>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						Miraculu[m].
					</marginalia_text>
				</position>
			</language>
			<translation>
				One great man most pleased Julian; an oracle in words, a miracle in deeds.
			</translation>
		</marginalia>
		
