	<page filename="Ha2.124v.tif" signature="[E4v]" pagination="72" reader="Harvey"/>	<!-- ABO 129left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Eutrapelus, pura ars in persona, et mera virtus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Singula facil&egrave; excellentiss[im]a omnia plan&egrave; efficacissima.
						Nihil vanum pulchra vniuersa singula
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						dicta, scripta, facta in flore, et fructa
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Artis, et Virtutis pra[e]domina[n]tis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( What spirit so pure, &amp; neate as it /
						should ?
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ Quicquid alij vulgares;
						Eutrapelus pro actionum, aut personaru[m]; locorum,
						aut temporum occasione; vel pura Ars,						
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						et mera Virtus:
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						vel mera Ars, et pura Virtus.
						Nihil supr&agrave; in vtraq[ue]: nihil in alterutra.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Nihil magis conspicuum, aut optatum: nihil tam excellens aut memorabile,
						qu&agrave;m Eutrapeli Singula. Sed tamen animus in carere Corporis,						
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						perpetuis eget Mnemosynis, et 
						diuino Calcare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
