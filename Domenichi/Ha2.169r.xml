		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Pra[e]stantissimi viroru[m], paru[m], aut nihil scribebat.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nihil Elias, Elisa[e]us, ipse Christus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nihil Pythagoras, Socrates,
						Apollonius.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Parum Moses, Daniel; David, Saloman; Apostoli.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sua fer&egrave; Moses, Esdras; Xenophon, Ca[e]sar.	
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Aut nihil: aut vtilissima humano generi.
						Multa paucis: vt omnes diuini Libri.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Entelechia, et Euschemosyne, instar omnium libroru[m], et instrumentorum. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Vulgaria supplementa omniu[m] defectuum, moru[m] humanitas, ingenij
						dexterias, corporis agilitas, et perpetua industria. Vera
						defectuum supplementa, Ars, et virtus in quaq[ue] specie: sed promptiss[im]a.
					</marginalia_text>
				</position>
			</language>
