		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Ignavus est, et sordidus, qui non dicit, scribit, agit
						excellenti&ugrave;s,
						qu&agrave;m isti vulgares: qui mundum circumagitant,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						et interturbant omnia: vt Davus in Comoedia.
						Multi politici, et pragmatici Daui: pauci Mitiones
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						et Scipiones.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						dilemma retortum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Multa dicuntur: pauca fiunt.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Flamma Sine fumo: solus Actor.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Stultus sapit, qui in principio cogitat de fine.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Magnus strepitus: parvus effectus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Axiophilus de Eutrapelo. Quis novus eluxit solerti Da[e]dalus orbi,
						vultu, voce, animo, techna insuperabilis, aut re? Delitium in dictis,
						in factis viua procella. Semper ad effectum: puncto strictissimus
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						omni.
					</marginalia_text>
					<emphasis method="pen" type="straight" text="omni."/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Vt Vulcanus, sic Da[e]dalus; Semper ad incudem: puncto strictissimus omni. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
