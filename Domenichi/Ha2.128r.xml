		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Nothing more excellent in Proaeresius himself, then a
					</marginalia_text>
					<person name="Prohaeresius"/>
					<emphasis method="pen" type="straight" text="Proaeresius"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						cumlie grace; fine discretion;
						&amp; a nimble
						 dexteritie to eueri
						 purpose: with a sweet descanting at anie
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ fitt occasion; &amp;
						allwais flowing vtterance at will; as pithie in
						sentences, as elegant intermes, &amp; swift in deliuerie.
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Euer as wittie, and pleasant as the matter
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ will beare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quisq[ue] in sua Arte.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Each in his art.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Bacchus, sed Osiris: Liber, sed Apollo: )
						( Euhius, sed fulmineus. Euohe./
					</marginalia_text>
					<person name="Bacchus"/>
					<person name="Osiris"/>
					<person name="Liber"/>
					<person name="Apollo"/>
				</position>
			</language>
			<translation>
				Bacchus, but Osiris; Liber, but Apollo.
				Bacchic, but destructive. Euohe!
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
