		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Primus Epictetus pragmaticus: tam foris serius,
					</marginalia_text>
					<person name="Epictetus"/>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						qu&agrave;m intus solidus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Post philosophi seruitutem, statim
						&AElig;sopus regius professor: et magnus Aulicus apud
						Croesum, Lydoru[m] regem;
						Babyloniorum: Nec tenabonem, A[e]gyptiorum.
					</marginalia_text>
					<person name="Aesop"/>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Summus etiam orbis Apodemicus, vbiq[ue]
						cum philosophis disputans, qu&agrave;m illi acubi&ugrave;s: et ipsius
						Delphici Oraculi contemptor: lic&egrave;t iusti propter
						sacerdotum inscitiam, tamen non imp&ugrave;ne propter eorum
						inexorabilem sa[e]uitiam.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Iuuat hodi&egrave; mouere: cras quiescere. Hodi&egrave; uigilare:
						cras dormire.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Scitum est hodi&egrave; ridere: cras plorare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
