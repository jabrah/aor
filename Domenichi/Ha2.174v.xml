	<page filename="Ha2.174v.tif" signature="[L6v]" pagination="172" reader="Harvey"/>	<!-- ABO 179left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						( Omnis doctrina aut practica; aut phantastica.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Non est Entelechia, qua[e] omnes non Superat praxes
						aureas. Tam Subtilissima, qu&agrave;m celerrima.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Quicquid non est solare, est vmbratile.
						Vt festa confestim: sic omnia ludicra.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Sola energetica, et practica in artibus, philosophia, professionibus. Ca[e]tera
						phantastica, aut sterilia. Mundus non eget friuolis, aut superfluis:
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						sed necessarijs, et vtilib[us].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Nond&ugrave;m notum orbi, quantu[m] valeat aphoristica artiu[m], professionu[m]q[ue] expeditio.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
