	<page filename="Ha2.117r.tif" signature="[D5r]" pagination="57" reader="Harvey"/>	<!-- ABO 121right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Plus adhuc, plusq[ue] spiritus: sed mult&ograve; magis plus,
						plusq[ue] valoris.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Still more and more spirit; yet even more and more valor. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Industria, singuloru[m] maxima: sed in rebus omnium maximis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Industriousness: greatest of singular matters, but counted among the greatest of all things.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quantum rei momentum, tanta cura.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The greater the importance of some matter, the greater the attention to that matter.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Perditus dies, qui non proficiens.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He who does not make process has wasted the day.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						&Ocirc; miser, cui pereunt foelices hora[e].
					</marginalia_text>
				</position>
			</language>
			<translation>
				O miserable is the man, for whom the happy hours pass by!
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Alij profiteantur: Eutrapelus proficiat.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				Let others be acknowledged; let Eutrapelus make progress.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						{ Pure Quintessence, meere flame, no fuming smoke:
						More fine then wildfire, sterne then hart of Oke.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
