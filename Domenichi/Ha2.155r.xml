	<page filename="Ha2.155r.tif" signature="I3[r]" pagination="133" reader="Harvey"/>	<!-- ABO 159right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vnus auctor Optimus, instar reliquoru[m] in eodem genere
						Omnium. Non alius, sed aliud: ait quotidi&egrave;
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						proficiens.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Vnus Excellentissimus, quasi omnes: alij omnes, ne
						vnus quidem.
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Vno, m&agrave; vnico.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Tam nouis excogitandis, qu&agrave;m veteribus examinandis valet
						Inuentionis et Judicis ars. diuina Euretice,
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Nondum est prudens, qui sibi ipse non est
						instar omnium Librorum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Verbum sapienti sat: et prudens sibi ipse regula. Semper ego auditor tantu[m]?
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Is dem&ugrave;m est artifex, qui in seipso habet omnes artes, philosophias,
						professiones. Nec solid&egrave; doctus est, qui non posset omnes artes supplere,
						si nihil esset Librorum in Mundo.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
