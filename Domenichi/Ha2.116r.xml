	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						The dowtiest actions ar to be managed valiantly, &amp;
						pleasantly.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						The most laborious works to be dispatched constantly,
						and delightfully. 
						Allthings woold be doone diligently, &amp; delectably.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						4.to di Guazzo.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Virtu doth allwais dispose the Will, to accomplish
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						her actions reddily,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						&amp; delectably. A nimble &amp; iollie actour.
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						A speedie, and gallant man.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						vna distinctionu[m] inter principes, et
						priuatos.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						( A serious, &amp; cheerfull workman. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
