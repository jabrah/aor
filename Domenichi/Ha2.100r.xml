	<page filename="Ha2.100r.tif" signature="B4[r]" pagination="23" reader="Harvey"/>	<!-- ABO 104right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian" marginalia_continues_from="Ha2.099v_0001" marginalia_from_transcription="Ha2.099v.xml" book_id="FolgersHa2" id="Ha2.100r_0001">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						<!-- Continued from facing page, head, Ha2.99v --> [B3v]: { Pro vno Amico singula:] 
						{ In vnum Hostem omnia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				[[B3v]: Singular things on behalf of one friend:] All things against one enemy.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Minuta non curat Jurisprudentia: summa
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						considerat, et finem respicit.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Jurisprudence cares not for minutiae, but considers the highest matters
				and looks towards the end.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Aurifaber Fortuna[e]. Centimanus.
					</marginalia_text>
					<person name="Appius Claudius Caecus"/>
				</position>
			</language>
			<translation>
				The goldsmith of Fortune. Appius Claudius Caecus.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Eutrapelus, a peerles 
						Artist: a matchles Professour: &amp; a most excelle[n]t
						man, at euerie proofe of the Worthiest men.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						At anie negotiation, euer Singular, for vigour, reason, &amp; diligence: which
						easely supplie other defects; &amp; generally reigne in all specials.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						In illum ingentem omnia terribilia: sed
						Cryptic&egrave;: vt Themistocles in Xerxem.
					</marginalia_text>
					<person name="Themistocles"/>
					<person name="Xerxes"/>
				</position>
			</language>
			<translation>
				All things are terrifying in that remarkable matter: but
				concealed, as Themistocles against Xerxes.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Multi Patrocli: vnus Achilles. Non multi Pompeij: vnicus Ca[e]sar.
					</marginalia_text>
					<person name="Patroclus"/>
					<person name="Achilles"/>
					<person name="Pompey"/>
					<person name="Julius Caesar"/>
				</position>
			</language>
			<translation>
				Many Patrocluses, one Achilles. Several Pompeys, one sole Caesar.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Pro Rege, aut Duce Optima: contra Regem, aut Ducem Maximum. 
					</marginalia_text>
				</position>
			</language>
			<translation>
				On behalf of the king or best leader: against the king, or greatest leader.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Resolutio Achitophelis: executio Jehu: scriptio Esdra[e].
					</marginalia_text>
					<person name="Ahitophel"/>
					<person name="Jehu"/>
					<person name="Ezra"/>
				</position>
			</language>
			<translation>
				Ahitophel's solution; Jehu's execution; Ezra's writing. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Sapit, qui prouidet suo Microcosmo: tam matur&egrave;, qu&agrave;m apt&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He who provides for his microcosm both at the right time and fittingly is one who understands.
			</translation>
		</marginalia>
		
