		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Negocium ostendit virum. In quo decet esse
						serium virum: atq[ue] ade&ograve; virorum virum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nihil magis commendat virum, qu&agrave;m prosequendi, expediendiq[ue]
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						negocij promptissima resolutio, dexteritas, industria,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) facultas: 
						postrem&ograve; inuicta constantia; cum effectu plan&egrave; efficacissimo.
						Quo magis insperato, aut arcano, e&ograve; magis egregio,
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						et honorifico.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Ipse Diogenes, qui
						Aristippum spreuit, et Platonem, voluisset esse aulicus,
						A[e]sopi vitam cognouisset. Cui foelici&ugrave;s successum est apud Reges,
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						qu&agrave;m apud sacerdotes, aut philosophos.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Sollus Chymicus nouit vsum, et abusu[m] eleme[n]toru[m]: =
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						= quibus nihil potentius.
					</marginalia_text>
