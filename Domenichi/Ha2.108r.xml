	<page filename="Ha2.108r.tif" signature="C4r" pagination="39" reader="Harvey"/>	<!-- ABO 112right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Igneus est ollis vigor, et coelistis origo.
						Axiophili tandem Emblema.
					</marginalia_text>
					<person name="Axiophilus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						/ Simplicitie
						Wrangles about trifles: iudgment
						) contemnes common
						debates, &amp;
						scornes a thousand quarrels.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Mulier perpaucarum Mulierum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				A woman of very few women.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Singula vel potenti&ugrave;s, vel dexteri&ugrave;s.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Singular matters either more effectively or more skillfully.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Omnia in Xenophonte, et Ca[e]sare expedita, et quasi sua sponte fluentia. Nihil affectatum. Summa eorum laus 
					</marginalia_text>
					<person name="Xenophon"/>
					<person name="Julius Caesar"/>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						in claritate, prudentia, vigore.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
			
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Artifex ille est peritus, qui singulas sui corporis, animiq[ue] partes, uel
						fortiores facit, vel saltem dexteriores, qu&agrave;m sunt aliorum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Skilled is that artist who makes the singular parts of his body and mind stronger, or at least more dexterous than those of other people.
			</translation>
		</marginalia>

