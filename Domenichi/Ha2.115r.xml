	<page filename="Ha2.115r.tif" signature="D3[r]" pagination="53" reader="Harvey"/>	<!-- ABO 119right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Non tam lege, qu&agrave;m repete, repete, repete qua[e] lecta
						sunt vtilissima. In animo scribit prudens, quicquid
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						esse sentit efficacissimu[m]. Ca[e]tera concurrunt sua sponte.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Don't just read, but repeat, repeat, repeat those readings which are most useful.
				The prudent man will write in his mind whatever he feels to be most efficacious.
				Other things will happen of their own accord.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( The best, that euer Writt.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						A per se A.)
					</marginalia_text>
				</position>
			</language>
			<translation>
				( The best, that euer Writt.
				A in itself A. )
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Or henceforth nothing at all.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Virgilio plus Virtutis in A[e]neide:
						Ouidio plus Artis in Metamorphosibus. Vtriusq[ue]
					</marginalia_text>
					<person name="Virgil"/>
					<person name="Ovid"/>
					<book title="Aeneid"/>
					<book title="Metamorphoses"/>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						plurimu[m] in Seneca[e] tragoedijs: vt etiam
					</marginalia_text>
					<person name="Seneca"/>
				</position>
				<position place="tail" book_orientation="0">
					<marginalia_text>
						in Lucano. Pra[e] quibus alij pla[e]riq[ue] poeta[e], quasi
						nuga[e], et fragmenta rerum vulgarium.
					</marginalia_text>
					<person name="Lucan"/>
					<X-ref person="Petrarch" book_title="Rerum vulgarium fragmenta" language="LA" text="fragmenta rerum vulgarium"/>
				</position>
			</language>
			<translation>
				More Virtue than Virgil in the Aeneid, more Art than Ovid in the Metamorphoses.
				More of both in Seneca's tragedies, and even in Lucan. 
				Compared with them, most other poets are almost trifles and 'fragments of common things'.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						s[upr]a /
					</marginalia_text>
				</position>
			</language>
			<translation>
				[See] above.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Pulchr&egrave;: sed Memorem mones: et agilem moues.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Excellently, but you remind the mindful man, and you move the agile man. 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Quicquid optimu[m], Noster ulteri&ugrave;s.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Whatever is best is ours in the greatest degree.
			</translation>
		</marginalia>
		
