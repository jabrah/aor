	<page filename="Ha2.087r.tif" signature="[**7r]" reader="Harvey"/>	<!-- ABO 91right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						In pla[e]risq[ue] rarus: in nullo insolens.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Rare among the majority of men: arrogant among no man.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Laudarem Oleum, nisi medullam gustassem.
					</marginalia_text>
				</position>
			</language>
			<translation>
				If I had not tasted the marrow, I would praise oil.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Supra Vulgus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Above the common man.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Erigete super alios:
						saltem erecta mente.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Elevate yourself above the others, at least with an elevated mind.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Tanti eris alijs, quanti fueris tibi.
					</marginalia_text>
				</position>
			</language>
			<translation>
				<!--JG: Translation missing!-->
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Homo vulgaris est, /
						/ non vir Singularis, qui non /
						facil&egrave; pra[e]lucet omnibus.
						Iuuat ire per altum.
						Sed nemo uolat sine alis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				He who does not easily outshine all others is a common man, a man who is not singular. 
				It is useful to pass through high places.
				But no one flies without wings.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Looue, the strongest Charme, that is.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Laconismus, et Cynismus. Compendiaria
						Ars virilis virtutis.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Laconism and Cynicism. The quick art of virile virtue.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Acerrimor[um] hominu[m] vir maxim&egrave; serius: et vnic&egrave; momentosus ad omnem occasionu[m] instantiam. 
					</marginalia_text>
				</position>
			</language>
			<translation>
				A man from the most sagacious people is especially serious and especially quick in every opportunity.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Laudo in ciuibus Laconismum: non reprehendo in philosophis Cynismum.
						Alij Artium studiosi, idq[ue] ben&egrave;: hi virtutum obseruantissimi, idq[ue] optim&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				I praise Laconism in citizens. I do not censure Cynicism in philosophers.
				Others are fond of the arts, and this is well; yet these men are most observant of virtue, and this is the best.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Lacones, et Cynici, virtute instructi, se nulla alia re egere asserebant.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Laconic and Cynical men, instructed in virtue, claimed that they required no other thing.
			</translation>
		</marginalia>
		
