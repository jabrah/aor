	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Plus spiritus: plus, plusq[ue] viuida[e] affectationis.
						Sed pragmatica affectatio, occulta proprietas.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Natura dell'huomini.	
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Animus int&ugrave;s A[e]tna: extr&agrave; Tempe. Quicquid
						obijcitur, aut opponitur; semper magis,
						magisq[ue] Ironia. Contemnens maxima opposita,
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						tanq[uam] nihil; apud tant&ograve;
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) maiorem animu[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						risoluto.	
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						{ Certa, ett stata corporis durities:
						cum perpetua agilitate. Habitus solida[e].
						Constitutionis inviolatus. Nihil effoeminatu[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						He is a right man, that doth enhable euerie part of his
						boddie to do more, then another can do with thte same
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						part. Especially the hands, &amp; [the] feete: 
						&amp; abooue all, the voice.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Dailie labour, &amp; sweatt enowgh, will do much.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Animus, Corpus, status in expedito: omnia succincta: experta Singula.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
