	<page filename="Ha2.095r.tif" signature="[A7r]" pagination="13" reader="Harvey"/>	<!-- ABO 99right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Maxim&egrave; sapit, qui prouidet maxim&egrave;, vt &agrave; Seipso dependeat:
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						quantu[m] facere
						potest vllo modo.
						Summa summaru[m] Epicteti, Seneca[e], Antonini.
						Quomod&ograve; Crates; nihilo egere philosophum.
						Stoici; Omnia esse Sapientum. 
					</marginalia_text>
				</position>
			</language>
			<language ident="EL">
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						oti
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						monos ho sofosploustos
						oti autarces he arete pros eudaimonian.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Omnes Magistros superat profundus discipulus.
						Puer est, qui nondum didicit viuere.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sera nimis sophia est crastina; disce hodi&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EL">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						oti hoi sophoi eleutheroi, kai hoi moroi douloi.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Crastinia sera nimis copia: cresce hodi&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Eutuchus,
						summus hodiernista. Euemarus, ephermus.
					</marginalia_text>
					<person name="Eutuchus"/>
				</position>
			</language>
		</marginalia>
