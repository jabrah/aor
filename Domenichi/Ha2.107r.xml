	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.107r.tif" signature="C3[r]" pagination="37" reader="Harvey"/>	<!-- ABO 111right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Fundamentu[m] viri Ethicum, Epictetus.
					</marginalia_text>
					<person name="Epictetus"/>
				</position>
			</language>
			<translation>
				The ethical foundation of man: Epictetus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						{ Sententiaru[m] acutarum formula[e], Mimi Publij:
						et apophthegmatum acerrima.
					</marginalia_text>
					<person name="Publilius Syrus"/>
				</position>
			</language>
			<translation>
				The formulae for penetrating ideas: the mimes and most pointed sayings of Publius Syrus. 
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Spiritus vini: et mera Quinta Essentia, }
					</marginalia_text>
				</position>
			</language>
			<translation>
				The spirit of wine and the unmixed fifth essence.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Omnes isti subtiles, in horas crassi.
					</marginalia_text>
				</position>
			</language>
			<translation>
				All those subtle matters become coarse in time.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Humanu[m] genus, quotidi&egrave; mutabile: sine
						spiriti Epicteti.
					</marginalia_text>
					<person name="Epictetus"/>
				</position>
			</language>
			<translation>
				The human species is inconstant daily, without the spirit of Epictetus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Habet ensem Machiauelli; et clypeum
						Taciti: sed caret pugione Epicteti.
					</marginalia_text>
					<person name="Machiavelli"/>
					<person name="Tacitus"/>
					<person name="Epictetus"/>
				</position>
			</language>
			<translation>
				He has the sword of Machiavelli, and the shield of Tacitus, but he lacks the dagger of Epictetus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Plus vita[e]: plus, plusq[ue] Spiritus. Qui omnium adhuc Oratorum defectus, 
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						ipsorumq[ue] poetaru[m]. 
					</marginalia_text>
				</position>
			</language>
			<language ident="EN">
				<position place="intext" book_orientation="90">
					<marginalia_text>
						Of all other,
						the most liuelie, &amp; nimble.
					</marginalia_text>
				</position>
			</language>
			<translation>
				More life, more and more Spirit. He who, until now, has lacked all orators and the poets themselves.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Axiophili Panacea. Angelica est Medicina, animus diabuglossatus,
						perfususq[ue] anima borraginis, atq[ue] melissa[e]. Spiritus ipse rosas, violas,
						aromata spirans, Ille, ille ambrosius Cordis, Cerebriq[ue] syrupus.
					</marginalia_text>
					<person name="Axiophilus"/>
				</position>
			</language>
			<translation>
				The Panacea of Axiophilus. Angelic is the Medicine, the mind is infused with Buglossa, 
				and imbued with essence of Borage and Melissa. The spirit itself breathes in the sweet odors
				of roses and violets; that, that is the ambrosial syrup of the heart and brain.
			</translation>
		</marginalia>
		
