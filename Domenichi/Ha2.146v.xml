	<page filename="Ha2.146v.tif" signature="[Hv]" pagination="114" reader="Harvey"/>	<!-- ABO 150left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Quicquid est in tuo vnius Globo, aut in tua ipsius
						Spha[e]rra: id omne decet
						singulari vigore exertare. Nec vnq[uam] desinit.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Eutrapelus seipsum cogere: quantus quantus potest extendi.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						His original arts: &amp; final professions: 
						the onlie Singular bookes, which he principally regardes.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Sed quasi ludens, ut est pragmaticoru[m] dexterrimus.
						Nec possu[nt] illum parua irritare obuia, qui magna sperni opposita.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Illas tantummod&ograve; literas curat Eutrapelus: qua[e] viru[m]
						faciunt alijs pra[e]claris viris pra[e]stantiorem.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Soli studet doctrina[e], qua[e] est in operibus efficaciss[im]a
						in mundo potentissima.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
    </annotation>
