	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Obserua semper peritissima scientiaru[m]: subtiliss[im]a
						Experimentorum: potentissima actionum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quorsum tot libri, et tant studia, si non plus
						possunt doctores, qu&agrave;m
						illiterati.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						sals&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Medice, cura teipsum. Ante omnia, Nosce teipsu[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Maximus mundi Theurgus, et Panurgus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Qui se habet, nil perdidit: Ausus est dicere Seneca.
					</marginalia_text>
					<person name="Seneca"/>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Etiam potentissima, potenti uiro impotentiora.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Totum in se habet, qui se habet: Ausus est dicere
						Cassiodorus. Quod petis, int&ugrave;s habes.
					</marginalia_text>
					<person name=/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						et ipsis A[e]gyptijs operabatur periti&ugrave;s: et omnibus Magis
						potentiora Miracula perpetrabat.
							<!-- Continued from facing page, right margin, Ha2.093v -->
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Hominum scripta, errantes planeta[e]: Artiu[m], et Virtutum impressiones, fixa[e] stella[e].
					</marginalia_text>
					<emphasis method="pen" type="straight" text="Hominum scripta"/>
					<emphasis method="pen" type="straight" text="Artiu[m]"/>
					<emphasis method="pen" type="straight" text="Virtutum"/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Florescat ars: virescat virtus: splendescat vir. Nunq[uam] languidus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

