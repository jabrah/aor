				<position place="head" book_orientation="0">
					<marginalia_text>
						Apollonius Tyaneus, omnium Linguarum, Artiumq[ue]
						promptissimus disipulus, et magister.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Qui vnum, aut duos
						pra[e]gnantissimos discursus legit, aut etiam
						prudentiss[im]as  historias;
						omnes Legit. Valeant discursus, pra[e]terq[uam] 
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						pauci,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						omnium
						potentissimi ad negociandum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						) Tantum
						habes literarum, quantum nosti pragmateiarum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Artium; philosophia[e];
						et summarum facultatum vnicus professor in
						Mundo. Nam in his etiam omnes discursus, et histo[riae].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
				
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Nem&oacute; adhuc hominum doctorum, satis artifex. Nec vllum in mundo tantum
						opera[e] precium, quantum Artium, philosophia[e], et propria[e] professionisi: quanten&ugrave;s
						in Mundi negocijs operantur efficacissim&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Peritissima[e] methodo; praxi; furori, omnia pra[e]sentissima.
						Idem heri, hodi&egrave;, cras. Vnum Hoc.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

