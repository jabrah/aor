	<page filename="Ha2.110r.tif" signature="[C6r]" pagination="43" reader="Harvey"/>	<!-- ABO 114right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Patience, &amp; longanimitie, the vertu of vertues.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						In Ethicis, quanta inania. In Physicis, nihil
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						vacuum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				In Ethics, how many empty spaces there are. In Physics, there is no void.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Semper dulcis animus: et ingenium semper Lautum; sed acre.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Vanoru[m] plena suna omnia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				All things are full of emptiness.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Asperius nihil est humili, cum surgit in altum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Non curat Hippoclides. Contemnit
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Axiophilus, omnia ad oppositum.
					</marginalia_text>
					<person name="Axiophilus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Placent apophthegmata, gnoma[e], epigrammata: sed si vnus
						Liber digendus, malim facetias: nisi cu[m] opus stratgamatis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Vulcanus, igneus Deus: A[e]olus, &auml;ereus: Neptunus, aqueus: Pluto, terreus. Deus deoru[m] vnus.
						Phocyon, Demosthenis securis: vt Diogenes, Platonis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="90">
					<marginalia_text>
						Diogenes, in sermone salsissimus: in
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						vita patientissimus. Magis magnanimus, qu&agrave;m Alexander: magis laboriosus,
						qu&agrave;m Hercules: gemina illius professio. Vtri error, qu&ograve;d in tantillis subiectis, tantus artifex.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
