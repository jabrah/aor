	<page filename="Ha2.112v.tif" signature="[C8v]" pagination="48" reader="Harvey"/>	<!-- ABO 117left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						The towre takes plotters, that woold take the towre.
						No man, that is no man at trialls howre.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Semper uiuu[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						| Pro se quisq[ue]: et contra alios quisq[ue]:
						nec Hercules contra duos, aut tres a[e]quales. Quid
						vnus contra Mundum, plenum auaritia[e], et superbia[e]?
						At vnus potest, quod mille non possunt: et
						Labor omnia vincit: et Domat omnia Virtus: et
						Prudentia sibi serua reddit, omnia: 
						et Ca[e]sar regit omnia terris.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Vir non est, qui non est Semper vir.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Nondum vir est, qui non est semper alacer, industrius, absolutus.
						S. Bonifacius: S. Victor: S. Perpetuus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Nec heri, nec hodi&egrave;, nec cras: sed sempter alacer, industrius, absolutus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
