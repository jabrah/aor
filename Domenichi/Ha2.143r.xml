		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						( Hos ego versiculos feci: tulit alter honores. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Maximus ille armis: maximus ille toga. Vt Drusus Ouidio.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Magnificum nobilissimi animi Emblema: Donec totu[m] compleat Orbe[m].
					</marginalia_text>
				</position>
			</language>
