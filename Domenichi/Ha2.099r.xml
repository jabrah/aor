	<page filename="Ha2.099r.tif" signature="B3r" pagination="21" reader="Harvey"/>	<!-- ABO 103right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Unicismo dell'Vnico.
					</marginalia_text>
					<person name="Bernardo Accolti"/>
				</position>
			</language>
			<translation>
				Something unique from Bernardo Accolti.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Acrior inflammet nocte, dieq[ue] furor.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Night and day, let the fury be kindled more vigorusly.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Pra[e]clarissima Mundi Securitas, Nihil omnium reru[m]
						curare, pra[e]ter vnu[m] ( ) in manu Solem.
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Questo vno, vnissimo.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The most illustrious safety of the world, 
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Pra[e]ter Hoc vnum,
					</marginalia_text>
				</position>
			</language>
			<language ident="ES">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Todos es nada.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Beyond this one thing here, everything is nothing.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						He that doth not allwais affect to be best; shall often waxe base; &amp; sumtime
						sceme one of the worst. Eutrapelus hateth nothing more, then
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						to sceme a Vulgar: or to be cou[n]teed an ordinarie ma[n].
						Therefore euer intermixeth su[m]thing extraordinarie.
						Allwais a fine, &amp; rare
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						man: &amp; the most excelling personage in [the] companie.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Ca[e]tera abijcienda pro tempore: vt nullius iam nun momenti.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
