	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Ad rem pra[e]sentem semper pra[e]sens.
					</marginalia_text>
				</position>
			</language>
			<translation>Always at hand for the matter at hand.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Prudenter, et suauiter.
					</marginalia_text>
				</position>
			</language>
			<translation>Prudently and pleasantly.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Columna composita.
					</marginalia_text>
				</position>
			</language>
			<translation>A composite pillar.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( &ograve; Cesare, &ograve; Catone. )
					</marginalia_text>
					<person name="Julius Caesar"/>
					<person name="Cato the Younger"/>
				</position>
			</language>
			<translation>Oh Caesar, oh Cato.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Vel Vlysses, vel Diomedes, uel poti&ugrave;s vterq[ue].
					</marginalia_text>
					<person name="Odysseus"/>
					<person name="Diomedes"/>
				</position>
			</language>
			<translation>Either Odysseus or Diomedes, or rather both.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Aut Julius Imp: aut Trebatius JC: aut Tullius orator:
						aut poti&ugrave;s isti omnes.
					</marginalia_text>
					<person name="Julius Caesar"/> 
					<person name="Trebatius Testa"/>
					<person name="Cicero"/>
				</position>
			</language>
			<translation>Either emperor Julius, or judge Trebatius: or Cicero the orator, or rather all of them.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Summa omnium in dicendo, agendoq[ue] facilitas.
					</marginalia_text>
				</position>
			</language>
			<translation>The highest facility of all in speaking and acting.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Hoc age: ca[e]tera patere. Ad hoc, Entelechia: ad ca[e]tera, Ironia.
						Vt Eutrapelus dulcissim&egrave; solet alia omnia pro tempore contemnere, pra[e]ter
					</marginalia_text>
					<person name="Eutrapelus"/>
					<emphasis method="pen" type="straight" text="hoc"/>
					<emphasis method="pen" type="straight" text="Entelechia"/>
					<emphasis method="pen" type="straight" text="ad ca[e]tera"/>
					<emphasis method="pen" type="straight" text="Ironia"/>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						Id vnu[m], quod est in Manibus.
					</marginalia_text>
				</position>
			</language>
			<translation>Do this: suffer the rest. For this, actualization: for the other, irony. 
				As Eutrapelus is used to contemn sweetly for a time everying else, except the one thing which is in his hands.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Eutrapelus, amabilis ingenuitatis, et omnis plan&egrave; humanitatis aureum speculum:
						erga Omnes a[e]quissimus; siue a[e]mulos, siue inuidos, siue maleuolos, siue aduerarios.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						respectiu&egrave; candidus in omne genus humanum.
					</marginalia_text>
				</position>
			</language>
			<translation>Eutrapelus, lovable for his ingenuity and a golden mirror of all humanity in its entirety: 
				most fair towards all; whether rivals, or envious, malevolent or hostile.
				He is respectfully bright in all of the human kind.</translation>
		</marginalia>
		
