	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Pra[e]sently run-ouer the worthiest bookes you haue,
						or can gett: and see what important vse you can
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						make of the worthiest points
						in them: pra[e]cepts, or examples.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Qui solido iudico
						procedit, et ver&egrave; Critico; statim animaduertit
						qua[e] sunt maximu[m] factura
						opera[e]precium. Ca[e]tere leuiter percurrenda.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Prudentis est, Sibi ipsi
						nunq[uam] repugnare in suo cursu.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sibi constat proficiens.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
