	<page filename="Ha2.103v.tif" signature="[B7v]" pagination="30" reader="Harvey"/>	<!-- ABO 108left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Adytum Ethices, et Physices.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The inner sancutary of Ethics and Physics.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						In paruo magnum: vt Epictetus, et Vrso.
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Urso of Salerno"/>
				</position>
			</language>
			<translation>
				The greatness in small things, as with Epictetus and Urso.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Libelli parui; sed Optimi Maximi: vt regula[e] iuris; et
						Bellorum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Small little books, but the best greatest works, as with the rules of law and war.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						scit&egrave;.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Knowingly.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Autorum abunde: 
						Scientia[e] parum.
					</marginalia_text>
				</position>
			</language>
			<translation>
				Authors in abundance, knowledge very little.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Ante omnia, Epictetus, et Vrso ad unguem.
						Epitome sapientia[e] Salomonica[e].
					</marginalia_text>
					<person name="Epictetus"/>
					<person name="Urso of Salerno"/>
					<person name="Solomon"/>
				</position>
			</language>
			<translation>
				Before all things, Epictetus and Urso by heart.
				The epitome of Solomonic wisdom.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Nusqu[am] tanta Ethicorum, Physicorumq[ue] varietas tam breu&icirc;;
						qu&agrave;m in nouissimo adyto Ethices, et Physices. Fundamentu[m],
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						vt ipse vocat, Salomonicum.
					</marginalia_text>
					<person name="Solomon"/>
				</position>
			</language>
			<translation>
				So much variety of Ethics and Physics is never as concise,
				as in the most recent inner sanctuary of Ethics and Physics. The foundation,
				as it is called, of Solomon.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Princeps omnes amat, et neminem.
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						Vita[e]berga, mea Academia.
					</marginalia_text>
					<location name="Wittenberg"/>
				</position>
			</language>
			<translation>
				The prince loves all and no one.
				Wittenberg, my academy.
			</translation>
		</marginalia>
		
