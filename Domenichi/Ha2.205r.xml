	<page filename="Ha2.205r.tif" signature="[P5r]" pagination="233" reader="Harvey"/>	<!-- ABO 209right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="FR">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Desir n'a repos.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Oculus Lyncis, in manu
						Archimedis.
					</marginalia_text>
					<person name="Archimedes"/>
				</position>
			</language>
			<translation>Desire doesn't have rest. The eye of the gem, in the hand of Archimedes.</translation> <!--JG: Not entirely sure abt 'lynchis'-->
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vnicus Mundi Panergus.
					</marginalia_text>
				</position>
			</language>
			<translation>The unique panergus of the world.</translation> <!--JG: Have to agree on a standardized way to translate panergus!-->
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						mirabile forza dell'Occasione.	
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Quod semidiscitur, aut semiexercetur, nihil ualet.
					</marginalia_text>
				</position>
			</language>
			<translation>The wonderful force of the occassion. That which is learned or exercised half-heartedly, is worth nothing.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Iamiamq[ue] Singula necessaria: statim omnia vtilia.
					</marginalia_text>
				</position>
			</language>
			<translation>At every moment, a singular necessity: at once entirely useful.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Vita, Vigilia. Vita, Militia.
					</marginalia_text>
				</position>
			</language>
			<translation>Life, vigilant. Life, military service.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Solus Entelechus, scit eloqui; exequi;
						et viuere.
					</marginalia_text>
				</position>
			</language>
			<translation>The only actualization knows to utter, strive, and to live.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Plus anima[e], viuidiq[ue] spiritus.
					</marginalia_text>
				</position>
			</language>
			<translation>A spirit, more vital and lively.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Viua dictorum, factorumq[ue] hyperbole.
					</marginalia_text>
				</position>
			</language>
			<translation>A fresh hyperbole of maxims and actions.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Non prodest alius, sed aliud. Da mihi non plures artifices, sed plus artis:
						nec operum numero, sed pondere crescit scientia, facultas, fama.
					</marginalia_text>
				</position>
			</language>
			<translation>One thing is not useful, but another. Do not give my more artists, but more of art: </translation> <!--JG: HIER GEBLEVEN MET VERTALEN!!!-->
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Is ver&egrave; Sapiens, qui Vtilia, non qui Multa scit. &Egrave; gra[e]co A[e]schyli. 
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Chi troppo abraccia, poco stringe.
					</marginalia_text>
				</position>
			</language>
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Dimidiu[m] perfectu[m], plus toto imperfecto.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
