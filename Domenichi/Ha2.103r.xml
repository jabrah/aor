	<page filename="Ha2.104r.tif" signature="[B7r]" pagination="29" reader="Harvey"/>	<!-- ABO 107right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Articuli continua agitatione exercendi: sensusq[ue] similiter.
						Eudoxi medici perpetua admonitio.
					</marginalia_text>
					<person name="Eudoxus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Articulorum, Sensuumq[ue] perennis agitatio.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Singularis exercendi aphorismus: apud Laertiu[m],
						in vita Eudoxi.
					</marginalia_text>
					<person name="Diogenes Laertius"/>
					<person name="Eudoxus"/>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Eudromi entelechia ante omnes.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Animus est omnia. Mens super omnia.
					</marginalia_text>
				</position>
			</language>
			<translation>
				The rational soul is all things. The mind is above all things.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nihil omnia, nisi animus animosus.
					</marginalia_text>
				</position>
			</language>
			<translation>
				All things are nothing, unless the soul is animated.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Omnis cura, hodierna: hesterna, nimis sera:
						crastina, nimis cita. Sufficit cuiq[ue] dici sua cura.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Axiophili expeditum viaticu[m]. Seria vis hodierna: industria crastina sera.
						Viua, potensq[ue] animi possessio, copia vita[e]. diuinum Arcanu,
						Soum vita[e] ephodion, Viuus, potensq[ue] animus.
					</marginalia_text>
					<person name="Axiophilus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="270">
					<marginalia_text>
						= pra[e]sentia,	<!-- CG: I'm unsure where this word goes. -->
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Apostoli, nihil habentes viatici, nullo carebant viatico.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
