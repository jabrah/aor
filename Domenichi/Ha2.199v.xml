	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Omnia familiaria, et gratiosa: nihil vulgare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Consuetudo, altera naturua: ut etiam dia[e]ta.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Quomodocunq[ue] alij loquu[n]tur, aut agunt; tu&acirc; refert, semper et loqui, et agere,
						qu&agrave;m possis Optim&egrave;: pra[e]sertim apud Optimos. Nihil nimis
						pulchr&egrave;: aut
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						satis bene.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Valet ordinatum ingenium, acriterq[ue] vnitum: vacillat confusum.
						Hoc fac: et Viues. Hoc age: et excelles. Pra[e]dominatur quarto modo
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						propria.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<underline method="pen" type="straight" language="IT" text="la Contessa valorosa"/>
		<underline method="pen" type="straight" language="IT" text="preso vn nobilissimo espediente"/>
		<underline method="pen" type="straight" language="IT" text="promesse"/>
		<underline method="pen" type="straight" language="IT" text="se la lasciauano entrar l&agrave;"/>
		<underline method="pen" type="straight" language="IT" text="per sede"/>
		<underline method="pen" type="straight" language="IT" text="promessai figliuoli per istatichi"/>
		<underline method="pen" type="straight" language="IT" text="la donna entrata nella fortezza"/>
		<underline method="pen" type="straight" language="IT" text="rimprouerando con acerbissime"/>
		<underline method="pen" type="straight" language="IT" text="a' co[n]giurati la morte del"/>
		<underline method="pen" type="straight" language="IT" text="presi i suoi"/>
		<underline method="pen" type="straight" language="IT" text="gifli &amp; vn cotello"/>
		<underline method="pen" type="straight" language="IT" text="volergli vccidere in"/>
		<underline method="pen" type="straight" language="IT" text="la Contessa nimosa"/>
		<underline method="pen" type="straight" language="IT" text="non mutando faccia, alzatisi tostamente"/>
		<underline method="pen" type="straight" language="IT" text="egli stolti"/>
		<underline method="chalk" type="straight" language="IT" text="habbia la forma da farne de gli altri"/>
		<underline method="pen" type="straight" language="IT" text="la forma da farne de gli altri"/>
		<underline method="pen" type="straight" language="IT" text="loro errore"/>
		<underline method="pen" type="straight" language="IT" text="lasciati quei figliuoli"/>
		<underline method="pen" type="straight" language="IT" text="fueor della citt&agrave; si fuggirono"/>
		<underline method="pen" type="straight" language="IT" text="L'adulatione esser estremamente"/>
		<underline method="pen" type="straight" language="IT" text="magnanimi, abhorrita"/>
		<underline method="pen" type="straight" language="IT" text="i fatti Alessandro Magno"/>
		<underline method="pen" type="straight" language="IT" text="tant' adulatione"/>
		<underline method="pen" type="straight" language="IT" text="strappatogli il libro"/>
		<underline method="pen" type="straight" language="IT" text="ne fiume Hidaspe"/>
		<underline method="pen" type="straight" language="IT" text="ancor piu degno"/>
		<underline method="pen" type="straight" language="IT" text="iui precipitato che"/>
