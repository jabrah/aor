	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Omnia familiaria, et gratiosa: nihil vulgare.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Consuetudo, altera naturua: ut etiam dia[e]ta.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Quomodocunq[ue] alij loquu[n]tur, aut agunt; tu&acirc; refert, semper et loqui, et agere,
						qu&agrave;m possis Optim&egrave;: pra[e]sertim apud Optimos. Nihil nimis
						pulchr&egrave;: aut
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						satis bene.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Valet ordinatum ingenium, acriterq[ue] vnitum: vacillat confusum.
						Hoc fac: et Viues. Hoc age: et excelles. Pra[e]dominatur quarto modo
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						propria.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

