	<page filename="Ha2.156v.tif" signature="[I4v]" pagination="136" reader="Harvey"/>	<!-- ABO 161left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Entelechia[e] semper opus purificato animo, et spiritu illuminato.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Sola fer&egrave; apophthegmata, epigr[m]mata, sales, et stratagemata dicunt,
						aguntq[ue] adamantin&egrave;: cu[m] ipsis Legibus, et Armis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nisi qu&ograve;d ipsa[e] Artes, et Virtutes,
						omnium dicendoru[m], agendoru[m]q[ue] magistra[e]
						peritissima[e], subtilissima[e].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Eudromi singulare Mnemosynon: Et exquisitiss[im]us
						singulorum; et omnium expeditissimus.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Eutrapeli anima, candidissima omnium anima: Eudromi ingenium, nitidissimum Mundi
						ingenium. Illa etiam omnium alacerrima, Sine vlla Nubercula: hoc Mundi viuidissimu[m],
						sine vlla habescentia aut tepore. Quarum neutrum potest consistere sine aurea
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="90">
					<marginalia_text>
						dia[e]ta: adamantina exercitatione:
						et angelica semper 
						illuminatione
						omniu[m] animi, corporisq[ue]
						potentiaru[m].
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="90">
					<marginalia_text>
						Paru[m], sed pueru[m]: diuinu[m] axioma:
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						et Apolloniji dicta, factaq[ue] omnia adamantina. Rarissima scripsit et Semper breuissim&egrave;. 
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
	</annotation>
