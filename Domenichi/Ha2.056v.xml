		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Sum do all bie halues: as Castor liues
					</marginalia_text>
					<person name="Castor"/>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						halfe a Life: &amp; Pollux
						dyed half
						a death.
					</marginalia_text>
					<person name="Pollux"/>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Tota vita,
						Eutrapeli vita.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
			<translation>
				An entire life is the life of Eutrapelus.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
