	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Ex Eutrapeli arcanis, singulare secretum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Eutrapelus
						nihil pra[e]tendit apud quenq[uam]: intendit summa apud
						| omnes.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Nulla tanta Entelechia:
						Nulla talis Euschemosyne. Sed Ironia,
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						vtramq[ue] celans auget
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						| vtramq[ue]. Nec vlla Mundi
						disciplina tam potens. Pra[e]tendere gloriosa,
						hominis est impotentis: Ipsa extendere maxima,
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Serij est viri.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						dextr&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Nugatur, qui gloriatur. Rem gerit, qui rem tenet.
						Nec vlla tam conspicua demonstratio, qu&agrave;m ipse
						Euentus. Qui e&ograve; magnificentior, qu&ograve; modestior.
					</marginalia_text>
					<emphasis method="pen" type="straight" text="Rem gerit, qui rem tenet"/>
					<emphasis method="pen" type="straight" text="qu&agrave;m ipse"/>
					<emphasis method="pen" type="straight" text="Euentus"/>
					<emphasis method="pen" type="straight" text="e&ograve; magnificentior, qu&ograve; modestior"/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Plus candore valet Eutrapelus, qu&agrave;m alij superbia: et magnificentior est =
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						= huius dignitas, qu&agrave;m illorum pompa.
						Vt nihil vulgare: sic nihil odiosum. Pulchra omnia: nulla vana.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
