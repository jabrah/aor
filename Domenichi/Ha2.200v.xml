	<page filename="Ha2.200v.tif" signature="[O8v]" pagination="224" reader="Harvey"/>	<!-- ABO 205left -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Prudentia, ars viuendi fortitudo praxis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						( Qui sero sapit, frustra sapit.)
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Nemo satis Matutinus. )
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Naturam expellas furea licet, vsq[uam] recurret.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						( Necessaria, no[n] necessarijs: certa, incertis:
						erga, parergis.)
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Energus
						contemnit parergum: ne se quidem excepto.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Cuiq[ue] diei, suum onus.
						Omnia dexterrim&egrave;: Singula efficacissim&egrave;.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Qui non quotidi&egrave; sua Maxima meditatur, quotidi&egrave; Sui obliuiscitur. Etiam
						veteranus, cum desinit exercere, Tyro est.
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Vnico Fortio, Il filosofo del Sole; et volante Caualiere. Piu Oltre.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Rudis est, cum possis loqui melius, tamen loqui peius: Semplicis est, cu[m] ualeas
						agere maius, tamen agere minus. Proficiendi nullus sit modus, vbi
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						ullus locus.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
