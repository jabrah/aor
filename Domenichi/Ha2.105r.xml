<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
	
	<page filename="Ha2.105r.tif" signature="Cr" pagination="33" reader="Harvey"/>	<!-- ABO 109right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Vnum necessarium: reliqua speciosa.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Illud materiale: ha[e]c pro forma.
					</marginalia_text>
				</position>
			</language>
			<translation>
				That is material, this is according to form.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Trismegistus in Suo Epicyclo.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						In vno, voluptas, et foelicitas: in pluribus,
						tristitia, et miseria.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Da magistrum, dicebat Monophilus: id est, hac
						hebdomade Althusium Solum:
						proxima hebdomade Venatorium Solum. Vtrunq[ue]
						semper ad unguem: vt nihil in mundo promptius.
					</marginalia_text>
					<person name="Monophilus"/>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Monophili inuiolabilis Cursus.
					</marginalia_text>
					<person name="Monophilus"/>
				</position>
			</language>
			<translation>
				The inviolable path of Monophilus.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Cui Domenicus, vnicus iocandi artifex.
					</marginalia_text>
					<person name="Lodovico Domenichi"/>
				</position>
			</language>
			<translation>
				For whom Domenichi, the sole master of jest.
			</translation>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Vnus plan&egrave;, pleneq[ue] expositus, instar omnium: omnes obscure aut leuiter
						propositi, instar nullius.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Axiophili adamantinus delectus. Gemmifera[e] concha[e] pauca[e]: multi ecc&egrave; Canales.
						Pondere, non numero res optima maxima constat. Hunc, atq[ue] hunc volo
					</marginalia_text>
					<person name="Axiophilus"/>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						Phoenicem, cum magna geruntur.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="270">
					<marginalia_text>
						Doctor Vnionis.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						In vno, omnes.
					</marginalia_text>
				</position>
			</language>
		</marginalia>

    </annotation>
</transcription>