        <marginalia hand="Italian" anchor_text="de natali die Domini conueniebat, quemadmodum neque conuenit hodie">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Non convenit hodie de Natali Die Chr[ist]i</marginalia_text>
                    <person name="Jesus Christ"/>
                </position>
            </language>
            <translation>It is not agreed today about the birth date of Christ.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ex iis quae nota[n]tur &agrave; Clemente, libro primo Stromatum, rem istam vari&egrave; iactatam fuisse constat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cleme[n]tis in hac re valde su[n]t confusa Testimonia.</marginalia_text>
                    <person name="Clement of Alexandria"/>
                </position>
            </language>
            <translation>The evidence of Clement is very much confused regarding this matter.</translation>
        </marginalia>
        
