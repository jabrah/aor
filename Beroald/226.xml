    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Anni Iuliani veru[m] Initium ergo erit ad media[m] nocte[m] vltima[e] diei Dece[m]bris.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>But the beginning of the Julian year will therefore be in the middle of the night of the final day of December.</translation> <!-- FSF: I wouldn't translate verum with but here; rather truly, in truth, indeed or something affirmative -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="annum sacru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Annus sacer.</marginalia_text>
                </position>
            </language>
            <translation>The sacred year.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cyclos enim suos Paschales incoeperunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cycli Paschales.</marginalia_text>
                </position>
            </language>
            <translation>The Paschal cycles.</translation>
        </marginalia>
        
