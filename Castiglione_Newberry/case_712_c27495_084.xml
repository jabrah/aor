    <page filename="case_712_c27495_084.tif" reader="Harvey"  signature="[Giii[v]]"/>
    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="enriching this tunge,"/>
        <underline method="pen" type="straight" language="EN" text="it maiestye and light"/>
        <underline method="pen" type="straight" language="EN" text="it poore"/>
        <underline method="pen" type="straight" language="EN" text="to shut it vp into so narrowe a rowme"/>
        <underline method="pen" type="straight" language="EN" text="folow onely"/>
        <underline method="pen" type="straight" language="EN" text="Laurence"/>
        <underline method="pen" type="straight" language="EN" text="great miserye"/>
        <underline method="pen" type="straight" language="EN" text="to stoppe"/>
        <underline method="pen" type="straight" language="EN" text="without wading any farther"/>
        <underline method="pen" type="straight" language="EN" text="many and so noble wittes shall neuer find out"/> 
        <underline method="pen" type="straight" language="EN" text="then one good maner of speach in [the] tung that"/>
        <underline method="pen" type="straight" language="EN" text="proper &amp; naturall"/>
        <underline method="pen" type="straight" language="EN" text="scrupulous"/>
        <underline method="pen" type="straight" language="EN" text="religion &amp; high"/>
        <underline method="pen" type="straight" language="EN" text="such an awe"/>
        <underline method="pen" type="straight" language="EN" text="open their mouth"/>
        <underline method="pen" type="straight" language="EN" text="of their nurses"/>
        <underline method="pen" type="straight" language="EN" text="their cradel"/>
        <underline method="pen" type="straight" language="EN" text="the opinions and wittes of men are"/>
        <underline method="pen" type="straight" language="EN" text="vehement and quicke of nature"/>
        <underline method="pen" type="straight" language="EN" text="soft and quiet"/>
        <underline method="pen" type="straight" language="EN" text="seuere and graue"/>
        <underline method="pen" type="straight" language="EN" text="should apply him"/>
        <underline method="pen" type="straight" language="EN" text="own proper inclinatio[n]"/>
        <underline method="pen" type="straight" language="EN" text="the nature of their scholers"/>
        <underline method="pen" type="straight" language="EN" text="the yll husbandmanne"/>
        <underline method="pen" type="straight" language="EN" text="sometime in a solyle"/>
        <underline method="pen" type="straight" language="EN" text="for vynes"/>
        <underline method="pen" type="straight" language="EN" text="will sowe graine"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="And truly it should be a great miserye to stoppe, without wading any any farther then almost the first that euer wrote: and to dispaire, that
            so many and so noble wittes shall neuer find out any mo then one good maner of speach in [the] tung that vnto them is proper &amp; naturall"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="such an awe"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="vehement and quicke of nature"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="own proper inclinatio[n]"/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
