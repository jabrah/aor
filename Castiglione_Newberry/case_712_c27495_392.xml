        <underline method="pen" type="straight" language="EN" text="To swimme well"/>
        <underline method="pen" type="straight" language="EN" text="renn well"/>
        <underline method="pen" type="straight" language="EN" text="To vaute well"/>
        <underline method="pen" type="straight" language="EN" text="To wrastle well"/>
        <underline method="pen" type="straight" language="EN" text="cast te barr well"/>
        <underline method="pen" type="straight" language="EN" text="Sildome in open syght"/>
        <underline method="pen" type="straight" language="EN" text="priuilye with himselfe alone, or"/>
        <underline method="pen" type="straight" language="EN" text="friendes and familiers"/>
        <underline method="pen" type="straight" language="EN" text="renn well at tilt, and at ring"/>
        <underline method="pen" type="straight" language="EN" text="To routney"/>
        <underline method="pen" type="straight" language="EN" text="To fight at Barriers"/>
        <underline method="pen" type="straight" language="EN" text="kepe a pasage or"/>
        <underline method="pen" type="straight" language="EN" text="renn at Bull"/>
        <underline method="pen" type="straight" language="EN" text="To fling a Speare or Dart"/>
        <underline method="pen" type="straight" language="EN" text="open syght to delyte"/>
        <underline method="pen" type="straight" language="EN" text="the commune"/>
        <underline method="pen" type="straight" language="EN" text="men of the Countrey"/>
        <underline method="pen" type="straight" language="EN" text="be sure to gete the victorie"/>
        <underline method="pen" type="straight" language="EN" text="himself in feates of chiualrie in open"/>
        <underline method="pen" type="straight" language="EN" text="well prouided of horse and harneis"/>
        <underline method="pen" type="straight" language="EN" text="well trapped"/>
        <underline method="pen" type="straight" language="EN" text="armed"/>
        <underline method="pen" type="straight" language="EN" text="thathe may showe himselfe nymeble on horsbacke"/>
        <underline method="pen" type="straight" language="EN" text="be of the last"/>
        <underline method="pen" type="straight" language="EN" text="appere in the liftes at iustes"/>
        <underline method="pen" type="straight" language="EN" text="in triumphes"/>
        <underline method="pen" type="straight" language="EN" text="comelie armour"/>
        <underline method="pen" type="straight" language="EN" text="bases"/>
        <underline method="pen" type="straight" language="EN" text="scarfes"/>
        <underline method="pen" type="straight" language="EN" text="trappinges"/>
        <underline method="pen" type="straight" language="EN" text="liueries"/>
        <underline method="pen" type="straight" language="EN" text="such other thinges"/>
        <underline method="pen" type="straight" language="EN" text="sightlie and meerie coulours"/>
        <underline method="pen" type="straight" language="EN" text="rich to beehoulde"/>
        <underline method="pen" type="straight" language="EN" text="wittie poesies and pleasant diuises"/>
        <underline method="pen" type="straight" language="EN" text="to allure vnto him chefflie the eyes of the people"/>
        <underline method="pen" type="straight" language="EN" text="disguise"/>
        <underline method="pen" type="straight" language="EN" text="himself in maskerie"/>
        <underline method="pen" type="straight" language="EN" text="on horsebacke"/>
        <underline method="pen" type="straight" language="EN" text="a foote"/>
        <underline method="pen" type="straight" language="EN" text="vpon hym that shall be contrarie to the feate that he mindeth to worke"/>
        <underline method="pen" type="straight" language="EN" text="bould feates"/>
        <underline method="pen" type="straight" language="EN" text="out of companye"/>
        <underline method="pen" type="straight" language="EN" text="in the sight of the most noble personages in the campe"/>
        <underline method="pen" type="straight" language="EN" text="beefore his Princis eyes"/>
        
