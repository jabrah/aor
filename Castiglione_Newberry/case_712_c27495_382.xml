        <underline method="pen" type="straight" language="EN" text="wandringe in vanitye"/>
        <underline method="pen" type="straight" language="EN" text="the"/>
        <underline method="pen" type="straight" language="EN" text="sound ioye"/>
        <underline method="pen" type="straight" language="EN" text="to smell those spirituall"/>
        <underline method="pen" type="straight" language="EN" text="relieue the vertues of the"/>
        <underline method="pen" type="straight" language="EN" text="the heauenlye harmonie so tunable"/>
        <underline method="pen" type="straight" language="EN" text="of passion"/>
        <underline method="pen" type="straight" language="EN" text="dronken with the bottomlesse fountain of contentation"/>
        <underline method="pen" type="straight" language="EN" text="giueth a smacke of the right blisse vnto"/>
        <underline method="pen" type="straight" language="EN" text="the renning and cleere water"/>
        <underline method="pen" type="straight" language="EN" text="shininge beames of thy light our eyes"/>
        <underline method="pen" type="straight" language="EN" text="from mysty ignoraunce"/>
        <underline method="pen" type="straight" language="EN" text="be not"/>
        <underline method="pen" type="straight" language="EN" text="be in effect"/>
        <underline method="pen" type="straight" language="EN" text="oure soules"/>
        <underline method="pen" type="straight" language="EN" text="for a sacrifice"/>
        <underline method="pen" type="straight" language="EN" text="Barn them in the liuelye"/>
        <underline method="pen" type="straight" language="EN" text="wasteth al grosse filthines"/>
        <underline method="pen" type="straight" language="EN" text="euerlasting &amp; most sweet bonde to the heauenly beawty"/>
        <underline method="pen" type="straight" language="EN" text="chaunged"/>
        <underline method="pen" type="straight" language="EN" text="right louers into the beloued"/>
        <underline method="pen" type="straight" language="EN" text="admitted to the feast of the aungelles"/>
        <underline method="pen" type="straight" language="EN" text="where fed with immortall ambrosia and nectar"/>
        <underline method="pen" type="straight" language="EN" text="liuelye death"/>
        <underline method="pen" type="straight" language="EN" text="past died the fathers of olde"/>
        <underline method="pen" type="straight" language="EN" text="diddest hale from the"/>
        <underline method="pen" type="straight" language="EN" text="bodye and"/>
        <underline method="pen" type="straight" language="EN" text="coopleddest them with God"/>
        <underline method="pen" type="straight" language="EN" text="vehemencye, that"/>
        <underline method="pen" type="straight" language="EN" text="man woulde haue thought him"/>
        <underline method="pen" type="straight" language="EN" text="rauished and beeside himselfe"/>
        <underline method="pen" type="straight" language="EN" text="without once moouing"/>
        <underline method="pen" type="straight" language="EN" text="houldynge his eyes towarde heauen as astonied"/>
        <underline method="pen" type="straight" language="EN" text="most diligent eare to"/>
        <underline method="pen" type="straight" language="EN" text="the plaite of hys garment and pluckinge"/>
        <underline method="pen" type="straight" language="EN" text="your soule also to forsake"/>
        <underline method="pen" type="straight" language="EN" text="the bodye"/>

