    <annotation>
        
        <underline method="pen" type="straight" language="EN"  text="judgement and art doeth polishe and correct them"/> 
        <underline method="pen" type="straight" language="EN" text="belongeth to witt"/>
        <underline method="pen" type="straight" language="EN" text="in art:"/>
        <underline method="pen" type="straight" language="EN" text="jestes and meery conceites"/>
        <underline method="pen" type="straight" language="EN" text="moue laughter"/>
        <underline method="pen" type="straight" language="EN" text="are meete for the Courtier"/>
        <underline method="pen" type="straight" language="EN" text="whyche are"/>
        <underline method="pen" type="straight" language="EN" text="maner they ought"/>
        <underline method="pen" type="straight" language="EN" text="smilynge"/>
        <underline method="pen" type="straight" language="EN" text="will not geue place vnto"/>
        <underline method="pen" type="straight" language="EN" text="in Iestinge"/>
        <underline method="pen" type="straight" language="EN" text="folies:"/>
        <underline method="pen" type="straight" language="EN" text="menne laugh manie"/>
        <underline method="pen" type="straight" language="EN" text="wittie saiynges"/>
        <underline method="pen" type="straight" language="EN" text="Bernarde Bibiena"/>
        <underline method="pen" type="straight" language="EN" text="be the maisters of this facultie"/>
        <underline method="pen" type="straight" language="EN" text="whom in case"/>
        <underline method="pen" type="straight" language="EN" text="must first learne"/>
        <underline method="pen" type="straight" language="EN" text="practise that"/>
        <underline method="pen" type="straight" language="EN" text="are not skilfull"/>
        <underline method="pen" type="straight" language="EN" text="Lordes laughe in mocking M. Bernarde"/>
        <underline method="pen" type="straight" language="EN" text="much more perfectly in you"/>
        <underline method="pen" type="straight" language="EN" text="to deferr the"/>
        <underline method="pen" type="straight" language="EN" text="talke till tomorrowe"/>
        <underline method="pen" type="straight" language="EN" text="consumed vpon your praise"/>
        <underline method="pen" type="straight" language="EN" text="verie well knowen all"/>
        <underline method="pen" type="straight" language="EN" text="diuided not the paines takinge equallye"/>
        <underline method="pen" type="straight" language="EN" text="charge of speakynge"/>
        <underline method="pen" type="straight" language="EN" text="M. Bernarde Bibiena"/>
        <underline method="pen" type="straight" language="EN" text="quicke wytted in talkynge"/>
        <underline method="pen" type="straight" language="EN" text="wythoute intermission"/>
        
