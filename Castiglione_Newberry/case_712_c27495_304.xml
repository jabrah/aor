    <annotation>

        <underline method="pen" type="straight" language="EN" text="plucke vppe your strength"/>
        <underline method="pen" type="straight" language="EN" text="backewarde to giue a greater pushe at the encounter"/>
        <underline method="chalk" type="straight" language="EN" text="plucke vppe your strength, like them that reture backewarde to giue a greater pushe at 
            the encounter. And to say the truth, it is ill done of you"/>
        <underline method="pen" type="straight" language="EN" text="ennemies begine to breake and to square one"/>
        <underline method="chalk" type="straight" language="EN" text="and to square one wyth"/>
        <underline method="pen" type="straight" language="EN" text="your aduersarie"/>
        <underline method="chalk" type="straight" language="EN" text="reuile them"/>
        <underline method="pen" type="straight" language="EN" text="reuile them more then he"/>
        <underline method="pen" type="straight" language="EN" text="to praise them perhappes somwhat"/>
        <underline method="pen" type="straight" language="EN" text="length of the talke"/>
        <underline method="pen" type="straight" language="EN" text="manye other pretye matters"/>
        <underline method="pen" type="straight" language="EN" text="yet beehinde to be said of the Courtier"/>
        <underline method="pen" type="straight" language="EN" text="what they"/>
        <underline method="pen" type="straight" language="EN" text="you, ne any man elles can ad ought therto"/>
        <underline method="pen" type="straight" language="EN" text="malice you beare to the honour of women"/>
        <underline method="pen" type="straight" language="EN" text="alreadie spoken, of"/>
        <underline method="pen" type="straight" language="EN" text="muche more in him"/>
        <underline method="chalk" type="straight" language="EN" text="somwhat more frindlye to women, then"/>
        <underline method="pen" type="straight" language="EN" text="youre witt"/>
        <underline method="chalk" type="straight" language="EN" text="a greater perfection"/>
        <underline method="pen" type="straight" language="EN" text="not ad vnto him more then hath alreadie bine"/>
        <underline method="pen" type="straight" language="EN" text="to diminish the praises and worthinesse"/>
        
