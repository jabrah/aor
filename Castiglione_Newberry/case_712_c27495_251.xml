    <annotation>  
        
        <underline method="pen" type="straight" language="EN" text="attemptinge certein other wayes for"/>
        <underline method="pen" type="straight" language="EN" text="tender pitie so to quicken"/>
        <underline method="pen" type="straight" language="EN" text="called Pail, [that] he had respect to no kind of daunger"/>
        <underline method="pen" type="straight" language="EN" text="eyther to die or to deliuer his father"/>
        <underline method="pen" type="straight" language="EN" text="brought to passe"/>
        <underline method="pen" type="straight" language="EN" text="such priuie conueiaunce"/>
        <underline method="pen" type="straight" language="EN" text="first in Ligurno beefore"/>
        <underline method="pen" type="straight" language="EN" text="writ to his wief"/>
        <underline method="pen" type="straight" language="EN" text="his settinge at libertie"/>
        <underline method="pen" type="straight" language="EN" text="and sodeine ioye"/>
        <underline method="pen" type="straight" language="EN" text="throughe the zeale as prowesse"/>
        <underline method="pen" type="straight" language="EN" text="lifted her eyes"/>
        <underline method="pen" type="straight" language="EN" text="starke dead to the grounde"/>
        <underline method="pen" type="straight" language="EN" text="to temper the willies of men"/>
        <underline method="pen" type="straight" language="EN" text="died for sorowe or"/>
        <underline method="pen" type="straight" language="EN" text="her lief was nothinge"/>
        <underline method="pen" type="straight" language="EN" text="tary the lingering"/>
        <underline method="pen" type="straight" language="EN" text="thens with couetinge, fled by and"/>
        <underline method="pen" type="straight" language="EN" text="was ouerlouing"/>
        <underline method="pen" type="straight" language="EN" text="extremitie, which"/>
        
