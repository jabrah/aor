    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="loue more happelye then"/>
        <underline method="pen" type="straight" language="EN" text="was my drift"/>
        <underline method="pen" type="straight" language="EN" text="what waye they must"/>
        <underline method="pen" type="straight" language="EN" text="guided by reason"/>
        <underline method="pen" type="straight" language="EN" text="vnpossible for loue to stand with reason"/>
        <underline method="pen" type="straight" language="EN" text="Dutchesse desired"/>
        <underline method="pen" type="straight" language="EN" text="nourish it with that onelye, which is commune"/>
        <underline method="pen" type="straight" language="EN" text="to the other"/>
        <underline method="pen" type="straight" language="EN" text="parte, whiche is propre to her"/>
        <underline method="pen" type="straight" language="EN" text="vpon this noble matter"/>
        <underline method="pen" type="straight" language="EN" text="vnworthy to talke of"/>
        <underline method="pen" type="straight" language="EN" text="holye misteries of loue"/>
        <underline method="pen" type="straight" language="EN" text="him to"/>
        <underline method="pen" type="straight" language="EN" text="this excelent"/>
        <underline method="pen" type="straight" language="EN" text="how to loue contrarye"/>
        <underline method="pen" type="straight" language="EN" text="commune ignorant sort"/>
        <underline method="pen" type="straight" language="EN" text="haue dedicated all my wholl lief vnto"/>
        <underline method="pen" type="straight" language="EN" text="my wordes may be answerable"/>
        <underline method="pen" type="straight" language="EN" text="prayse of him"/>
        <underline method="pen" type="straight" language="EN" text="he is yong"/>
        <underline method="pen" type="straight" language="EN" text="to lour sensuallye"/>
        <underline method="pen" type="straight" language="EN" text="be good &amp; circumspect"/>
        <underline method="pen" type="straight" language="EN" text="beeguyle not him self"/>
        <underline method="pen" type="straight" language="EN" text="men deserueth more to be pitied"/>
        <underline method="pen" type="straight" language="EN" text="them blamed"/>
        <underline method="pen" type="straight" language="EN" text="be blamed then pitied"/>
        
