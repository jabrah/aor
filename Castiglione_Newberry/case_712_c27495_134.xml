    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="dissemble [the] malice"/>
        <underline method="pen" type="straight" language="EN" text="men laugh them to scorne"/>
        <underline method="pen" type="straight" language="EN" text="fauour some one"/>
        <underline method="pen" type="straight" language="EN" text="to spite them"/>
        <underline method="pen" type="straight" language="EN" text="enter in fauour"/>
        <underline method="pen" type="straight" language="EN" text="so dronken"/>
        <underline method="pen" type="straight" language="EN" text="that thei wist not what wer become of their feete and handes"/>
        <underline method="pen" type="straight" language="EN" text="to cal company to behoulde them"/>
        <underline method="pen" type="straight" language="EN" text="him esteame fauour and"/>
        <underline method="pen" type="straight" language="EN" text="could not liue without"/>
        <underline method="pen" type="straight" language="EN" text="whan he hath"/>
        <underline method="pen" type="straight" language="EN" text="let him not shew himself new or straunge"/>
        <underline method="pen" type="straight" language="EN" text="wonder at it whan it is offred him"/>
        <underline method="pen" type="straight" language="EN" text="refuse it in such sort as some"/>
        <underline method="pen" type="straight" language="EN" text="for very ignorance receiue it not, and"/>
        <underline method="pen" type="straight" language="EN" text="make men beleue that thei acknowledge themselues vnworthy of"/>
        <underline method="pen" type="straight" language="EN" text="humble himself somewhat vnder his"/>
        <underline method="pen" type="straight" language="EN" text="fauour &amp; promocions so easilye"/>
        <underline method="pen" type="straight" language="EN" text="be offred"/>
        <underline method="pen" type="straight" language="EN" text="refuse them modestlye"/>
        <underline method="pen" type="straight" language="EN" text="he much estemeth them"/>
        <underline method="pen" type="straight" language="EN" text="to offer them"/>
        <underline method="pen" type="straight" language="EN" text="great deale more instance"/>
        <underline method="pen" type="straight" language="EN" text="more resistance"/>
        <underline method="pen" type="straight" language="EN" text="to be estemed"/>
        <underline method="pen" type="straight" language="EN" text="benefite whiche he bestoweth is"/>
        <underline method="pen" type="straight" language="EN" text="muche"/>
        <underline method="pen" type="straight" language="EN" text="seemeth to make of it"/>
        <underline method="pen" type="straight" language="EN" text="much honoured"/>
        <underline method="pen" type="straight" language="EN" text="perfect promotions that"/>
        <underline method="pen" type="straight" language="EN" text="esteamed of such"/>
        <underline method="pen" type="straight" language="EN" text="not craued"/>
        <underline method="pen" type="straight" language="EN" text="true vertue"/>
        <underline method="pen" type="straight" language="EN" text="accompanied with modestie"/>
        <underline method="pen" type="straight" language="EN" text="lowest"/>
        <underline method="pen" type="straight" language="EN" text="friende come higher"/>
        
