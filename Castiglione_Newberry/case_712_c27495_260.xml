        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="be not vnkinde"/>
        <underline method="pen" type="straight" language="EN" text="fewe kynges or great Princis, that by him"/>
        <underline method="pen" type="straight" language="EN" text="bine surmounted"/>
        <underline method="pen" type="straight" language="EN" text="Naples likewyse"/>
        <underline method="pen" type="straight" language="EN" text="Hungarye, as excellent a Ladye as"/>
        <underline method="pen" type="straight" language="EN" text="kinge Mathew Couruin her husbande"/>
        <underline method="pen" type="straight" language="EN" text="Dutchesse Isabell or Aragon"/>
        <underline method="pen" type="straight" language="EN" text="which as golde in the fire, so"/>
        <underline method="pen" type="straight" language="EN" text="the stormes of fortune hath she showed her vertue"/>
        <underline method="pen" type="straight" language="EN" text="receyue great wronge in"/>
        <underline method="pen" type="straight" language="EN" text="temperatelye, as whoso"/>
        <underline method="pen" type="straight" language="EN" text="neuer again wonder at a woma[n]s wit"/>
        <underline method="pen" type="straight" language="EN" text="Dutches of Ferrara"/>
        <underline method="pen" type="straight" language="EN" text="her moste excellent vertues gaue"/>
        <underline method="pen" type="straight" language="EN" text="a woorthie daughter"/>
        <underline method="pen" type="straight" language="EN" text="a Queene ouer"/>
        <underline method="pen" type="straight" language="EN" text="a farr greater State"/>
        <underline method="pen" type="straight" language="EN" text="the worlde"/>
        <underline method="pen" type="straight" language="EN" text="the bitter strokes of fortune"/>
        <underline method="pen" type="straight" language="EN" text="pacientlye"/>
        <underline method="pen" type="straight" language="EN" text="Queene Isabell of Naples hath done"/>
        <underline method="pen" type="straight" language="EN" text="the losse of her kingdome,"/>
        <underline method="pen" type="straight" language="EN" text="Fridericke her husbande"/>
        <underline method="pen" type="straight" language="EN" text="showeth her selfe a Queene"/>
        <underline method="pen" type="straight" language="EN" text="myserable inconueniences of wretched"/>
        <underline method="pen" type="straight" language="EN" text="chaunged fortune, yet"/>
        
        <underline method="pen" type="straight" language="EN" text="Isavel Marq."/>
        
