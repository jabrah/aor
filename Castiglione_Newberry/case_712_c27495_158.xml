    <annotation>
        
        <marginalia hand="Italian" anchor_text="lette him come alwaies prouided and thinke on it beefore hande, showyng notwithstanding, the whole to bee done ex tempore, and at the first sight">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        semper pra[e]meditatus Augustus; sed quasi ex tempore.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Augustus always considered in advance, but as if suddenly.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="allure men to heare or to looke on that he supposeth himselfe"/>
        <underline method="pen" type="straight" language="EN" text="making ssemblant"/>
        <underline method="pen" type="straight" language="EN" text="to doe it"/>
        <underline method="pen" type="straight" language="EN" text="not for a bragge and to shewe it for vainglory"/>
        <underline method="pen" type="straight" language="EN" text="at a chaunce, &amp; rahter praied by others"/>
        <underline method="pen" type="straight" language="EN" text="commyng of his owne free will"/>
        <underline method="pen" type="straight" language="EN" text="lette him come alwaies prouided "/>
        <underline method="pen" type="straight" language="EN" text="and thinke on it beefore hande"/>
        <underline method="pen" type="straight" language="EN" text="showyng notwithstanding"/>
        <underline method="pen" type="straight" language="EN" text="the whole to bee done"/>
        <underline method="pen" type="straight" language="LA" text="ex tempore"/>
        <underline method="pen" type="straight" language="EN" text="and at the first sight"/>
        <underline method="pen" type="straight" language="EN" text="meane"/>
        <underline method="pen" type="straight" language="EN" text="touche them"/>
        <underline method="pen" type="straight" language="EN" text="without grounding muche"/>
        <underline method="pen" type="straight" language="EN" text="great deale more cunning therein, then he vttereth"/>
        <underline method="pen" type="straight" language="EN" text="as certein Poetes sometime that harped vpon verye subtill pointes of Philsosophie"/>
        <underline method="pen" type="straight" language="EN" text="other sciences, and parauenture"/>
        <underline method="pen" type="straight" language="EN" text="had small vnderstanding"/>
        <underline method="pen" type="straight" language="EN" text="altogether ignoraunt in"/>
        <underline method="pen" type="straight" language="EN" text="haue im make any profession at all"/>
        <underline method="pen" type="straight" language="EN" text="him anye fame by it"/>
        <underline method="pen" type="straight" language="EN" text="whan occasion serueth"/>
        <underline method="pen" type="straight" language="EN" text="confesse to haue no"/>
        <underline method="pen" type="straight" language="EN" text="Nicholetto neuer haue"/>
        <underline method="pen" type="straight" language="EN" text="verye excellent Philosopher"/>
        <underline method="pen" type="straight" language="EN" text="more skilfull in the lawe then in feeling"/>
        <underline method="pen" type="straight" language="EN" text="of those Lectures in the lawe"/>
        <underline method="pen" type="straight" language="EN" text="neuer yelde at the"/>
        <underline method="pen" type="straight" language="EN" text="to deceyue the"/>
        <underline method="pen" type="straight" language="EN" text="that he had no vnderstanding in it"/>
        <underline method="pen" type="straight" language="EN" text="Socrates opinion"/>
        <underline method="pen" type="straight" language="EN" text="is not a Physosophers"/>
        <underline method="pen" type="straight" language="EN" text="that he hath no vnderstanding"/>
        <underline method="pen" type="straight" language="EN" text="say he hath no vnderstandyng, without it bee required of him: for"/>
        <underline method="pen" type="straight" language="EN" text="allowe not this fondnesse to accuse &amp; debase himselfe. Againe"/>
        <underline method="pen" type="straight" language="EN" text="willingly disclose some matters"/>
        <underline method="pen" type="straight" language="EN" text="without any faulte"/>
        <underline method="pen" type="straight" language="EN" text="shadowe of sclaunder"/>
        
        <underline method="pen" type="straight" language="EN" text="Somtyme a mannes ignoraunce is to be confessed."/>
        <underline method="pen" type="straight" language="EN" text="their shame"/>
        
