    <annotation>  
        
        <marginalia hand="Italian" anchor_text="Queene Isabel of Spaine">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Queene Isabell:
                    </marginalia_text>
                    <person name="Isabella I of Castile"/>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="liuelode"/>
        <underline method="pen" type="straight" language="EN" text="their lief"/>
        <underline method="pen" type="straight" language="EN" text="for their law, and to"/>
        <underline method="pen" type="straight" language="EN" text="with counsell and"/>
        <underline method="pen" type="straight" language="EN" text="vertue &amp; prowesse,"/>
        <underline method="pen" type="straight" language="EN" text="fewe Princis haue had"/>
        <underline method="pen" type="straight" language="EN" text="steppes, but to enuie her"/>
        <underline method="pen" type="straight" language="EN" text="suche a diuine maner of gouernment"/>
        <underline method="pen" type="straight" language="EN" text="that her will onlye"/>
        <underline method="pen" type="straight" language="EN" text="to do [that] he"/>
        <underline method="pen" type="straight" language="EN" text="scase durst a man in his owne"/>
        <underline method="pen" type="straight" language="EN" text="suspected he woulde displease"/>
        <underline method="pen" type="straight" language="EN" text="had in knowinge and cousing"/>
        <underline method="pen" type="straight" language="EN" text="ministers meete for the offices"/>
        <underline method="pen" type="straight" language="EN" text="ioigne the rigour of iustice with the mildenesse of mercye"/>
        <underline method="pen" type="straight" language="EN" text="anye yll, to sore punished"/>
        <underline method="pen" type="straight" language="EN" text="sprange a erie great reuerence diriued"/>
        <underline method="pen" type="straight" language="EN" text="of loue and feare"/>
        <underline method="pen" type="straight" language="EN" text="so settled"/>
        <underline method="pen" type="straight" language="EN" text="looked she [that] should beehoulde"/>
        <underline method="pen" type="straight" language="EN" text="from heauen"/>
        <underline method="pen" type="straight" language="EN" text="Realmes are still ruled, in"/>
        <underline method="pen" type="straight" language="EN" text="lke a whiele that longe symnged about with violence, keepeth [the] same course a good while after of it"/>
        <underline method="pen" type="straight" language="EN" text="no man moue it anye more"/>
        <underline method="pen" type="straight" language="EN" text="all the great men of Spaine"/>
        <underline method="pen" type="straight" language="EN" text="bine made by"/>
        <underline method="pen" type="straight" language="EN" text="Queene Isabel"/>
        <underline method="pen" type="straight" language="EN" text="great Capitain Gonsalue"/>
        <underline method="pen" type="straight" language="EN" text="more setbye for"/>
        <underline method="pen" type="straight" language="EN" text="that in peace &amp; warr"/>
        <underline method="pen" type="straight" language="EN" text="so notable and famous, that"/>
        <underline method="pen" type="straight" language="EN" text="case fame be"/>
        <underline method="pen" type="straight" language="EN" text="Gonsaluo"/>
        
