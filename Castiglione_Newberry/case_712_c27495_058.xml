        <underline method="pen" type="straight" language="EN" text="in goyng"/>
        <underline method="pen" type="straight" language="EN" text="tender and feint, that"/>
        <underline method="pen" type="straight" language="EN" text="dawningly, that"/>
        <underline method="pen" type="straight" language="EN" text="the higher in degree"/>
        <underline method="pen" type="straight" language="EN" text="vse suche"/>
