        <underline method="pen" type="straight" language="EN" text="your countenaunce is most acceptable"/>
        <underline method="pen" type="straight" language="EN" text="pleasant to beholde"/>
        <underline method="pen" type="straight" language="EN" text="manly and"/>
        <underline method="pen" type="straight" language="EN" text="hath a good grace withall"/>
