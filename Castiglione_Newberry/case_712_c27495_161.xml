        <underline method="pen" type="straight" language="EN" text="sweare that there is not a person in the worle whom"/>
        <underline method="pen" type="straight" language="EN" text="wyllynge to ieoparde their lyfe for"/>
        <underline method="pen" type="straight" language="EN" text="matters out of reason"/>
        <underline method="pen" type="straight" language="EN" text="makewise to weepe"/>
        <underline method="pen" type="straight" language="EN" text="speake a woorde for sorowe"/>
        <underline method="pen" type="straight" language="EN" text="counted to louynge woormes"/>
