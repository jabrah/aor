    <page filename="case_712_c27495_365.tif" reader="Harvey"  signature="Tt.iiii.[r]"/>
    <annotation>
        
        
        <underline method="chalk" type="straight" language="EN" text="M. Morello: But I reme[m]bre rather that I haue"/>
        <underline method="chalk" type="straight" language="EN" text="manie beautifull women of a most"/>
        <underline method="pen" type="straight" language="EN" text="beautifull women of a most yll"/>
        <underline method="chalk" type="straight" language="EN" text="spitefull"/>
        <underline method="chalk" type="straight" language="EN" text="(in a maner( it happeneth alwaies so, for beawtie maketh them proude: and pride, cruell"/>
        <underline method="pen" type="straight" language="EN" text="old men ought to couet beawtye"/>
        <underline method="pen" type="straight" language="EN" text="Ladies handes, and what"/>
        <underline method="pen" type="straight" language="EN" text="somwhat out of pacience"/>
        <underline method="pen" type="straight" language="EN" text="not knowe the thinge"/>
        <underline method="pen" type="straight" language="EN" text="that toucheth me not"/>
        <underline method="pen" type="straight" language="EN" text="the yonge men"/>
        <underline method="pen" type="straight" language="EN" text="are not so fresh and lusty as"/>
        <underline method="pen" type="straight" language="EN" text="is not altogether"/>
        <underline method="pen" type="straight" language="EN" text="out of the way"/>
        <underline method="pen" type="straight" language="EN" text="beautye of women"/>
        <underline method="chalk" type="straight" language="EN" text="in the worlde, hatred, warr, mortality, &amp; destruction, wherof the rasinge of Troye can be a good witnesse:"/>
        <underline method="pen" type="straight" language="EN" text="proude"/>
        <underline method="pen" type="straight" language="EN" text="cruell"/>
        <underline method="pen" type="straight" language="EN" text="vnchast"/>
        <underline method="pen" type="straight" language="EN" text="Morello woulde finde no faulte"/>
        <underline method="chalk" type="straight" language="EN" text="may be the redier to deceiue, and that this amiable looke were like a baite that couereth the hooke"/>
        <underline method="pen" type="straight" language="EN" text="that couereth the hooke"/>
        <underline method="pen" type="straight" language="EN" text="beautie is alwayes good"/>
        
