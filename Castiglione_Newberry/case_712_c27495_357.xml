        <underline method="pen" type="straight" language="EN" text="forsake his seruice"/>
        <underline method="pen" type="straight" language="EN" text="hartgreefe that"/>
        <underline method="pen" type="straight" language="EN" text="woorthy a personage"/>
        <underline method="pen" type="straight" language="EN" text="Aristotel and Plato be his mates"/>
        <underline method="chalk" type="straight" language="EN" text="Yet wott I not whether I may beleaue that Aristotel and Plato euer daunsed or were musitiens in all their lief time"/>
        <underline method="pen" type="straight" language="EN" text="euer daunsed"/>
        <underline method="pen" type="straight" language="EN" text="were musitiens in all"/>
        <underline method="pen" type="straight" language="EN" text="feates of chiualrye"/>
        <underline method="pen" type="straight" language="EN" text="diuine wittes"/>
        <underline method="pen" type="straight" language="EN" text="skilfull in euerye thinge"/>
        <underline method="chalk" type="straight" language="EN" text="therfore it is to be presupposed that"/>
        <underline method="pen" type="straight" language="EN" text="euer beelongeth to"/>
        <underline method="chalk" type="straight" language="EN" text="Courtlynesse"/>
        <underline method="pen" type="straight" language="EN" text="so penn the matter"/>
        <underline method="pen" type="straight" language="EN" text="very craftesmaisters"/>
        <underline method="chalk" type="straight" language="EN" text="know"/>
        <underline method="pen" type="straight" language="EN" text="vnderstoode the whol euen to the pith an innermost"/>
        <underline method="chalk" type="straight" language="EN" text="euen to the pith an innermost rootes"/>
        <underline method="pen" type="straight" language="EN" text="ende,"/>
        <underline method="pen" type="straight" language="EN" text="the good qualities which"/>
        <underline method="pen" type="straight" language="EN" text="so graue a Philosophie"/>
        <underline method="pen" type="straight" language="EN" text="against goodnesse,"/>
        <underline method="pen" type="straight" language="EN" text="in all age, and"/>
        <underline method="pen" type="straight" language="EN" text="the Courtier"/>
        <underline method="pen" type="straight" language="EN" text="to vertue"/>
        <underline method="pen" type="straight" language="EN" text="be aged, for"/>
        <underline method="pen" type="straight" language="EN" text="knowleage commeth"/>
        <underline method="pen" type="straight" language="EN" text="wyth experyence"/>
        <underline method="pen" type="straight" language="EN" text="a louer,"/>
        <underline method="pen" type="straight" language="EN" text="a louer"/>

