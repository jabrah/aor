        <underline method="chalk" type="straight" language="EN" text="litle or no woorthinesse in respect of men"/>
        <underline method="pen" type="straight" language="EN" text="L. Octauians"/>
        <underline method="chalk" type="straight" language="EN" text="Men of wisdome sett no store by them"/>
        <underline method="pen" type="straight" language="EN" text="men of woorthinesse at anye time"/>
        <underline method="pen" type="straight" language="EN" text="doe not loue and obserue women, the vertue"/>
        <underline method="pen" type="straight" language="EN" text="consequentlye the woorthinesse"/>
        <underline method="pen" type="straight" language="EN" text="whom I deeme"/>
        <underline method="chalk" type="straight" language="EN" text="Yet if we should come"/>
        <underline method="chalk" type="straight" language="EN" text="women were lyke to quaile greatlie"/>
        <underline method="chalk" type="straight" language="EN" text="haue shaped a Courtier that"/>
        <underline method="pen" type="straight" language="EN" text="shaped a Courtier that"/>
        <underline method="pen" type="straight" language="EN" text="manie diuine qualities"/>
        <underline method="pen" type="straight" language="EN" text="consider him to be such a one as he"/>
        <underline method="pen" type="straight" language="EN" text="desrtes of women can"/>
        <underline method="pen" type="straight" language="EN" text="witty"/>
        <underline method="pen" type="straight" language="EN" text="eloquent a person"/>
        <underline method="chalk" type="straight" language="EN" text="as is Count Lewis &amp; Sir Fridericke, to shape a gentilwoman"/>
        <underline method="pen" type="straight" language="EN" text="a gentilwoman of the Palaice"/>
        <underline method="pen" type="straight" language="EN" text="perfections due to a woman"/>
        <underline method="pen" type="straight" language="EN" text="Courtier with the perfections"/>
        <underline method="pen" type="straight" language="EN" text="thinge wittie"/>
        <underline method="pen" type="straight" language="EN" text="eloquent"/>
        <underline method="pen" type="straight" language="EN" text="women are as full of vertues as men be"/>
        <underline method="chalk" type="straight" language="EN" text="vertue is the female, and vice the male"/>
        <underline method="pen" type="straight" language="EN" text="your iudgement"/>
        <underline method="pen" type="straight" language="EN" text="I am sorte for"/>
        <underline method="pen" type="straight" language="EN" text="he is so seduced"/>
        <underline method="pen" type="straight" language="EN" text="promises and flatteringe"/>
        <underline method="pen" type="straight" language="EN" text="into an errour to"/>
        <underline method="pen" type="straight" language="EN" text="sake I am ashamed of"/>
        <underline method="chalk" type="straight" language="EN" text="ashamed for your owne sake, whan you"/>
        <underline method="pen" type="straight" language="EN" text="confesse"/>
        <underline method="pen" type="straight" language="EN" text="pardon"/>
        <underline method="pen" type="straight" language="EN" text="Men of worthines"/>
        
