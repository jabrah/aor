        <underline method="pen" type="straight" language="EN" text="doubtfull woordes and feigned disdeignes"/>
        <underline method="pen" type="straight" language="EN" text="an vncerteintie and nippe"/>
        <underline method="pen" type="straight" language="EN" text="extreme passion which he fealeth"/>
        <underline method="pen" type="straight" language="EN" text="tokens of loue &amp;"/>
        <underline method="pen" type="straight" language="EN" text="beegine to beethinke themselues"/>
        <underline method="pen" type="straight" language="EN" text="beegine a freshe"/>
        <underline method="pen" type="straight" language="EN" text="beegane his seruice but"/>

