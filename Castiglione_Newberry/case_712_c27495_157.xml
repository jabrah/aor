    <annotation>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        A fine &amp; iudicious Courtier. 
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="nor riotous and vnordinate in any il condicion: not filthy and vnclenly in his liuing, with"/>
        <underline method="pen" type="straight" language="EN" text="boysterous beehauiours that smell of the plough and cart a thousand mile of, for he"/>
        <underline method="pen" type="straight" language="EN" text="make a good Courtier"/>
        <underline method="pen" type="straight" language="EN" text="no better vse then to kepe sheepe"/>
        <underline method="pen" type="straight" language="EN" text="perfect vnderstandinge in that"/>
        <underline method="pen" type="straight" language="EN" text="euery possible thinge may"/>
        <underline method="pen" type="straight" language="EN" text="be easye"/>
        <underline method="pen" type="straight" language="EN" text="and all men wonder at him, and"/>
        <underline method="pen" type="straight" language="EN" text="he at no manne"/>
        <underline method="pen" type="straight" language="EN" text="certaine loftye and vnmanerlye stubburnnesse"/>
        <underline method="pen" type="straight" language="EN" text="showe themselues"/>
        <underline method="pen" type="straight" language="EN" text="not to wonder at the thinges"/>
        <underline method="pen" type="straight" language="EN" text="can do them much better"/>
        <underline method="pen" type="straight" language="EN" text="their silence"/>
        <underline method="pen" type="straight" language="EN" text="vnworthy to be spoken of"/>
        <underline method="pen" type="straight" language="EN" text="make a gesture"/>
        <underline method="pen" type="straight" language="EN" text="vndersanding of [the] profoundnes of their couning"/>
        <underline method="pen" type="straight" language="EN" text="these fateful maners"/>
        <underline method="pen" type="straight" language="EN" text="&amp; iwht gentlenesse &amp; courtesie praise other mens good dedes"/>
        <underline method="pen" type="straight" language="EN" text="thoughe he perceyue himselfe execllent and farr boue others"/>
        <underline method="pen" type="straight" language="EN" text="showe that he esteameth not hymselfe"/>
        <underline method="pen" type="straight" language="EN" text="a one"/>
        <underline method="pen" type="straight" language="EN" text="full perfections are"/>
        <underline method="pen" type="straight" language="EN" text="to lay aside his hope to come to a good passe"/>
        <underline method="pen" type="straight" language="EN" text="perfect &amp; high excellency which"/>
        <underline method="pen" type="straight" language="EN" text="aspireth vnto"/>
        <underline method="pen" type="straight" language="EN" text="all praisewoorthye"/>
        <underline method="pen" type="straight" language="EN" text="by [the] highest"/>
        <underline method="pen" type="straight" language="EN" text="sildome sene that he"/>
        <underline method="pen" type="straight" language="EN" text="[the] meane"/>
        <underline method="pen" type="straight" language="EN" text="himself excellent in anie thinge"/>
        <underline method="pen" type="straight" language="EN" text="beeside armes"/>
        <underline method="pen" type="straight" language="EN" text="gete him estymatyon by it after an honest sorte"/>
        <underline method="pen" type="straight" language="EN" text="so dyscreete and of so good"/>
        <underline method="pen" type="straight" language="EN" text="a iudgemente"/>
        <underline method="pen" type="straight" language="EN" text="the vnderstandinge after a comelye maner"/>
        <underline method="pen" type="straight" language="EN" text="commende not thynges well"/>
        <underline method="pen" type="straight" language="EN" text="Many places"/>
        <underline method="pen" type="straight" language="EN" text="beeside the best"/>
        <underline method="pen" type="straight" language="EN" text="his counin[g]."/>
        
