    <annotation>  
        
        <underline method="pen" type="straight" language="EN" text="farthynge left him to blesse himselfe"/>
        <underline method="pen" type="straight" language="EN" text="chafe, to curse, and to"/>
        <underline method="pen" type="straight" language="EN" text="tearinge of God"/>
        <underline method="pen" type="straight" language="EN" text="blew out the candels"/>
        <underline method="pen" type="straight" language="EN" text="raked vp the fire and"/>
        <underline method="pen" type="straight" language="EN" text="speake aloude"/>
        <underline method="pen" type="straight" language="EN" text="greatest hurly buryle in the worlde, makinge wise to"/>
        <underline method="pen" type="straight" language="EN" text="carde vnderneath"/>
        <underline method="pen" type="straight" language="EN" text="viede vpon flush"/>
        <underline method="pen" type="straight" language="EN" text="vs mount"/>
        <underline method="pen" type="straight" language="EN" text="open his eyes"/>
        <underline method="pen" type="straight" language="EN" text="maner light in the"/>
        <underline method="pen" type="straight" language="EN" text="he awoke"/>
        <underline method="pen" type="straight" language="EN" text="for certeintie"/>
        <underline method="pen" type="straight" language="EN" text="neyther fire nor anye kinde of lighte and"/>
        <underline method="pen" type="straight" language="EN" text="cardes without light"/>
        <underline method="pen" type="straight" language="EN" text="lost thy sight as wel as thy money"/>
        <underline method="pen" type="straight" language="EN" text="Seest thou"/>
        <underline method="pen" type="straight" language="EN" text="laughinge and"/>
        <underline method="pen" type="straight" language="EN" text="about to mock them"/>
        <underline method="pen" type="straight" language="EN" text="I see you"/>
        <underline method="pen" type="straight" language="EN" text="seeme to wonder much"/>
        <underline method="pen" type="straight" language="EN" text="By good Lord, I"/>
        <underline method="pen" type="straight" language="EN" text="reach me the candell, and lett vs see"/>
        
