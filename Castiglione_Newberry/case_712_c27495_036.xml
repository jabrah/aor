            <translation>The remarkable idea of the most beautiful and pleasant court.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="greatnesse of his courage"/>
        <underline method="pen" type="straight" language="EN" text="quickened hym"/>
        <underline method="pen" type="straight" language="EN" text="hys personne to practise"/>
        <underline method="pen" type="straight" language="EN" text="delyte to behoulde them in other"/>
        <underline method="pen" type="straight" language="EN" text="his wordes"/>
        <underline method="pen" type="straight" language="EN" text="euerye man accordynge to hys desertes"/>
        <underline method="pen" type="straight" language="EN" text="greate a iudgement he"/>
        <underline method="pen" type="straight" language="EN" text="at Tylt, at Tourneye"/>
        <underline method="pen" type="straight" language="EN" text="in playinge at all sortes of weapon"/>
        <underline method="pen" type="straight" language="EN" text="in all exercises"/>
        <underline method="pen" type="straight" language="EN" text="myght deserue to"/>
        <underline method="pen" type="straight" language="EN" text="all the houres of the daye"/>
        <underline method="pen" type="straight" language="EN" text="pleasaunt exercyses"/>
        <underline method="pen" type="straight" language="EN" text="of the bodye"/>
        <underline method="pen" type="straight" language="EN" text="mynde"/>
        <underline method="pen" type="straight" language="EN" text="Dutchesse was"/>
        <underline method="pen" type="straight" language="EN" text="Elizabeth Gonzaga"/>
        <underline method="pen" type="straight" language="EN" text="so liuelye a wytt and iudgement"/>
        <underline method="pen" type="straight" language="EN" text="maistresse and ringe leader"/>
        <underline method="pen" type="straight" language="EN" text="pleasaunte communication and merye conceytes"/>
        <underline method="pen" type="straight" language="EN" text="euery mannes countenaunce a manne myght perceyue"/>
        <underline method="pen" type="straight" language="EN" text="a louynge iocoundenesse"/>
        <underline method="pen" type="straight" language="EN" text="thys house"/>
        <underline method="pen" type="straight" language="EN" text="the verye mansion"/>
        <underline method="pen" type="straight" language="EN" text="place of Myrth and Joye"/>
        <underline method="pen" type="straight" language="EN" text="neuer so tasted in other"/>
        <underline method="pen" type="straight" language="EN" text="sweete conuersation"/>
        <underline method="pen" type="straight" language="EN" text="amyable an louynge companye, as"/>
        
