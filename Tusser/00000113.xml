        <underline method="pen" type="straight" language="EN" text="hawine is"/>
        <underline method="pen" type="straight" language="EN" text="strawe,"/>
        <underline method="pen" type="straight" language="EN" text="and rake it, and sets it on"/>
        <underline method="pen" type="straight" language="EN" text="binds it, and sets it on shocks"/>
        <underline method="pen" type="straight" language="EN" text="standing by dallops"/>
        <underline method="pen" type="straight" language="EN" text="early in morning"/>
        <underline method="pen" type="straight" language="EN" text="barlie is raked"/>
        <underline method="pen" type="straight" language="EN" text="of such raking,"/>
        <underline method="pen" type="straight" language="EN" text="scatring of barlie, is seene to"/>
        <underline method="pen" type="straight" language="EN" text="conscience"/>
        <underline method="pen" type="straight" language="EN" text="taken in time, is a husbandly"/>
        <underline method="pen" type="straight" language="EN" text="beaten with showers"/>
        <underline method="pen" type="straight" language="EN" text="turning and tossing, they shed as they lie"/>
        <underline method="pen" type="straight" language="EN" text="weather be faire, and tidie thy graine"/>
        <underline method="pen" type="straight" language="EN" text="speedily carrege, for feare of a raine"/>
        <underline method="pen" type="straight" language="EN" text="and showers, deceiueth a menie"/>
        <underline method="pen" type="straight" language="EN" text="lingering lubbers, loose many a penie"/>
        
        <underline method="pen" type="straight" language="EN" text="Tithe of rakings."/>
        <underline method="pen" type="straight" language="EN" text="Lingring"/>
        
