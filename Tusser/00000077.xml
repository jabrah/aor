    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
    
    <page filename="00000077.tif" reader="Harvey" pagination="36[r]" signature="ij."/>
    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="LAy compas"/>
        <underline method="pen" type="straight" language="EN" text="euer ye plow"/>
        <underline method="pen" type="straight" language="EN" text="doong heapes alowe"/>
        <underline method="pen" type="straight" language="EN" text="barlie to growe"/>
        <underline method="pen" type="straight" language="EN" text="s&eacute;edes"/>
        <underline method="pen" type="straight" language="EN" text="Moone doth growe"/>
        <underline method="pen" type="straight" language="EN" text="pease not too"/>
        <underline method="pen" type="straight" language="EN" text="plough ye set"/>
        <underline method="pen" type="straight" language="EN" text="sowen, sore noieth"/>
        <underline method="pen" type="straight" language="EN" text="ripe, hog stroieth"/>
        <underline method="pen" type="straight" language="EN" text="Some prouender"/>
        <underline method="pen" type="straight" language="EN" text="oxen that"/>
        <underline method="pen" type="straight" language="EN" text="hay, and not strawe"/>
        <underline method="pen" type="straight" language="EN" text="st&eacute;eres ye"/>
        <underline method="pen" type="straight" language="EN" text="mixe strawe"/>
        <underline method="pen" type="straight" language="EN" text="carting, ill tillage"/>
        <underline method="pen" type="straight" language="EN" text="makes som to flie village"/>
        <underline method="pen" type="straight" language="EN" text="cattle aright"/>
        <underline method="pen" type="straight" language="EN" text="k&eacute;epe them in plight"/>
        
        <underline method="pen" type="straight" language="EN" text="Cut vines and osier"/>
        <underline method="pen" type="straight" language="EN" text="plash hedge of enclosier"/>
        <underline method="pen" type="straight" language="EN" text="strike"/>
        <underline method="pen" type="straight" language="EN" text="of deluing mowles."/>
        <underline method="pen" type="straight" language="EN" text="Such hillocks in"/>
        <underline method="pen" type="straight" language="EN" text="lay leauelled plaine"/>
        <underline method="pen" type="straight" language="EN" text="Cow little giueth"/>
        <underline method="pen" type="straight" language="EN" text="that hardly liueth"/>
        
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="LAy compas ynow, er euer ye plow."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Place doong heapes alowe, more barlie to growe."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Fine s&eacute;edes then sowe, whilst Moone doth growe."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Late sowen, fore noieth, late ripe, hog stroieth."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="To oxen that drawe, give hay, and not strawe."/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="st&eacute;eres ye"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="mixe strawe"/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Much carting, ill tillage, makes som to flie village."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Use cattle aright, to k&eacute;epe them in plight."/>
        
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Cut vines and osier, plash hedge of enclosier."/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="of deluing mowles"/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="of deluing mowles. Such hillocks in vaine, lay leauelled plaine."/>
        <mark name="vertical_bar" method="pen" place="intext" language="EN" text="Cow little giueth, that hardly liueth."/>
        
    </annotation>
</transcription>