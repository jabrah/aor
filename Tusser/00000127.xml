        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>This, &amp; the next, two notable sonets, aswel for stile, as matter.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="poore at enclosing doo grutch"/>
        <underline method="pen" type="straight" language="EN" text="should haue but too much"/>
        <underline method="pen" type="straight" language="EN" text="againe nothing at all"/>
        <underline method="pen" type="straight" language="EN" text="If order might"/>
        <underline method="pen" type="straight" language="EN" text="what were to the feuerall ground"/>
        <underline method="pen" type="straight" language="EN" text="an enuious"/>
        <underline method="pen" type="straight" language="EN" text="naughtie neighbour"/>
        <underline method="pen" type="straight" language="EN" text="cumbersome fetches"/>
        <underline method="pen" type="straight" language="EN" text="friendship like Judas that carried the purse"/>
        <underline method="pen" type="straight" language="EN" text="His head is a storehouse, with quarrels full fraught"/>
        <underline method="pen" type="straight" language="EN" text="memorie pregnant"/>
        <underline method="pen" type="straight" language="EN" text="mouth full of venim, his"/>
        <underline method="pen" type="straight" language="EN" text="lips out of frame"/>
        <underline method="pen" type="straight" language="EN" text="tonge a false witnes"/>
        <underline method="pen" type="straight" language="EN" text="eies be promooters"/>
        <underline method="pen" type="straight" language="EN" text="ears be as spials, alarum to"/>
        <underline method="pen" type="straight" language="EN" text="hands be as tyrants"/>
        <underline method="pen" type="straight" language="EN" text="feete at thine elbow, as serpent"/>
        <underline method="pen" type="straight" language="EN" text="breast full of rancor, like"/>
        <underline method="pen" type="straight" language="EN" text="Canker to freat"/>
        <underline method="pen" type="straight" language="EN" text="hart like a Lion"/>
        <underline method="pen" type="straight" language="EN" text="gate like a sheepebiter, fleering aside"/>
        <underline method="pen" type="straight" language="EN" text="looke like a coxcombe, vp puffed"/>
        <underline method="pen" type="straight" language="EN" text="made of brasse"/>
        <underline method="pen" type="straight" language="EN" text="iesture like Dauus"/>
        <underline method="pen" type="straight" language="EN" text="brag as Thersites, with"/>
        <underline method="pen" type="straight" language="EN" text="in his furie, shall swell"/>
        
        <mark name="quotation_mark" method="pen" place="left_margin" language="EN" text="If order might therein be found:"/>
        <mark name="quotation_mark" method="pen" place="left_margin" language="EN" text="what were to the feuerall ground?"/>
