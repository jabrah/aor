    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    
    <page filename="00000028.tif" reader="Harvey" pagination="[11v]"/>
    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="seeketh reuengement"/>
        <underline method="pen" type="straight" language="EN" text="quiet nor safetie"/>
        <underline method="pen" type="straight" language="EN" text="of wilfulnes trieth"/>
        <underline method="pen" type="straight" language="EN" text="striue for a coxcome"/>
        <underline method="pen" type="straight" language="EN" text="thriue as a daw"/>
        <underline method="pen" type="straight" language="EN" text="To hunters and haukers,"/>
        <underline method="pen" type="straight" language="EN" text="milde answere"/>
        <underline method="pen" type="straight" language="EN" text="where a mans better, wil open a gap"/>
        <underline method="pen" type="straight" language="EN" text="resist not with rudenes for"/>
        <underline method="pen" type="straight" language="EN" text="world, for a churle that"/>
        <underline method="pen" type="straight" language="EN" text="keepe that is his owne"/>
        <underline method="pen" type="straight" language="EN" text="as of curtesie smels"/>
        <underline method="pen" type="straight" language="EN" text="fauor and friendship, where"/>
        <underline method="pen" type="straight" language="EN" text="from gadding, but when"/>
        <underline method="pen" type="straight" language="EN" text="and fasting daie"/>
        <underline method="pen" type="straight" language="EN" text="what custome thou keepest"/>
        <underline method="pen" type="straight" language="EN" text="let others"/>
        <underline method="pen" type="straight" language="EN" text="their tithing, be"/>
        <underline method="pen" type="straight" language="EN" text="vnto Godward, not"/>
        <underline method="pen" type="straight" language="EN" text="ill tithers, ill thriuers, most"/>
        <underline method="pen" type="straight" language="EN" text="Paie weekelie thy workman"/>
        <underline method="pen" type="straight" language="EN" text="paie quarterlie seruants"/>
        <underline method="pen" type="straight" language="EN" text="Giue garment to such, as deserue"/>
        <underline method="pen" type="straight" language="EN" text="Beware raskabilia, slothfull"/>
        <underline method="pen" type="straight" language="EN" text="purloiners and filchers, that"/>
        <underline method="pen" type="straight" language="EN" text="lubbers"/>
        <underline method="pen" type="straight" language="EN" text="roules in expences, but"/>
        <underline method="pen" type="straight" language="EN" text="Good wife, and"/>
        <underline method="pen" type="straight" language="EN" text="good seruant, good laborer, earneth"/>
        <underline method="pen" type="straight" language="EN" text="Good friend, and good neighbor"/>
        <underline method="pen" type="straight" language="EN" text="hartilie welcome"/>
        
        <symbol name="JC" place="head"/>
        
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="milde answere"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="resist not with rudenes for"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="ill tithers, ill thriuers, most"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="purloiners and filchers, that"/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="good seruant, good laborer, earneth"/>
        
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="seeketh reuengement"/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="as of curtesie smels"/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="and fasting daie"/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="Giue garment to such, as deserue"/>
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="Good wife, and good children, are worthie to eate, good seruant, good laborer, earneth their meate. Good friend, and good neighbor, that fellowlie gest: with hartilie welcome, should haue of the best."/>
        
        <mark name="dot" method="pen" place="intext"/> <!--saie,-->
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="gadding"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="slothfull"/>
        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="dot" method="pen" place="intext"/> <!--eate,-->
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="earneth"/>
        
    </annotation>
