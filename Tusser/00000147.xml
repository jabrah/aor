    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="Good droie woorth"/>
        <underline method="pen" type="straight" language="EN" text="Good droie"/>
        <underline method="pen" type="straight" language="EN" text="needfull is truelie, than some in"/>
        <underline method="pen" type="straight" language="EN" text="Though homelie be"/>
        <underline method="pen" type="straight" language="EN" text="let cleanlie"/>
        <underline method="pen" type="straight" language="EN" text="slut and a slouen, be"/>
        <underline method="pen" type="straight" language="EN" text="Laie bane for a rat"/>
        <underline method="pen" type="straight" language="EN" text="dairie, haue trap for"/>
        <underline method="pen" type="straight" language="EN" text="poisoning seruant, thy selfe and thy brats."/>
        <underline method="pen" type="straight" language="EN" text="No scouring for pride"/>
        <underline method="pen" type="straight" language="EN" text="scouring be"/>
        <underline method="pen" type="straight" language="EN" text="pride without profit"/>
        <underline method="pen" type="straight" language="EN" text="scouring"/>
        <underline method="pen" type="straight" language="EN" text="kettles from knocks"/>
        <underline method="pen" type="straight" language="EN" text="tubs out"/>
        <underline method="pen" type="straight" language="EN" text="mending is costlie, and crackt is soone"/>
        <underline method="pen" type="straight" language="EN" text="Else run in the"/>
        <underline method="pen" type="straight" language="EN" text="wash well, &amp; wring well"/>
        <underline method="pen" type="straight" language="EN" text="but beat ye"/>
        <underline method="pen" type="straight" language="EN" text="Washing"/>
        <underline method="pen" type="straight" language="EN" text="lack beating, I feare it be yow"/>
        <underline method="pen" type="straight" language="EN" text="In washing by hand"/>
        <underline method="pen" type="straight" language="EN" text="launders and millers, be quick of their"/>
        <underline method="pen" type="straight" language="EN" text="Drie sunne"/>
        <underline method="pen" type="straight" language="EN" text="drie winde"/>
        <underline method="pen" type="straight" language="EN" text="Safe binde"/>
        <underline method="pen" type="straight" language="EN" text="safe finde"/>
        <underline method="pen" type="straight" language="EN" text="wring well saith"/>
        <underline method="pen" type="straight" language="EN" text="trust without heede"/>
        <underline method="pen" type="straight" language="EN" text="tale and take count, is"/>
        
