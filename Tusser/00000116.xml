        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="One aker well compassed, passeth some three"/>
        <underline method="pen" type="straight" language="EN" text="barne shall at haruest, declare it to thee"/>
        <underline method="pen" type="straight" language="EN" text="riding"/>
        <underline method="pen" type="straight" language="EN" text="prices of vittels, the yeere"/>
        <underline method="pen" type="straight" language="EN" text="what to be selling,"/>
        <underline method="pen" type="straight" language="EN" text="what to be buieng, to bring"/>
        <underline method="pen" type="straight" language="EN" text="Though buieng and selling, doth"/>
        <underline method="pen" type="straight" language="EN" text="chopping and changing, I cannot"/>
        <underline method="pen" type="straight" language="EN" text="theefe and his marrow"/>
        <underline method="pen" type="straight" language="EN" text="rich in his bargaining, needes"/>
        <underline method="pen" type="straight" language="EN" text="buier and seller, full far is he sought"/>
        <underline method="pen" type="straight" language="EN" text="who buieth at first hand"/>
        <underline method="pen" type="straight" language="EN" text="who at the next"/>
        <underline method="pen" type="straight" language="EN" text="At first hand he"/>
        <underline method="pen" type="straight" language="EN" text="paieth all downe"/>
        <underline method="pen" type="straight" language="EN" text="at second, that hath"/>
        <underline method="pen" type="straight" language="EN" text="At third hand he"/>
        <underline method="pen" type="straight" language="EN" text="buieth of trust"/>
        <underline method="pen" type="straight" language="EN" text="his hand who"/>
        <underline method="pen" type="straight" language="EN" text="to buie it the cheaper, haue chinkes in"/>
        <underline method="pen" type="straight" language="EN" text="Touch kept is"/>
        <underline method="pen" type="straight" language="EN" text="is paie and dispatchu him, er euer"/>
        
        <underline method="pen" type="straight" language="EN" text="at first hand"/>
        <underline method="pen" type="straight" language="EN" text="Readie monie"/>
        <underline method="pen" type="straight" language="EN" text="best cheape"/>
        
