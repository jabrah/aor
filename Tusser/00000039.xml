        <underline method="pen" type="straight" language="EN" text="seede haue his longing"/>
        <underline method="pen" type="straight" language="EN" text="let soile haue hir lust"/>
        <underline method="pen" type="straight" language="EN" text="beare out the hardnes"/>
        <underline method="pen" type="straight" language="EN" text="mixeth to miller, the rie with the wheat"/>
        <underline method="pen" type="straight" language="EN" text="Temmes lofe, on"/>
        <underline method="pen" type="straight" language="EN" text="sowe it not mixed"/>
        <underline method="pen" type="straight" language="EN" text="least rie tarie wheat, till"/>
        <underline method="pen" type="straight" language="EN" text="it shed"/>
        <underline method="pen" type="straight" language="EN" text="white wheat be ton, be"/>
