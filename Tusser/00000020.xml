    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="Where huswife and huswiferie, ioineth with thease"/>
        <underline method="pen" type="straight" language="EN" text="there wealth in abundance, is gotten with"/>
        <underline method="pen" type="straight" language="EN" text="husband is he, that to labour doth fall"/>
        <underline method="pen" type="straight" language="EN" text="thrift by that labour, be any way caught"/>
        <underline method="pen" type="straight" language="EN" text="houshold and housholdrie"/>
        <underline method="pen" type="straight" language="EN" text="and huswiferie, if it be good"/>
        <underline method="pen" type="straight" language="EN" text="togither, as cosins in blood"/>
        
        <underline method="pen" type="straight" language="EN" text="Ladder to thrift"/>
        <underline method="pen" type="straight" language="EN" text="grudge in youth no drudgery"/>
        <underline method="pen" type="straight" language="EN" text="come by knowledge perfectly"/>
        <underline method="pen" type="straight" language="EN" text="count no trauell slauerie"/>
        <underline method="pen" type="straight" language="EN" text="brings in penie fauerlie"/>
        <underline method="pen" type="straight" language="EN" text="k&eacute;epe thy gettings couertlie"/>
        <underline method="pen" type="straight" language="EN" text="To lash not out, too lashinglie"/>
        <underline method="pen" type="straight" language="EN" text="for feare of pinching"/>
        <underline method="pen" type="straight" language="EN" text="To get good plot to occupie"/>
        <underline method="pen" type="straight" language="EN" text="k&eacute;epe they couenants"/>
        
        <symbol name="SS" place="right_margin" language="EN" text="Ladder to thrift"/>
        
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="that brings in penie sauerlie"/>
        
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="thrift by that labour, be any way caught"/>
        
        <mark name="dot" method="pen" place="intext"/> <!--drudgery, to-->
        <mark name="plus_sign" method="pen" place="intext"/>
        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="bracket" method="pen" place="intext" language="EN" text="To lash not out, too lashinglie, for feare of pinching penurie."/>
        
