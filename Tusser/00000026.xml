    <annotation>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>Borrowing, &amp; Lending.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="Who liuing but lends, and"/>
        <underline method="pen" type="straight" language="EN" text="buieng and selling, might"/>
        <underline method="pen" type="straight" language="EN" text="thou keepest, thy thouch for"/>
        <underline method="pen" type="straight" language="EN" text="Who quick be to borow, and slow be to"/>
        <underline method="pen" type="straight" language="EN" text="their credit is naught, go they neuer"/> 
        <underline method="pen" type="straight" language="EN" text="By shifting and borrowing, who so"/>
        <underline method="pen" type="straight" language="EN" text="Then lay to liue warily, and wisely to"/>
        <underline method="pen" type="straight" language="EN" text="for prodigall liuers, haue seldom"/>
        <underline method="pen" type="straight" language="EN" text="the foole at the bottom"/>
        <underline method="pen" type="straight" language="EN" text="wife, at the brim"/>
        <underline method="pen" type="straight" language="EN" text="Who careth, nor spareth, till spent"/>
        <underline method="pen" type="straight" language="EN" text="bobbing, not robbing, be fearefull"/>
        <underline method="pen" type="straight" language="EN" text="Where welthines floweth, no friendship"/>
        <underline method="pen" type="straight" language="EN" text="whom pouertie pincheth, hath friendship"/>
        <underline method="pen" type="straight" language="EN" text="happie is he, by example"/>
        <underline method="pen" type="straight" language="EN" text="take heede by the fall, of a mischieued"/>
        <underline method="pen" type="straight" language="EN" text="breaketh his credit, or cracketh"/>
        <underline method="pen" type="straight" language="EN" text="trust such with a suretie"/>
        <underline method="pen" type="straight" language="EN" text="Account it wel sold, that is justlie"/>
        <underline method="pen" type="straight" language="EN" text="count it wel bought, that"/>
        <underline method="pen" type="straight" language="EN" text="Leaue Princes affaires vndeskanted"/>
        <underline method="pen" type="straight" language="EN" text="keepe thy selfe out, of the Magistrates clawes"/>
        
        <symbol name="JC" place="head"/>
        
        <symbol name="Bisected_circle" place="left_margin"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="At some time to borow, account it no shame, if iustlie thou keepest, thy touch for the same: Who quick be to borow, and slow be to paie, their credit is naught, go they neuer so gaie."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="their credit is naught, go they neuer"/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Where welthines floweth, no friendship can lack, whom pouertie pincheth, hath friendship as slack: Then happie is he, by example that can, take heede by the fall, of a mischieued man."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="whom pouertie pincheth, hath friendship"/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="take heede by the fall, of a mischieued"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="trust such with a suretie"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Account it wel sold, that is justlie well paid. and count it wel bought, that is neuer denaid: But yet here is tone, here is tother, doth best, for buier and seller, for quiet and rest."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="keepe thy selfe out, of the Magistrates clawes"/>
        
        <mark name="plus_sign" method="pen" place="right_margin"/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="trust such with a suretie"/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="Account it wel sold, that is justlie well paid"/>
        <mark name="plus_sign" method="pen" place="right_margin"/>
        <mark name="double_vertical_bar" method="pen" place="right_margin"/>
        
