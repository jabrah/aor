        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="suretie"/>
        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="Princes affaires"/>
