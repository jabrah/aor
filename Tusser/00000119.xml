    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="thriftie that teacheth"/>
        <underline method="pen" type="straight" language="EN" text="timelie to trauerse, the thing that thou triue"/>
        <underline method="pen" type="straight" language="EN" text="trustie"/>
        <underline method="pen" type="straight" language="EN" text="to thee"/>
        <underline method="pen" type="straight" language="EN" text="Thee"/>
        <underline method="pen" type="straight" language="EN" text="trustily thriftines, trowleth"/>
        <underline method="pen" type="straight" language="EN" text="trauell, to tarie the tide"/>
        <underline method="pen" type="straight" language="EN" text="thankfull thy talent"/>
        <underline method="pen" type="straight" language="EN" text="teacheth, thy time to transpose"/>
        <underline method="pen" type="straight" language="EN" text="twise to thee teached"/>
        <underline method="pen" type="straight" language="EN" text="teach twentie times ten"/>
        <underline method="pen" type="straight" language="EN" text="trade, thou"/>
        <underline method="pen" type="straight" language="EN" text="vnder, sir hobbard de hoy,"/>
        <underline method="pen" type="straight" language="EN" text="let lustie, laie wisely to wiue"/>
        <underline method="pen" type="straight" language="EN" text="laie now, or else neuer to thriue"/>
        <underline method="pen" type="straight" language="EN" text="make sure for terme of thy life,"/>
        <underline method="pen" type="straight" language="EN" text="saue somewhat for children and wife"/>
        <underline method="pen" type="straight" language="EN" text="Who looseth their youth, shall rue it in age"/>
        
