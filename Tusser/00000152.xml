        <underline method="pen" type="straight" language="EN" text="two legged foxes, keepe watches"/>
        <underline method="pen" type="straight" language="EN" text="thing at quiet"/>
        <underline method="pen" type="straight" language="EN" text="Take heede it"/>
        <underline method="pen" type="straight" language="EN" text="True pittie is meedefull"/>
        <underline method="pen" type="straight" language="EN" text="clothes in garden"/>
        <underline method="pen" type="straight" language="EN" text="no trinkets"/>
        <underline method="pen" type="straight" language="EN" text="doore leaue vnbolted, for"/>
