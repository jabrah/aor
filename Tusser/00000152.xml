        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="Make lackey to trudge, Make seruant thy drudge."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="two legged foxes, keepe watches"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="No clothes in garden, no trinkets without, no doore leaue vnbolted, for feare of a dout."/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Vse mirth and good woorde, At bed and at boorde."/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="At bed and at boord, howsoeuer befall, what euer God sendeth, be merrie withall."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="No brawling make, No ielousie take."/>
        
