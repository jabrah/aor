        <underline method="pen" type="straight" language="EN" text="and at boorde"/>
        <underline method="pen" type="straight" language="EN" text="make him good"/>
        <underline method="pen" type="straight" language="EN" text="merrie togither, while"/>
        <underline method="pen" type="straight" language="EN" text="At bed"/>
        <underline method="pen" type="straight" language="EN" text="and at boord,"/>
        <underline method="pen" type="straight" language="EN" text="euer God sendeth, be"/>
