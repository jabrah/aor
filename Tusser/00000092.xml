        <underline method="pen" type="straight" language="EN" text="Three poles to a hillock (I pas"/>
        <underline method="pen" type="straight" language="EN" text="yeeld thee more profit"/>
        <underline method="pen" type="straight" language="EN" text="barke to the tanner, er timber"/>
        <underline method="pen" type="straight" language="EN" text="by the ground, or else"/>
        <underline method="pen" type="straight" language="EN" text="saue crooked, for mill and for ships"/>
        <underline method="pen" type="straight" language="EN" text="hewing, saue carpenters"/>
        <underline method="pen" type="straight" language="EN" text="well fenced"/>
        <underline method="pen" type="straight" language="EN" text="well stadled, without"/>
        <underline method="pen" type="straight" language="EN" text="Leaue growing for stadles"/>
        <underline method="pen" type="straight" language="EN" text="In bushes, in hedgerowe, in groue and in"/>
        <underline method="pen" type="straight" language="EN" text="elme, ash and crabtree"/>
        <underline method="pen" type="straight" language="EN" text="cart and for"/>
        <underline method="pen" type="straight" language="EN" text="step for a stile"/>
        <underline method="pen" type="straight" language="EN" text="hazel for forks"/>
        <underline method="pen" type="straight" language="EN" text="huluer and thorne"/>
