    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="The yonger sonne"/>
        <underline method="pen" type="straight" language="EN" text="glad to seeke, from creeke"/>
        <underline method="pen" type="straight" language="EN" text="In world is set, ynough to get"/>
        <underline method="pen" type="straight" language="EN" text="where and whan, that scarsely"/>
        <underline method="pen" type="straight" language="EN" text="By learning some"/>
        <underline method="pen" type="straight" language="EN" text="By ship and plough, some"/>
        <underline method="pen" type="straight" language="EN" text="some so wiue, that trim"/>
        <underline method="pen" type="straight" language="EN" text="Youth hardnes taught"/>
        <underline method="pen" type="straight" language="EN" text="knowlege wraught"/>
        <underline method="pen" type="straight" language="EN" text="do prooue, to shift and shooue"/>
        <underline method="pen" type="straight" language="EN" text="make sawsie"/>
        <underline method="pen" type="straight" language="EN" text="to fetch, a Tiburne"/>
        <underline method="pen" type="straight" language="EN" text="rampish"/>
        <underline method="pen" type="straight" language="EN" text="garment trim"/>
        <underline method="pen" type="straight" language="EN" text="the summe"/>
        <underline method="pen" type="straight" language="EN" text="end it is, that noted is"/>
        <underline method="pen" type="straight" language="EN" text="if it bide, with vertue"/>
        <underline method="pen" type="straight" language="EN" text="deserueth fame"/>
        
        <underline method="pen" type="straight" language="EN" text="Hardnes"/>
        <underline method="pen" type="straight" language="EN" text="worst"/>
        <underline method="pen" type="straight" language="EN" text="Cocking of youth"/>
        <underline method="pen" type="straight" language="EN" text="Not pride"/>
        <underline method="pen" type="straight" language="EN" text="but welth in age"/>
        
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="By learning some, to riches come,"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="The end it is, that noted is,"/>
        
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="do prooue, to shift and shooue,"/>
        
        <mark name="vertical_bar" method="chalk" place="intext" language="EN" text="But where and whan, that scarsely can, the wisest tell: By learning some, to riches come, By ship and plough, some get ynough, And some so wiue, that trun they thriue, and speede full well.
            35 To this before, adde one thing more, Youth hardnes taught, with knowlege wraught, Most apt to prooue, to shift and shooue, among the best: Where cockind Dads, make sawsie lads, In youth to rage, to beg in age,"/> <!--JG: I'd, in general, not link the stretched chalk marks to any printed text-->

