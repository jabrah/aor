    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="Depart not with al that thou hast to thy childe"/>
        <underline method="pen" type="straight" language="EN" text="much lesse"/>
        <underline method="pen" type="straight" language="EN" text="greatest preferment, that"/>
        <underline method="pen" type="straight" language="EN" text="learning and nurture, to traine"/>
        <underline method="pen" type="straight" language="EN" text="though left as a"/>
        <underline method="pen" type="straight" language="EN" text="consumeth to nothing, as block in"/>
        <underline method="pen" type="straight" language="EN" text="fruites of thy faith, to heauen aforehand"/>
        <underline method="pen" type="straight" language="EN" text="maketh thy store, with his blessing to swim"/>
        <underline method="pen" type="straight" language="EN" text="to get riches, by sea and by land"/>
        <underline method="pen" type="straight" language="EN" text="ventreth his life, in his enimies hand"/> 
        <underline method="pen" type="straight" language="EN" text="setteth his soule vpon sixe or on seauen"/>
        <underline method="pen" type="straight" language="EN" text="fearing nor caring, for hell nor for heauen"/>
        <underline method="pen" type="straight" language="EN" text="pincheth and spareth, and pineth his life"/>
        <underline method="pen" type="straight" language="EN" text="vp bags, for to leaue to his wife"/>
        <underline method="pen" type="straight" language="EN" text="open the chest"/>
        <underline method="pen" type="straight" language="EN" text="can sooth hir, and"/>
        <underline method="pen" type="straight" language="EN" text="alway"/>
        <underline method="pen" type="straight" language="EN" text="takes part of Gods benefits"/>
        <underline method="pen" type="straight" language="EN" text="lessons approoued"/>
        <underline method="pen" type="straight" language="EN" text="ye note"/>
        <underline method="pen" type="straight" language="EN" text="saue and auantage ye, many a grote"/>
        <underline method="pen" type="straight" language="EN" text="can follow"/>
        <underline method="pen" type="straight" language="EN" text="occasion found"/>
        <underline method="pen" type="straight" language="EN" text="euerie lesson, may saue ye a pound"/>
        
