    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="In gouing at haruest"/>
        <underline method="pen" type="straight" language="EN" text="selfe on a mow"/>
        <underline method="pen" type="straight" language="EN" text="Stack pease"/>
        <underline method="pen" type="straight" language="EN" text="it for rowen, til"/>
        <underline method="pen" type="straight" language="EN" text="haruest time, haruest"/>
        <underline method="pen" type="straight" language="EN" text="make all togither, good cheere"/>
        <underline method="pen" type="straight" language="EN" text="out the black boule, of bleith"/>
        <underline method="pen" type="straight" language="EN" text="them be merie, all haruest"/>
        <underline method="pen" type="straight" language="EN" text="better thou thriuest, the gladder am I"/>
        <underline method="pen" type="straight" language="EN" text="carrie out compas"/>
        <underline method="pen" type="straight" language="EN" text="it on heape, in the"/>
        <underline method="pen" type="straight" language="EN" text="Whose compas is rotten,"/>
        <underline method="pen" type="straight" language="EN" text="spred as it should"/>
        <underline method="pen" type="straight" language="EN" text="ladder may"/>
        
        <underline method="pen" type="straight" language="EN" text="Peasestack"/>
        <underline method="pen" type="straight" language="EN" text="gleaning"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="In haruest time, haruest folke, seruants and all, should make all togither, good cheere in the hall. And fill out the black boule, of bleith to their song: and let them be merie, all haruest time long."/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        
