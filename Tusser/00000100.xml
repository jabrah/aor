<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor_20141118.xsd">
    
    <page filename="00000100.tif" reader="Harvey" pagination="[47v]"/>
    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="twifallow thy land"/>
        <underline method="pen" type="straight" language="EN" text="plow thou with the furst"/>
        <underline method="pen" type="straight" language="EN" text="get tumbrell"/>
        <underline method="pen" type="straight" language="EN" text="compas that fallow, as soone"/>
        <underline method="pen" type="straight" language="EN" text="skilfull bestowit,"/>
        <underline method="pen" type="straight" language="EN" text="dripped and shadowd, with bushes"/>
        <underline method="pen" type="straight" language="EN" text="plots full of galles, if ye"/>
        <underline method="pen" type="straight" language="EN" text="to lay to their bones"/>
        <underline method="pen" type="straight" language="EN" text="To gras with"/>
        <underline method="pen" type="straight" language="EN" text="some medow"/>
        <underline method="pen" type="straight" language="EN" text="neither their mothers"/>
        <underline method="pen" type="straight" language="EN" text="Where water is plentie"/>
        <underline method="pen" type="straight" language="EN" text="barth to sit"/>
        <underline method="pen" type="straight" language="EN" text="looke well vnto"/>
        <underline method="pen" type="straight" language="EN" text="Pinch neuer thy wennels, of water"/>
        <underline method="pen" type="straight" language="EN" text="Sommer"/>
        <underline method="pen" type="straight" language="EN" text="in frost"/>
        <underline method="pen" type="straight" language="EN" text="much, ouerlay not"/>
        <underline method="pen" type="straight" language="EN" text="thy cattel, be lustie and"/>
        <underline method="pen" type="straight" language="EN" text="pinch them of pasture"/>
        <underline method="pen" type="straight" language="EN" text="lift at their tailes, er an Winter"/>
        
        <underline method="pen" type="straight" language="EN" text="Twifallowing."/>
        <underline method="pen" type="straight" language="EN" text="Carie out"/>
        <underline method="pen" type="straight" language="EN" text="grasse"/>
        <underline method="pen" type="straight" language="EN" text="Ouerlay"/>
        <underline method="pen" type="straight" language="EN" text="not thy"/>
        
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="Twifallowing"/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="Ouerlay"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="What wisedome for profit, aduiseth vnto: that husband and huswife, must willingly do."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="looke well vnto"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="For coueting much, ouerlay not thy ground, and then shall thy cattel, be lustie and sound."/>
        
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="Pinch neuer thy wennels, of water"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="much, ouerlay not"/>
        
    </annotation>
</transcription>