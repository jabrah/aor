    <page filename="00000140.tif" reader="Harvey" pagination="[67v]"/>
    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="makes huswife to shine"/>
        <underline method="pen" type="straight" language="EN" text="chance, to be readie at hand"/>
        <underline method="pen" type="straight" language="EN" text="that all thing in season, be huswifelie fed."/>
        <underline method="pen" type="straight" language="EN" text="Dame practise is she"/>
        <underline method="pen" type="straight" language="EN" text="childe to be ciuill, keepe seruant in awe"/>
        <underline method="pen" type="straight" language="EN" text="Who careles"/>
        <underline method="pen" type="straight" language="EN" text="Occasion doe"/>
        <underline method="pen" type="straight" language="EN" text="where, a respect to"/>
        <underline method="pen" type="straight" language="EN" text="of thy life, any slander"/>
        <underline method="pen" type="straight" language="EN" text="do knowe, though"/>
        <underline method="pen" type="straight" language="EN" text="will abrode, when a mischiefe"/>
        <underline method="pen" type="straight" language="EN" text="loue of they"/>
        <underline method="pen" type="straight" language="EN" text="Use friendly thy"/>
        <underline method="pen" type="straight" language="EN" text="hath thy friendship"/>
        <underline method="pen" type="straight" language="EN" text="Strike nothing"/>
        <underline method="pen" type="straight" language="EN" text="Take heede"/>
        <underline method="pen" type="straight" language="EN" text="by like malice, be bid to like"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Haue euerie wehere, a respect to thy waies, that none of thy life, any slander may raies. What many do knowe, though a time it be hid, at length will abrode, when a mischiefe shall bid."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="by like malice, be bid to like feast"/>
        
    </annotation>
</transcription>