    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="Howse calfe"/>
        <underline method="pen" type="straight" language="EN" text="go sockle"/>
        <underline method="pen" type="straight" language="EN" text="The senior weaned, his yoonger"/>
        <underline method="pen" type="straight" language="EN" text="More stroken and made of, when ought"/>
        <underline method="pen" type="straight" language="EN" text="more gentle ye make it, for yoke"/>
        <underline method="pen" type="straight" language="EN" text="the paile"/>
        <underline method="pen" type="straight" language="EN" text="Geld bulcalfe"/>
        <underline method="pen" type="straight" language="EN" text="ramlamb"/>
        <underline method="pen" type="straight" language="EN" text="the ton, for to pleasure"/>
        <underline method="pen" type="straight" language="EN" text="shew greater, when"/>
        <underline method="pen" type="straight" language="EN" text="readie to farrow"/>
        <underline method="pen" type="straight" language="EN" text="losse of a fare of the sow"/>
        <underline method="pen" type="straight" language="EN" text="great than the losse, of two calues"/>
        <underline method="pen" type="straight" language="EN" text="togither, reare few aboue"/>
        <underline method="pen" type="straight" language="EN" text="the fairest, and"/>
        <underline method="pen" type="straight" language="EN" text="pig, that sucketh before"/>
        <underline method="pen" type="straight" language="EN" text="haue store verie"/>
        <underline method="pen" type="straight" language="EN" text="reare of a sow at once"/>
        <underline method="pen" type="straight" language="EN" text="onely but three"/>
        <underline method="pen" type="straight" language="EN" text="them also, a bore let"/>
        <underline method="pen" type="straight" language="EN" text="Geld vnder the dam"/>
        <underline method="pen" type="straight" language="EN" text="saddle, geld yoong to"/>
        <underline method="pen" type="straight" language="EN" text="cart doo not"/>
        <underline method="pen" type="straight" language="EN" text="they be lustie and"/>
        <underline method="pen" type="straight" language="EN" text="a point, to be learned"/>
        
        <underline method="pen" type="straight" language="EN" text="Howling"/>
        <underline method="pen" type="straight" language="EN" text="Rearing of pigs."/>
        <underline method="pen" type="straight" language="EN" text="large br&eacute;ed of hogs"/>
        <underline method="pen" type="straight" language="EN" text="of horse coltes."/>
        
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="For now is the losse of a fare of the sow: more great than the losse, of two calues of thy cow."/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Who hath a desire, to haue store verie large, at Whitsontide, let him giue huswife a charge. To reare of a sow at once, onely but three: and one of them also, a bore let it bee."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="large br&eacute;ed of hogs"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="Thy colts for the saddle, geld yoong to be light, for cart doo not so, if thou iudgest aright. Nor geld not, but when they be lustie and fat: for there is a point, to be learned in that."/>
        
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="go sockle"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="readie to farrow"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="losse of a fare of the sow"/>
        <mark name="plus_sign" method="pen" place="intext"/>
        
        
