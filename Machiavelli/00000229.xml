    	<underline method="pen" type="straight" language="EN" text="bene diuers times sacked and destroied"/>
    	<underline method="pen" type="straight" language="EN" text="very same erroure, and liue in the verie same disorder"/>
    	<underline method="pen" type="straight" language="EN" text="consider not, that those, whoo in olde time"/>
    	<underline method="pen" type="straight" language="EN" text="keepe their"/>
    	<underline method="pen" type="straight" language="EN" text="done these thinges, which"/>
    	<underline method="pen" type="straight" language="EN" text="their studies"/>
    	<underline method="pen" type="straight" language="EN" text="prepare the body"/>
    	<underline method="pen" type="straight" language="EN" text="minde not to feare"/>
    	<underline method="pen" type="straight" language="EN" text="perrills"/>
    	<underline method="pen" type="straight" language="EN" text="that Cesar"/>
    	<underline method="pen" type="straight" language="EN" text="Alexander"/>
    	<underline method="pen" type="straight" language="EN" text="men and excellente Princes in old"/>
    	<underline method="pen" type="straight" language="EN" text="formost amo[n]gest the fighters, going armed on foote"/>
    	<underline method="pen" type="straight" language="EN" text="they loste theyr state"/>
    	<underline method="pen" type="straight" language="EN" text="they would loose their life, so"/>
    	<underline method="pen" type="straight" language="EN" text="they liued"/>
    	<underline method="pen" type="straight" language="EN" text="and died vertuously"/>
    	<underline method="pen" type="straight" language="EN" text="condemned to much"/>
    	<underline method="pen" type="straight" language="EN" text="ambicion to reason"/>
    	<underline method="pen" type="straight" language="EN" text="condemned any tendernesse"/>
    	<underline method="pen" type="straight" language="EN" text="that maketh men delicate and feeble"/>
    	<underline method="pen" type="straight" language="EN" text="impossible, that they should not chaunge their"/>
    	<underline method="pen" type="straight" language="EN" text="forme of liuing, and theyr"/>
    	<underline method="pen" type="straight" language="EN" text="prouinces not to chaunge fortune"/>
    	<underline method="pen" type="straight" language="EN" text="lamented youre ordinaunces, I"/>
    	<underline method="pen" type="straight" language="EN" text="selfe no good experience, you"/>
    	
    	<underline method="pen" type="straight" language="EN" text="Cesar &amp; Alexander"/>
    	<underline method="pen" type="straight" language="EN" text="formost in"/>

