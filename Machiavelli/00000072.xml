    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="first the armies"/>
    	<underline method="pen" type="straight" language="EN" text="neglected"/>
    	<underline method="pen" type="straight" language="EN" text="the orders"/>
    	<underline method="pen" type="straight" language="EN" text="the kingdomes"/>
    	<underline method="pen" type="straight" language="EN" text="matter of exercises, I saye"/>
    	<underline method="pen" type="straight" language="EN" text="make good armies"/>
    	<underline method="pen" type="straight" language="EN" text="hardened the"/>
    	<underline method="pen" type="straight" language="EN" text="strong, swift, and handsome"/>
    	<underline method="pen" type="straight" language="EN" text="stand in the orders"/>
    	<underline method="pen" type="straight" language="EN" text="to obey to signes"/>
    	<underline method="pen" type="straight" language="EN" text="to soundes"/>
    	<underline method="pen" type="straight" language="EN" text="to the voice of the capitayne"/>
    	<underline method="pen" type="straight" language="EN" text="knowe, standing, to retire them"/>
    	<underline method="pen" type="straight" language="EN" text="forwardes, both fighting, &amp; marching to maintayne"/>
    	<underline method="pen" type="straight" language="EN" text="with all serious"/>
    	<underline method="pen" type="straight" language="EN" text="was neuer"/>
    	<underline method="pen" type="straight" language="EN" text="the fierce and disordered men"/>
    	<underline method="pen" type="straight" language="EN" text="much more weaker, then the fearfull that are ordered"/>
    	<underline method="pen" type="straight" language="EN" text="order driueth"/>
    	<underline method="pen" type="straight" language="EN" text="men feare"/>
    	<underline method="pen" type="straight" language="EN" text="disorder abateth"/>
    	<underline method="pen" type="straight" language="EN" text="in the ordering of their men"/>
    	<underline method="pen" type="straight" language="EN" text="a principall"/>
    	<underline method="pen" type="straight" language="EN" text="member, the which though"/>
    	<underline method="pen" type="straight" language="EN" text="litle varied with the nomber"/>
    	<underline method="pen" type="straight" language="EN" text="between sixe &amp; viij."/>
    	<underline method="pen" type="straight" language="EN" text="Legion"/>
    	<underline method="pen" type="straight" language="EN" text="Grekes, a Fallange"/>
    	<underline method="pen" type="straight" language="EN" text="Frencheman, Caterua"/>
    	<underline method="pen" type="straight" language="EN" text="the Suizzers"/>
    	<underline method="pen" type="straight" language="EN" text="warfare, keepe"/>
    	<underline method="pen" type="straight" language="EN" text="the maine battaile"/>
    	<underline method="pen" type="straight" language="EN" text="deuided it, according to their"/>
    	<underline method="pen" type="straight" language="EN" text="name moste knowen"/>
    	<underline method="pen" type="straight" language="EN" text="according to the"/>
    	<underline method="pen" type="straight" language="EN" text="aunciente, &amp; to the orders nowe a daies"/>
    	<underline method="pen" type="straight" language="EN" text="to ordayne"/>
    	
    	<underline method="pen" type="straight" language="EN" text="knovvledge"/>

