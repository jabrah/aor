    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="haue chaunsed"/>
    	<underline method="pen" type="straight" language="EN" text="men on foote prepared"/>
    	<underline method="pen" type="straight" language="EN" text="of the vauntage, which the same"/>
    	<underline method="pen" type="straight" language="EN" text="sixe thousande horse"/>
    	<underline method="pen" type="straight" language="EN" text="footemen, went"/>
    	<underline method="pen" type="straight" language="EN" text="them, he was repulsed with his most great"/>
    	<underline method="pen" type="straight" language="EN" text="as a prudent man, knewe straight"/>
    	<underline method="pen" type="straight" language="EN" text="puissaunce of the enemies weapons"/>
    	<underline method="pen" type="straight" language="EN" text="the horses they"/>
    	<underline method="pen" type="straight" language="EN" text="debilitie"/>
    	<underline method="pen" type="straight" language="EN" text="against those on foote so appointed"/>
    	<underline method="pen" type="straight" language="EN" text="to finde"/>
    	<underline method="pen" type="straight" language="EN" text="them, hee"/>
    	<underline method="pen" type="straight" language="EN" text="men of armes, to alight from their horse, and"/>
    	<underline method="pen" type="straight" language="EN" text="fighting with them, he slue them all"/>
    	<underline method="pen" type="straight" language="EN" text="three thousande: the"/>
    	<underline method="pen" type="straight" language="EN" text="casting their weapons"/>
    	<underline method="pen" type="straight" language="EN" text="grounde, yeelded"/>
    	<underline method="pen" type="straight" language="EN" text="were vnarmed, to defende them"/>
    	<underline method="pen" type="straight" language="EN" text="to offende, the Pike and the swoorde"/>
    	<underline method="pen" type="straight" language="EN" text="these weapons, and with their orders"/>
    	<underline method="pen" type="straight" language="EN" text="bee well armed, to defende"/>
    	<underline method="pen" type="straight" language="EN" text="men of armes of Carminuola"/>
    	<underline method="pen" type="straight" language="EN" text="swoorde, and in their orders to finde"/>
    	<underline method="pen" type="straight" language="EN" text="difficultie, then to come neere to the Suizzers"/>
    	<underline method="pen" type="straight" language="EN" text="maye retche them with the swoorde"/>

    	
