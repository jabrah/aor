	<underline method="pen" type="straight" language="EN" text="said capitaynes"/>
    	<underline method="pen" type="straight" language="EN" text="k&eacute;eping continually still those capitaynes"/>
    	<underline method="pen" type="straight" language="EN" text="in one gouernement"/>
    	<underline method="pen" type="straight" language="EN" text="with reputation"/>
