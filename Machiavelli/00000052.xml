    	<underline method="pen" type="straight" language="EN" text="Capitaines shoulde"/>
    	<underline method="pen" type="straight" language="EN" text="had lesse occasion"/>
    	<underline method="pen" type="straight" language="EN" text="the Emperour lesse"/>
    	<underline method="pen" type="straight" language="EN" text="Senate in the"/>
