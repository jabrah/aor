    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					{ Gallicus Equitatus:
    					Hispanicus peditatus: approbatissimi.
    					In peroratione Principis machia.
    				</marginalia_text>
    				<person name="Machiavelli"/>
    				<book title="The Prince"/>
    			</position>
    		</language>
    		<translation>
    			French cavalry:
    			Spanish infantry: most approved.
    			In the conclusion of Machiavelli's Prince.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					The best &amp; surest arming of footemen.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="finde, Consaluo, who"/>
    	<underline method="pen" type="straight" language="EN" text="his men of armes"/>
    	<underline method="pen" type="straight" language="EN" text="thousand Dutcheme[n]"/>
    	<underline method="pen" type="straight" language="EN" text="Dutchmen incountered with their Pikes lowe"/>
    	<underline method="pen" type="straight" language="EN" text="opened the power of Spaniardes"/>
    	<underline method="pen" type="straight" language="EN" text="meane of their bucklers and of the agiletie of"/>
    	<underline method="pen" type="straight" language="EN" text="might retche them with the"/>
    	<underline method="pen" type="straight" language="EN" text="the death, almost of all them, and the victorye"/>
    	<underline method="pen" type="straight" language="EN" text="many Dutchmen"/>
    	<underline method="pen" type="straight" language="EN" text="slaine in the bataile of Rauenna"/>
    	<underline method="pen" type="straight" language="EN" text="the Spanish"/>
    	<underline method="pen" type="straight" language="EN" text="them within a swoords length"/>
    	<underline method="pen" type="straight" language="EN" text="destroyed them all"/>
    	<underline method="pen" type="straight" language="EN" text="horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="Dutchemen on foote, had not"/>
    	<underline method="pen" type="straight" language="EN" text="succoured: not"/>
    	<underline method="pen" type="straight" language="EN" text="Spaniardes close together, brought themselues into a safe place"/>
    	<underline method="pen" type="straight" language="EN" text="good power"/>
    	<underline method="pen" type="straight" language="EN" text="withstande the horses"/>
    	<underline method="pen" type="straight" language="EN" text="not to haue"/>
    	<underline method="pen" type="straight" language="EN" text="men on foote"/>
    	<underline method="pen" type="straight" language="EN" text="proceedeth of the armours, and of the order"/>
    	<underline method="pen" type="straight" language="EN" text="Dutchemens weapons"/>
    	<underline method="pen" type="straight" language="EN" text="one halfe, shoulde bee appointed"/>
    	<underline method="pen" type="straight" language="EN" text="other halfe like the Dutchemen"/>
    	<underline method="pen" type="straight" language="EN" text="sixe thousande footemen"/>
    	<underline method="pen" type="straight" language="EN" text="haue three thousande"/>
    	<underline method="pen" type="straight" language="EN" text="Targaettes, after the Romane"/>
    	<underline method="pen" type="straight" language="EN" text="thousande Pikes"/>
    	<underline method="pen" type="straight" language="EN" text="thousande Harkebutters"/>
    	<underline method="pen" type="straight" language="EN" text="Dutche fashion"/>
    	<underline method="pen" type="straight" language="EN" text="place the Pikes, either in the front"/>
    	<underline method="pen" type="straight" language="EN" text="feare most the horses"/>
    	<underline method="pen" type="straight" language="EN" text="the Targaettes and swoordes"/>
    	<underline method="pen" type="straight" language="EN" text="backe to the Pikes, and to winne the battaile"/>

    	<underline method="pen" type="straight" language="EN" text="Hovv to arme men"/>
    	<underline method="pen" type="straight" language="EN" text="vveapons to appoint"/>
    	<underline method="pen" type="straight" language="EN" text="Romaine maner"/>
    	<underline method="pen" type="straight" language="EN" text="Dutche fashion"/>
    	
