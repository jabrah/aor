    <annotation>
    	<marginalia hand="Italian" anchor_text="The artillerie of the armie">
    		<language ident="EN">
    			<position place="head" book_orientation="0">
    				<marginalia_text>
    					The Canons, &amp; other Artillery.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="The artillerie of the armie"/>
    	<underline method="pen" type="straight" language="EN" text="suffiseth ten"/>
    	<underline method="pen" type="straight" language="EN" text="Cannons, for the winning"/>
    	<underline method="pen" type="straight" language="EN" text="whose shotte"/>
    	<underline method="pen" type="straight" language="EN" text="passe fiftie pounde"/>
    	<underline method="pen" type="straight" language="EN" text="more for defence of the campe, then"/>
    	<underline method="pen" type="straight" language="EN" text="the battaile"/>
    	<underline method="pen" type="straight" language="EN" text="other artillerie, should"/>
    	<underline method="pen" type="straight" language="EN" text="of ten, then"/>
    	<underline method="pen" type="straight" language="EN" text="fifteene pounde"/>
    	<underline method="pen" type="straight" language="EN" text="afore"/>
    	<underline method="pen" type="straight" language="EN" text="on the front of all the armie, if sometime"/>
    	<underline method="pen" type="straight" language="EN" text="it by the flancke in a sure"/>
    	<underline method="pen" type="straight" language="EN" text="of the enemie be in daunger"/>
    	<underline method="pen" type="straight" language="EN" text="armie thus ordered"/>
    	<underline method="pen" type="straight" language="EN" text="fighting, vse"/>
    	<underline method="pen" type="straight" language="EN" text="the order of"/>
    	<underline method="pen" type="straight" language="EN" text="Falangi"/>
    	<underline method="pen" type="straight" language="EN" text="order of the Romane Legions: for"/>
    	<underline method="pen" type="straight" language="EN" text="the fronte, bee Pikes"/>
    	<underline method="pen" type="straight" language="EN" text="men bee set in"/>
    	<underline method="pen" type="straight" language="EN" text="rankes, after such"/>
    	<underline method="pen" type="straight" language="EN" text="Falangi, restore the firste ranckes, with those"/>
    	<underline method="pen" type="straight" language="EN" text="charged so sore, that they"/>
    	<underline method="pen" type="straight" language="EN" text="constrayned to breake the orders, and to retire themselues"/>
    	<underline method="pen" type="straight" language="EN" text="the voide places of the seconde"/>
    	<underline method="pen" type="straight" language="EN" text="which they haue behinde them, &amp; vnite"/>
    	<underline method="pen" type="straight" language="EN" text="selues with"/>
    	<underline method="pen" type="straight" language="EN" text="making a new force, withstande"/>
    	<underline method="pen" type="straight" language="EN" text="verie same maner, retire"/>
    	<underline method="pen" type="straight" language="EN" text="seconde time, and the third fight"/>
    	<underline method="pen" type="straight" language="EN" text="there is to renue them"/>
    	<underline method="pen" type="straight" language="EN" text="concerning the strength of the armie"/>
    	<underline method="pen" type="straight" language="EN" text="cannot be ordayned a more stronger"/>
    	<underline method="pen" type="straight" language="EN" text="one &amp; the other borne therof"/>
    	<underline method="pen" type="straight" language="EN" text="exceedingly"/>
    	<underline method="pen" type="straight" language="EN" text="replenished, both with heades, and weapons"/>
    	<underline method="pen" type="straight" language="EN" text="remayneth weake, other then the part behinde"/>
    	<underline method="pen" type="straight" language="EN" text="vnarmed, and"/>
    	<underline method="pen" type="straight" language="EN" text="flanckes impaled with the extraordinarie"/>
    	<underline method="pen" type="straight" language="EN" text="anye parte assaulte it"/>
    	<underline method="pen" type="straight" language="EN" text="shall not finde it"/>
    	<underline method="pen" type="straight" language="EN" text="appointed"/>
    	<underline method="pen" type="straight" language="EN" text="the hinder parte can not be assaulted"/>
    	<underline method="pen" type="straight" language="EN" text="bee an enemie, that hath so"/>
    		
    	<underline method="pen" type="straight" language="EN" text="many canons is requisite"/>
    	<underline method="pen" type="straight" language="EN" text="artillerie ought to be"/>
    	<underline method="pen" type="straight" language="EN" text="that vvere ordered"/>
    	<underline method="pen" type="straight" language="EN" text="aboue"/>
    	<underline method="pen" type="straight" language="EN" text="vse the Grekes"/>
    	<underline method="pen" type="straight" language="EN" text="Romane"/>
    		
