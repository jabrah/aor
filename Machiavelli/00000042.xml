    		</language>
    	</marginalia>

	<underline method="pen" type="straight" language="EN" text="armed on the"/>
    	<underline method="pen" type="straight" language="EN" text="warre on the lande, to defende"/>
    	<underline method="pen" type="straight" language="EN" text="sent one of their Citezens"/>
    	<underline method="pen" type="straight" language="EN" text="hired for their Capitayn"/>
    	<underline method="pen" type="straight" language="EN" text="the Marques"/>
    	<underline method="pen" type="straight" language="EN" text="of Mantua"/>
    	<underline method="pen" type="straight" language="EN" text="same foolish acte, whiche"/>
    	<underline method="pen" type="straight" language="EN" text="their legges, from climming"/>
    	<underline method="pen" type="straight" language="EN" text="to make"/>
    	<underline method="pen" type="straight" language="EN" text="selues, to make"/>
    	<underline method="pen" type="straight" language="EN" text="a mistruste not wise"/>
    	<underline method="pen" type="straight" language="EN" text="a Capitayn of the sea"/>
    	<underline method="pen" type="straight" language="EN" text="the windes, with the water, &amp; wth men"/>
    	<underline method="pen" type="straight" language="EN" text="Capitayne of the lande, where"/>
    	<underline method="pen" type="straight" language="EN" text="fighte with"/>
    	<underline method="pen" type="straight" language="EN" text="Capitayne of the lande, to"/>
    	<underline method="pen" type="straight" language="EN" text="on the lande, and on the sea"/>
    	<underline method="pen" type="straight" language="EN" text="were mightie on the sea"/>
    	<underline method="pen" type="straight" language="EN" text="not Grekes, or Spaniardes, accustumed"/>
    	<underline method="pen" type="straight" language="EN" text="same care, to their Citezens"/>
    	<underline method="pen" type="straight" language="EN" text="set on lande"/>
    	<underline method="pen" type="straight" language="EN" text="that one of their Citezens"/>
    	<underline method="pen" type="straight" language="EN" text="become a tiraunt"/>
    	<underline method="pen" type="straight" language="EN" text="if a Citezen with the powers on the"/>
    	<underline method="pen" type="straight" language="EN" text="neuer made a tiraunt in a Citie standing"/>
    	<underline method="pen" type="straight" language="EN" text="with the powers of the"/>
    	<underline method="pen" type="straight" language="EN" text="weapons in the handes of their Citezens"/>
    	<underline method="pen" type="straight" language="EN" text="not make tirauntes"/>
    	<underline method="pen" type="straight" language="EN" text="they tooke therfore"/>
    	<underline method="pen" type="straight" language="EN" text="vnwise way"/>
    	<underline method="pen" type="straight" language="EN" text="which hath b&eacute;en occasion"/>
    	<underline method="pen" type="straight" language="EN" text="them much glorie, and much felicitie"/>

