    <annotation>
    	
    	<marginalia hand="Italian" anchor_text="a Capitayn of the sea">
    		<language ident="EN">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					Captains at sea, soone also very seruiceable at Land.
    				</marginalia_text>
