    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="right_margin" book_orientation="0">
    				<marginalia_text>
    					notabilis victoria, paucorum de multis.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			A notable victory of a few out of many.
    		</translation>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					The power of footemen, surer &amp; stronger,
    					then the power of horses.
    				</marginalia_text>
    			</position>
    		</language>
    		<language ident="LA">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					Christianoru[m] Equites: Turcarum pedites, Valentiores.
    					Jouius in Amurathe 2. Lib. 3. Elog. Bellic.
    				</marginalia_text>
    				<person name="Paolo Giovio"/>
    				<person name="Murad II"/>
    				<book title="Elogia virorum bellica virtute illustrium"/>
    			</position>
    		</language>
    		<translation>
    			The Christians' horsemen: the Turks' infantry, more powerful.
    			Paolo Giovio on Murad II, in Book 3 of Elogia virorum bellica virtute illustrium.
    		</translation>
    	</marginalia>
    	
    	<!-- Underlines -->
    	<underline method="pen" type="straight" language="EN" text="that a power thus"/>
    	<underline method="pen" type="straight" language="EN" text="ouercome at this daye, any other power"/>
    	<underline method="pen" type="straight" language="EN" text="concerning footemen"/>
    	<underline method="pen" type="straight" language="EN" text="concernig horsemen, wee"/>
    	<underline method="pen" type="straight" language="EN" text="more stronger"/>
    	<underline method="pen" type="straight" language="EN" text="ours"/>
    	<underline method="pen" type="straight" language="EN" text="the antiquitie"/>
    	<underline method="pen" type="straight" language="EN" text="Saddelles bolstered, and to the stiroppes"/>
    	<underline method="pen" type="straight" language="EN" text="stande more stronglye on horsebacke"/>
    	<underline method="pen" type="straight" language="EN" text="a bande of men of armes"/>
    	<underline method="pen" type="straight" language="EN" text="paysing very muche, commeth"/>
    	<underline method="pen" type="straight" language="EN" text="more accompt of horses"/>
    	<underline method="pen" type="straight" language="EN" text="haue with the footemen receyued shame"/>
    	<underline method="pen" type="straight" language="EN" text="receyue alwayes, where"/>
    	<underline method="pen" type="straight" language="EN" text="of footemen armed, and ordered, as"/>
    	<underline method="pen" type="straight" language="EN" text="Tigrane king"/>
    	<underline method="pen" type="straight" language="EN" text="CI. thousande horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="armed, like vnto our"/>
    	<underline method="pen" type="straight" language="EN" text="Catafratti"/>
    	<underline method="pen" type="straight" language="EN" text="about sixe thousande"/>
    	<underline method="pen" type="straight" language="EN" text="with xxv. thousand footemen"/>
    	<underline method="pen" type="straight" language="EN" text="be horses enough for an imbassage"/>
    	<underline method="pen" type="straight" language="EN" text="incountering together, he was ouerthrown"/>
    	<underline method="pen" type="straight" language="EN" text="those Catafratti, declaring them"/>
    	<underline method="pen" type="straight" language="EN" text="had their faces couered"/>
    	<underline method="pen" type="straight" language="EN" text="a doe to see, and to offende"/>
    	<underline method="pen" type="straight" language="EN" text="falling, being laden with armour, coulde not"/>
    	<underline method="pen" type="straight" language="EN" text="nor welde themselues"/>
    	<underline method="pen" type="straight" language="EN" text="people or kingdomes"/>
    	<underline method="pen" type="straight" language="EN" text="esteeme more the power of horses"/>
    	<underline method="pen" type="straight" language="EN" text="then the power"/>
    	
    	<underline method="pen" type="straight" language="EN" text="of Lucullo"/>
    	<underline method="pen" type="straight" language="EN" text="Tigrane"/>
    	
