<page filename="00000150.tif" pagination="[lxviii[v]]" reader="Harvey"/>
    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="to be made to"/>
    	<underline method="pen" type="straight" language="EN" text="marching, to obserue all this order"/>
    	<underline method="pen" type="straight" language="EN" text="without"/>
    	<underline method="pen" type="straight" language="EN" text="sure from all the tumultes of the countrie"/>
    	<underline method="pen" type="straight" language="EN" text="other prouision"/>
    	<underline method="pen" type="straight" language="EN" text="the tumultuarye assaultes"/>
    	<underline method="pen" type="straight" language="EN" text="tumultuous people, will come"/>
    	<underline method="pen" type="straight" language="EN" text="of the swoorde, or Pikes poincte"/>
    	<underline method="pen" type="straight" language="EN" text="men out of order, haue feare of those that bee in araye"/>
    	<underline method="pen" type="straight" language="EN" text="with cries and rumours"/>
    	<underline method="pen" type="straight" language="EN" text="make a great assaulte"/>
    	<underline method="pen" type="straight" language="EN" text="like vnto barking curres about"/>
    	<underline method="pen" type="straight" language="EN" text="through all Fraunce, and"/>
    	<underline method="pen" type="straight" language="EN" text="Frenche tumultes, hee tooke small"/>
    	<underline method="pen" type="straight" language="EN" text="haue playners &amp; labourers afore, who"/>
    	<underline method="pen" type="straight" language="EN" text="bee garded of those"/>
    	<underline method="pen" type="straight" language="EN" text="horsemen, that are sent"/>
    	<underline method="pen" type="straight" language="EN" text="viewe the countrie"/>
    	<underline method="pen" type="straight" language="EN" text="order may march"/>
    	<underline method="pen" type="straight" language="EN" text="ten mile the day, and shal"/>
    	<underline method="pen" type="straight" language="EN" text="ynough to incampe"/>
    	<underline method="pen" type="straight" language="EN" text="and suppe before Sunne going downe"/>
    	<underline method="pen" type="straight" language="EN" text="that ordinaryly"/>
    	<underline method="pen" type="straight" language="EN" text="an army may march twentie mile"/>
    	<underline method="pen" type="straight" language="EN" text="set in order"/>
    	<underline method="pen" type="straight" language="EN" text="cannot growe sodainly: for that"/>
    	<underline method="pen" type="straight" language="EN" text="commeth with his pace"/>
    	<underline method="pen" type="straight" language="EN" text="inough, to set thy selfe in"/>
    	<underline method="pen" type="straight" language="EN" text="to fighte"/>
    	
    	<underline method="pen" type="straight" language="EN" text="made plaine"/>
    	<underline method="pen" type="straight" language="EN" text="marche in order"/>
    	<underline method="pen" type="straight" language="EN" text="miles a daie"/>
    	<underline method="pen" type="straight" language="EN" text="marche in battaile raie"/>
    	<underline method="pen" type="straight" language="EN" text="incampe before sun set"/>

    	<!-- Marks -->
    	<mark name="bracket" method="pen" place="left_margin"/>
