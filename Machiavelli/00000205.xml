	<underline method="pen" type="straight" language="EN" text="defende that breache"/>
    	<underline method="pen" type="straight" language="EN" text="abandoned it for to retire him selfe into"/>
    	<underline method="pen" type="straight" language="EN" text="entered into that parte withoute"/>
    	<underline method="pen" type="straight" language="EN" text="sodaine they"/>
    	<underline method="pen" type="straight" language="EN" text="Dukes men"/>
    	<underline method="pen" type="straight" language="EN" text="lordes of the bridges, which"/>
