    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="all the common weales"/>
    	<underline method="pen" type="straight" language="EN" text="left not anye waye to vertue"/>
    	<underline method="pen" type="straight" language="EN" text="that vertuous men began to be"/>
    	<underline method="pen" type="straight" language="EN" text="the vertue being"/>
    	<underline method="pen" type="straight" language="EN" text="[the] same was corrupted"/>
    	<underline method="pen" type="straight" language="EN" text="the worlde came to be corrupted"/>
    	<underline method="pen" type="straight" language="EN" text="Scithian people"/>
    	<underline method="pen" type="straight" language="EN" text="come to spoile the same"/>
    	<underline method="pen" type="straight" language="EN" text="extinguished the vertue of other"/>
    	<underline method="pen" type="straight" language="EN" text="not howe to maintaine their"/>
    	<underline method="pen" type="straight" language="EN" text="same Empire was deuided into"/>
    	<underline method="pen" type="straight" language="EN" text="vertue is not renued"/>
    	<underline method="pen" type="straight" language="EN" text="againe the orders"/>
    	<underline method="pen" type="straight" language="EN" text="because the maner of liuing"/>
    	<underline method="pen" type="straight" language="EN" text="hauing respecte to the Christian Religion"/>
    	<underline method="pen" type="straight" language="EN" text="not the same necessitie to men, to defende them selues"/>
    	<underline method="pen" type="straight" language="EN" text="whiche in olde times was for that"/>
    	<underline method="pen" type="straight" language="EN" text="the men ouercome in warre, either were killed"/>
    	<underline method="pen" type="straight" language="EN" text="remayned perpetuall slaues, where"/>
    	<underline method="pen" type="straight" language="EN" text="The townes"/>
    	<underline method="pen" type="straight" language="EN" text="rased"/>
    	<underline method="pen" type="straight" language="EN" text="inhabiters"/>
    	<underline method="pen" type="straight" language="EN" text="dispersed"/>
    	<underline method="pen" type="straight" language="EN" text="fewe bee killed, none is kept long in prison"/>
    	<underline method="pen" type="straight" language="EN" text="that with facelitie, they are set at libertie"/>
    	<underline method="pen" type="straight" language="EN" text="Cities also, which a thousand times haue rebelled, are not destroyed, the men whereof, are left a lone"/>
    	<underline method="pen" type="straight" language="EN" text="their gods, so that the greatest hurt that is feared"/>
    	<underline method="pen" type="straight" language="EN" text="but a taske"/>
    	<underline method="pen" type="straight" language="EN" text="muche, that men will not submit"/>
    	
