    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="rankes behinde, restored the faultes of those afore"/>
    	<underline method="pen" type="straight" language="EN" text="firste rankes"/>
    	<underline method="pen" type="straight" language="EN" text="consumed the last"/>
    	<underline method="pen" type="straight" language="EN" text="might sooner be consumed"/>
    	<underline method="pen" type="straight" language="EN" text="broken, for that"/>
    	<underline method="pen" type="straight" language="EN" text="the grosse bodye, made it more immooueable"/>
    	<underline method="pen" type="straight" language="EN" text="vsed at the beginning"/>
    	<underline method="pen" type="straight" language="EN" text="the Falangi"/>
    	<underline method="pen" type="straight" language="EN" text="order their Legio[n]s like vnto"/>
    	<underline method="pen" type="straight" language="EN" text="this order pleased them not"/>
    	<underline method="pen" type="straight" language="EN" text="Legions into"/>
    	<underline method="pen" type="straight" language="EN" text="that is, in bandes and companies"/>
    	<underline method="pen" type="straight" language="EN" text="same bodye, should haue"/>
    	<underline method="pen" type="straight" language="EN" text="manye Capitaynes"/>
    	<underline method="pen" type="straight" language="EN" text="made of sundry partes"/>
    	<underline method="pen" type="straight" language="EN" text="by it selfe, might be gouerned"/>
    	<underline method="pen" type="straight" language="EN" text="battailes"/>
    	<underline method="pen" type="straight" language="EN" text="maners of the Falangi, as wel in ordering it grosse"/>
    	<underline method="pen" type="straight" language="EN" text="in rescuing the one the other"/>
    	<underline method="pen" type="straight" language="EN" text="the maine battailes, the one to"/>
    	<underline method="pen" type="straight" language="EN" text="of the other"/>
    	<underline method="pen" type="straight" language="EN" text="[the] the first retiring"/>
    	<underline method="pen" type="straight" language="EN" text="maye bee receyued of [the] second"/>
    	<underline method="pen" type="straight" language="EN" text="to succour the one [the] other"/>
    	<underline method="pen" type="straight" language="EN" text="battaile before"/>
    	<underline method="pen" type="straight" language="EN" text="the same on the"/>
    	<underline method="pen" type="straight" language="EN" text="make forewarde, &amp; succour it"/>
    	<underline method="pen" type="straight" language="EN" text="thirde maine battaile, they"/>
    	<underline method="pen" type="straight" language="EN" text="but distant"/>
    	<underline method="pen" type="straight" language="EN" text="twoo maine battailes being"/>
    	<underline method="pen" type="straight" language="EN" text="that marcheth"/>
    	<underline method="pen" type="straight" language="EN" text="auoide the iustling of the one"/>
    	<underline method="pen" type="straight" language="EN" text="grosse multitude, cannot bee receuyed"/>
    	<underline method="pen" type="straight" language="EN" text="litle bodies being"/>
    	
    	<underline method="pen" type="straight" language="EN" text="Suizzers vse in their maine"/>

	<!-- Symbols -->
