    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian" anchor_text="a citie thus builded, to be altogether inuincible">
    		<language ident="EN">
    			<position place="intext" book_orientation="0">
    				<marginalia_text>
    					A City altogither inuincible.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="is most difficulte"/>
    	<underline method="pen" type="straight" language="EN" text="the capacitie"/>
    	<underline method="pen" type="straight" language="EN" text="difficultie, that is in"/>
    	<underline method="pen" type="straight" language="EN" text="comming neere it, the wall being strong and concaued"/>
    	<underline method="pen" type="straight" language="EN" text="after to go vp a breache"/>
    	<underline method="pen" type="straight" language="EN" text="ruine, which giueth thee most great"/>
    	<underline method="pen" type="straight" language="EN" text="a citie thus builded, to be altogether inuincible"/>
    	<underline method="pen" type="straight" language="EN" text="ditche within"/>
    	<underline method="pen" type="straight" language="EN" text="also withoute, shoulde"/>
    	<underline method="pen" type="straight" language="EN" text="bee without doubte"/>
    	<underline method="pen" type="straight" language="EN" text="make one ditche"/>
    	<underline method="pen" type="straight" language="EN" text="bettter within then without"/>
    	<underline method="pen" type="straight" language="EN" text="that water shoulde"/>
    	<underline method="pen" type="straight" language="EN" text="herein bee diuers"/>
    	<underline method="pen" type="straight" language="EN" text="ditches full of water"/>
    	<underline method="pen" type="straight" language="EN" text="thee from mines"/>
    	<underline method="pen" type="straight" language="EN" text="the Ditches without water"/>
    	<underline method="pen" type="straight" language="EN" text="more difficulte the filling of them"/>
    	<underline method="pen" type="straight" language="EN" text="them without water"/>
    	<underline method="pen" type="straight" language="EN" text="For ditches with water, haue been"/>
    	<underline method="pen" type="straight" language="EN" text="the Winter to be frosen"/>
    	<underline method="pen" type="straight" language="EN" text="make easy the winning"/>
    	<underline method="pen" type="straight" language="EN" text="to Mirandola, when Pope"/>
    	<underline method="pen" type="straight" language="EN" text="from mines"/>
    	<underline method="pen" type="straight" language="EN" text="make it so deepe, that he"/>
    	<underline method="pen" type="straight" language="EN" text="digge lower, should"/>
    	<underline method="pen" type="straight" language="EN" text="Fortresses also"/>
    	<underline method="pen" type="straight" language="EN" text="haue the like difficultie"/>
    	<underline method="pen" type="straight" language="EN" text="One thing I will earnestly"/>
    	<underline method="pen" type="straight" language="EN" text="defendeth a citie: and that"/>
    	<underline method="pen" type="straight" language="EN" text="that he make no Bulwarkes without"/>
    	<underline method="pen" type="straight" language="EN" text="buildeth the Fortresse"/>
    	<underline method="pen" type="straight" language="EN" text="not anye refuge place in them"/>
    	<underline method="pen" type="straight" language="EN" text="the firste wall being loste, maye retire"/>
    	
    	<underline method="pen" type="straight" language="EN" text="A drie diche is moste"/>
    	<underline method="pen" type="straight" language="EN" text="building &amp; defending of a Tourne"/>
    	
