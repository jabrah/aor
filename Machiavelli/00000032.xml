    	<underline method="pen" type="straight" language="EN" text="willingly for his loue"/>
    	<underline method="pen" type="straight" language="EN" text="the peace co[m]meth after, more"/>
    	<underline method="pen" type="straight" language="EN" text="their Prince do"/>
    	<underline method="pen" type="straight" language="EN" text="gentilmen to the tending"/>
    	<underline method="pen" type="straight" language="EN" text="common souldiours"/>
