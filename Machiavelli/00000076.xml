<page filename="00000076.tif" pagination="xxxi[v]" reader="Harvey" signature="[Hiii[v]]"/>
    <annotation>
    	<!-- Underlines -->
    	<underline method="pen" type="straight" language="EN" text="I maye retire"/>
    	<underline method="pen" type="straight" language="EN" text="to succeede in my"/>
    	<underline method="pen" type="straight" language="EN" text="alwayes fight with boldnesse"/>
    	<underline method="pen" type="straight" language="EN" text="my succour at hande"/>
    	<underline method="pen" type="straight" language="EN" text="not be afraide, for"/>
    	<underline method="pen" type="straight" language="EN" text="the same, which may giue the victory to my"/>
    	<underline method="pen" type="straight" language="EN" text="is made of newe"/>
    	<underline method="pen" type="straight" language="EN" text="olde armie is, they be also"/>
    	<underline method="pen" type="straight" language="EN" text="newe from their infanrie, the order of their armies"/>
    	<underline method="pen" type="straight" language="EN" text="continually did"/>
    	<underline method="pen" type="straight" language="EN" text="continuall exercises"/>
    	<underline method="pen" type="straight" language="EN" text="all the same multitude"/>
    	<underline method="pen" type="straight" language="EN" text="in the day of battaile"/>
    	<underline method="pen" type="straight" language="EN" text="profitable"/>
    	<underline method="pen" type="straight" language="EN" text="because they all knewe, howe to"/>
    	<underline method="pen" type="straight" language="EN" text="the orders, and to fight keeping the same"/>
    	<underline method="pen" type="straight" language="EN" text="armies of newe men"/>
    	<underline method="pen" type="straight" language="EN" text="together, to fighte straighte"/>
    	<underline method="pen" type="straight" language="EN" text="power to fighte, when neede"/>
    	<underline method="pen" type="straight" language="EN" text="without these exercises, as"/>
    	<underline method="pen" type="straight" language="EN" text="is made nothing"/>
    	<underline method="pen" type="straight" language="EN" text="orders being"/>
    	<underline method="pen" type="straight" language="EN" text="with double"/>
    	<underline method="pen" type="straight" language="EN" text="industrie and labour"/>
    	<underline method="pen" type="straight" language="EN" text="shewe them"/>
    	<underline method="pen" type="straight" language="EN" text="Capitaynes haue trauailed, without"/>
    	<underline method="pen" type="straight" language="EN" text="the wayes, with the which"/>
    	<underline method="pen" type="straight" language="EN" text="affection, which I beare to these"/>
    	<underline method="pen" type="straight" language="EN" text="seeing they be not put"/>
    		
