    	<!-- Underlines -->
    	<underline method="pen" type="straight" language="EN" text="such wise, that they might receiue betweene"/>
    	<underline method="pen" type="straight" language="EN" text="Suizzers not to be so"/>
    	<underline method="pen" type="straight" language="EN" text="fought with the Grekes Falangi"/>
    	<underline method="pen" type="straight" language="EN" text="were consumed of them"/>
    	<underline method="pen" type="straight" language="EN" text="that the kinde"/>
    	<underline method="pen" type="straight" language="EN" text="their weapons"/>
