    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					Ca[e]sars
    					</marginalia_text>
    				<person name="Julius Caesar"/>
    				</position>
    			</language>
    		<language ident="LA">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					quid pro quo:
    				</marginalia_text>
    			</position>
    		</language>
    		<language ident="EN">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					[the] worse place, [the] better munition.
    				</marginalia_text>
    			</position>
    		</language>
    		<language ident="LA">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					supplemendu[m] Arte, quod
    				</marginalia_text>
    			</position>
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					deest natur&acirc;. A[e]quipollentium jdem effectus.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Caesar's something for something:
    			the worst place, the better munition.
    			What nature is lacking must be supplied by skill. The same effect of equivalents.
    		</translation>
    	</marginalia>
    	
