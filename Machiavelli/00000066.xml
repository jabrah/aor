    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="that a hardye man"/>
    	<underline method="pen" type="straight" language="EN" text="vpon a vile horse"/>
    	<underline method="pen" type="straight" language="EN" text="coward vpon a good"/>
    	<underline method="pen" type="straight" language="EN" text="euil matching of stomackes"/>
    	<underline method="pen" type="straight" language="EN" text="that a bande"/>
    	<underline method="pen" type="straight" language="EN" text="footemen, susteyneth all violence of horses"/>
    	<underline method="pen" type="straight" language="EN" text="horse is a beast, that hath sense"/>
    	<underline method="pen" type="straight" language="EN" text="perils, and with an ill will, will"/>
    	<underline method="pen" type="straight" language="EN" text="what force maketh"/>
    	<underline method="pen" type="straight" language="EN" text="be greater, whiche keepeth"/>
    	<underline method="pen" type="straight" language="EN" text="spurre maketh them"/>
    	<underline method="pen" type="straight" language="EN" text="swoorde, or the Pike, keepeth"/>
    	<underline method="pen" type="straight" language="EN" text="bande of footemen to bee most safe"/>
    	<underline method="pen" type="straight" language="EN" text="inuinsible for horses"/>
    	<underline method="pen" type="straight" language="EN" text="heate, with"/>
    	<underline method="pen" type="straight" language="EN" text="furious to incounter, who that woulde"/>
    	<underline method="pen" type="straight" language="EN" text="vpon the poincte of the Pike, either"/>
    	<underline method="pen" type="straight" language="EN" text="them, he will tourne on the right"/>
