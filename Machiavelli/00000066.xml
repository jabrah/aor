    	<underline method="pen" type="straight" language="EN" text="euer furie he come withall, will strike against"/>
    	<underline method="pen" type="straight" language="EN" text="to fight with the Suizzers"/>
    	<underline method="pen" type="straight" language="EN" text="alighted, and made euery man alight on foote"/>
    	<underline method="pen" type="straight" language="EN" text="from the arayes, the horses"/>
    	<underline method="pen" type="straight" language="EN" text="flie"/>
    	<underline method="pen" type="straight" language="EN" text="these naturall"/>
    	<underline method="pen" type="straight" language="EN" text="impedimentes, which"/>
    	<underline method="pen" type="straight" language="EN" text="wayes, which haue for horse the moste"/>
    	<underline method="pen" type="straight" language="EN" text="maye be"/>
    	
    	<underline method="pen" type="straight" language="EN" text="men maie"/>
    	<underline method="pen" type="straight" language="EN" text="themselues from horsemen"/>

    	
