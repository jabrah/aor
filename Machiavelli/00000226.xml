    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="left_margin" book_orientation="0">
    				<marginalia_text>
    					What dominion, &amp; autority requisite, to
    					leuy, train, &amp; refourm an Army.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="make good and"/>
    	<underline method="pen" type="straight" language="EN" text="deserue muche more praise"/>
    	<underline method="pen" type="straight" language="EN" text="with old armies"/>
    	<underline method="pen" type="straight" language="EN" text="were Pelopida"/>
    	<underline method="pen" type="straight" language="EN" text="Epaminonda"/>
    	<underline method="pen" type="straight" language="EN" text="Tullus Hostillius"/>
    	<underline method="pen" type="straight" language="EN" text="Philip of"/>
    	<underline method="pen" type="straight" language="EN" text="Cirus king"/>
    	<underline method="pen" type="straight" language="EN" text="Graccus a Romane"/>
    	<underline method="pen" type="straight" language="EN" text="driuen first to"/>
    	<underline method="pen" type="straight" language="EN" text="armies good, and"/>
    	<underline method="pen" type="straight" language="EN" text="fighte with them"/>
    	<underline method="pen" type="straight" language="EN" text="as well through their prudence"/>
    	<underline method="pen" type="straight" language="EN" text="hauing subiectes whome"/>
    	<underline method="pen" type="straight" language="EN" text="otherwise possible"/>
    	<underline method="pen" type="straight" language="EN" text="good and full of all excellencie"/>
    	<underline method="pen" type="straight" language="EN" text="in a strau[n]ge countrie, full of men corrupted, not vsed"/>
    	<underline method="pen" type="straight" language="EN" text="any honest obedience"/>
    	<underline method="pen" type="straight" language="EN" text="brought to passe"/>
    	<underline method="pen" type="straight" language="EN" text="how to gouerne"/>
    	<underline method="pen" type="straight" language="EN" text="to make it, and after"/>
    	<underline method="pen" type="straight" language="EN" text="commaunde"/>
    	<underline method="pen" type="straight" language="EN" text="bee those Princes"/>
    	<underline method="pen" type="straight" language="EN" text="subiectes ynough, may haue"/>
    	<underline method="pen" type="straight" language="EN" text="commaunded, nor"/>
    	<underline method="pen" type="straight" language="EN" text="commaunde, but to armies of straungers"/>
    	<underline method="pen" type="straight" language="EN" text="bounde to other, and not"/>
    	<underline method="pen" type="straight" language="EN" text="possibile, or no, to introduce any"/>
    	<underline method="pen" type="straight" language="EN" text="when coulde I make one"/>
    	<underline method="pen" type="straight" language="EN" text="souldiours"/>
    	<underline method="pen" type="straight" language="EN" text="weare more"/>
    	<underline method="pen" type="straight" language="EN" text="armour then"/>
    	<underline method="pen" type="straight" language="EN" text="beare their owne meate for twoo or three"/>
    	<underline method="pen" type="straight" language="EN" text="mattocke"/>
    	<underline method="pen" type="straight" language="EN" text="them to digge"/>
    	<underline method="pen" type="straight" language="EN" text="many howers armed, in fained"/>
    	<underline method="pen" type="straight" language="EN" text="in the very thing in deede"/>
    	<underline method="pen" type="straight" language="EN" text="they abstayne from playe"/>
  
