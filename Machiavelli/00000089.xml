    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="Ansigne bearers, for"/>
    	<underline method="pen" type="straight" language="EN" text="faire muster, then for any"/>
    	<underline method="pen" type="straight" language="EN" text="them for guides"/>
    	<underline method="pen" type="straight" language="EN" text="themselues againe into order"/>
    	<underline method="pen" type="straight" language="EN" text="Ansigne stoode still"/>
    	<underline method="pen" type="straight" language="EN" text="knewe the place, that"/>
    	<underline method="pen" type="straight" language="EN" text="the same moouing"/>
    	<underline method="pen" type="straight" language="EN" text="should staie, or mooue"/>
    	<underline method="pen" type="straight" language="EN" text="be many bodies"/>
    	<underline method="pen" type="straight" language="EN" text="bande of men to haue his Ansigne, and his guide"/>
    	<underline method="pen" type="straight" language="EN" text="stomackes"/>
    	<underline method="pen" type="straight" language="EN" text="life enough"/>
    	<underline method="pen" type="straight" language="EN" text="march according to the Ansigne"/>
    	<underline method="pen" type="straight" language="EN" text="mooue, according to the Drumme"/>
    	<underline method="pen" type="straight" language="EN" text="well ordered, commaundeth to"/>
    	<underline method="pen" type="straight" language="EN" text="easyly thorders"/>
    	<underline method="pen" type="straight" language="EN" text="had Shalmes Flutes, and soundes perfectly"/>
    	<underline method="pen" type="straight" language="EN" text="that danseth"/>
    	<underline method="pen" type="straight" language="EN" text="proceadeth with the time of"/>
    	<underline method="pen" type="straight" language="EN" text="doeth not erre"/>
    	<underline method="pen" type="straight" language="EN" text="doeth not disorder"/>
    	<underline method="pen" type="straight" language="EN" text="varied the sounde"/>
    	<underline method="pen" type="straight" language="EN" text="varye the mocion"/>
    	<underline method="pen" type="straight" language="EN" text="would inflame, or quiete, or staie the mindes"/>
    	<underline method="pen" type="straight" language="EN" text="sounde Dorico"/>
    	<underline method="pen" type="straight" language="EN" text="sounde Frigio"/>
    	<underline method="pen" type="straight" language="EN" text="Alexander being at the Table"/>
    	<underline method="pen" type="straight" language="EN" text="it kindled so muche"/>
    	<underline method="pen" type="straight" language="EN" text="necesarie to finde"/>
    	<underline method="pen" type="straight" language="EN" text="that teache the Souldiour to obeye"/>
    	<underline method="pen" type="straight" language="EN" text="vary, and ordayne after"/>
    	<underline method="pen" type="straight" language="EN" text="practise, bee accustome the eares"/>
    	
    	<underline method="pen" type="straight" language="EN" text="Ansignes ought"/>
    	<underline method="pen" type="straight" language="EN" text="Drummes"/>
    	<underline method="pen" type="straight" language="EN" text="sou[n]des of instrumentes"/>
    	
