    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="antiquitie in harde things, then in the delicate"/>
    	<underline method="pen" type="straight" language="EN" text="much detested the liuing in ease"/>
    	<underline method="pen" type="straight" language="EN" text="a louer of the same hardenesse of"/>
    	<underline method="pen" type="straight" language="EN" text="bee able in person, nor in those of his sonnes"/>
    	<underline method="pen" type="straight" language="EN" text="in the hottest day of Sommer being naked"/>
    	<underline method="pen" type="straight" language="EN" text="himselfe upon the Sande; or"/>
    	<underline method="pen" type="straight" language="EN" text="Winter in the"/>
    	<underline method="pen" type="straight" language="EN" text="coldest monethes upon the snowe"/>
    	<underline method="pen" type="straight" language="EN" text="nourishe"/>
    	<underline method="pen" type="straight" language="EN" text="them to sleepe in the open ayre,"/>
    	<underline method="pen" type="straight" language="EN" text="feete naked"/>
    	<underline method="pen" type="straight" language="EN" text="washe them selues in the colde"/>
