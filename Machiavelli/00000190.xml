    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="head" book_orientation="0">
    				<marginalia_text>
    					De Secretis non reuelandis.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Of not revealing secrets.
    		</translation>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="doubtest, to hide"/>
    	<underline method="pen" type="straight" language="EN" text="some enterprise in hande"/>
    	<underline method="pen" type="straight" language="EN" text="easyly thou"/>
    	<underline method="pen" type="straight" language="EN" text="beguile and oppresse him"/>
    	<underline method="pen" type="straight" language="EN" text="to deminishe thy"/>
    	<underline method="pen" type="straight" language="EN" text="not to deminish"/>
    	<underline method="pen" type="straight" language="EN" text="to maintaine the signes, &amp; the orders"/>
    	<underline method="pen" type="straight" language="EN" text="the very same fires, and"/>
    	<underline method="pen" type="straight" language="EN" text="thou werte ingrossed"/>
    	<underline method="pen" type="straight" language="EN" text="not to increase"/>
    	<underline method="pen" type="straight" language="EN" text="the lodginges"/>
    	<underline method="pen" type="straight" language="EN" text="keeping secrete"/>
    	<underline method="pen" type="straight" language="EN" text="alwaies beene most profitable"/>
    	<underline method="pen" type="straight" language="EN" text="woulde doe"/>
    	<underline method="pen" type="straight" language="EN" text="his sherte knew therof, he"/>
    	<underline method="pen" type="straight" language="EN" text="beeluest thou to"/>
    	<underline method="pen" type="straight" language="EN" text="alone not to here the trumpet"/>
    	<underline method="pen" type="straight" language="EN" text="the secretes of thy enemie"/>
    	<underline method="pen" type="straight" language="EN" text="sende ambassadours, and"/>
    	<underline method="pen" type="straight" language="EN" text="seruauntes apparel, msote expertest"/>
    	<underline method="pen" type="straight" language="EN" text="taken occasion to see the"/>
    	<underline method="pen" type="straight" language="EN" text="consider his strength"/>
    	<underline method="pen" type="straight" language="EN" text="and weakenesse"/>
    	<underline method="pen" type="straight" language="EN" text="opportunitie to ouercome"/>
    	<underline method="pen" type="straight" language="EN" text="into exile one of their familiars"/>
    	<underline method="pen" type="straight" language="EN" text="knowen the deuises of his"/>
    	<underline method="pen" type="straight" language="EN" text="like secretes are"/>
    	<underline method="pen" type="straight" language="EN" text="when for this effecte"/>
    	<underline method="pen" type="straight" language="EN" text="were taken any prisoners"/>
    	<underline method="pen" type="straight" language="EN" text="the fayeth of those Frenchmen"/>
    	<underline method="pen" type="straight" language="EN" text="Lombardie, and were in"/>
    	<underline method="pen" type="straight" language="EN" text="them letters open"/>
    	<underline method="pen" type="straight" language="EN" text="sealed"/>
    	
    	<underline method="pen" type="straight" language="EN" text="enemie shal not perceiue"/>
    	<underline method="pen" type="straight" language="EN" text="same be deminished, or increased"/>
    	<underline method="pen" type="straight" language="EN" text="secretes of thy enemie"/>
    	<underline method="pen" type="straight" language="EN" text="hovv he might trust the Frenchmen"/>

