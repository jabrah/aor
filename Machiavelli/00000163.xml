    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="right_margin" book_orientation="0">
    				<marginalia_text>
    					Ironia Militaris.
    				</marginalia_text>
    			</position>
    		</language>
    		<translation>
    			Military irony.
    		</translation>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="to geue tyme to them to fyght wyth hym"/>
    	<underline method="pen" type="straight" language="EN" text="fained, that he would lodge there"/>
    	<underline method="pen" type="straight" language="EN" text="caused trenches to be made, and"/>
    	<underline method="pen" type="straight" language="EN" text="pauilions to be erected"/>
    	<underline method="pen" type="straight" language="EN" text="certayne horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="countrey for forredge"/>
    	<underline method="pen" type="straight" language="EN" text="that he incamped"/>
    	<underline method="pen" type="straight" language="EN" text="also incamped, and"/>
    	<underline method="pen" type="straight" language="EN" text="partes, to prouyde"/>
    	<underline method="pen" type="straight" language="EN" text="beyng aware, passed the riuer"/>
    	<underline method="pen" type="straight" language="EN" text="hauyng no brydge, haue"/>
    	<underline method="pen" type="straight" language="EN" text="deuyded it, and"/>
    	<underline method="pen" type="straight" language="EN" text="turned behynde theyr backes"/>
    	<underline method="pen" type="straight" language="EN" text="becomynge shallower, wyth"/>
    	<underline method="pen" type="straight" language="EN" text="bee swyft, purposyng"/>
    	<underline method="pen" type="straight" language="EN" text="men to passe"/>
    	<underline method="pen" type="straight" language="EN" text="theyr strongest horses on the higher syde, [that] they may"/>
    	<underline method="pen" type="straight" language="EN" text="parte be lower [that] may succour the men, yf any"/>
    	<underline method="pen" type="straight" language="EN" text="[that] bee verie deepe"/>
    	<underline method="pen" type="straight" language="EN" text="bridges"/>
    	<underline method="pen" type="straight" language="EN" text="botes"/>
    	<underline method="pen" type="straight" language="EN" text="wyth barrelles"/>
    	<underline method="pen" type="straight" language="EN" text="armie wherewyth to be able"/>
    	<underline method="pen" type="straight" language="EN" text="make all these"/>
    	<underline method="pen" type="straight" language="EN" text="standing agaynst"/>
    	<underline method="pen" type="straight" language="EN" text="other banke"/>
    	<underline method="pen" type="straight" language="EN" text="of Cesar"/>
    	<underline method="pen" type="straight" language="EN" text="banke of"/>
    	<underline method="pen" type="straight" language="EN" text="passage beyng"/>
    	<underline method="pen" type="straight" language="EN" text="Vergintorige"/>
    	<underline method="pen" type="straight" language="EN" text="Frenche man, the"/>
    	<underline method="pen" type="straight" language="EN" text="dayes a longe the riuer, and the"/>
    	<underline method="pen" type="straight" language="EN" text="incamping in a woddie place, apte to hide men"/>
    	<underline method="pen" type="straight" language="EN" text="legion three cohortes"/>
    	<underline method="pen" type="straight" language="EN" text="them to tarye in the"/>
    	<underline method="pen" type="straight" language="EN" text="soone as he was departed, they"/>
    	<underline method="pen" type="straight" language="EN" text="shoulde caste ouer a bridge, and should"/>
    	
    	<underline method="pen" type="straight" language="EN" text="policie to passe ouer"/>
    	<underline method="pen" type="straight" language="EN" text="riuer"/>
    	<underline method="pen" type="straight" language="EN" text="vvithout a bridge"/>
    	<underline method="pen" type="straight" language="EN" text="A policie of Cesar to passe a riuer"/>

