    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="the Hastati"/>
    	<underline method="pen" type="straight" language="EN" text="retired by a litle, and litle, by the"/>
    	<underline method="pen" type="straight" language="EN" text="together with"/>
    	<underline method="pen" type="straight" language="EN" text="al in the rarenesse of the orders of the"/>
    	<underline method="pen" type="straight" language="EN" text="altogether on a heape began againe the fight"/>
    	<underline method="pen" type="straight" language="EN" text="was no more remedy"/>
    	<underline method="pen" type="straight" language="EN" text="remained no more waies to renue"/>
    	<underline method="pen" type="straight" language="EN" text="The horses stoode on"/>
    	<underline method="pen" type="straight" language="EN" text="likenesse of two winges to a body"/>
    	<underline method="pen" type="straight" language="EN" text="with [the] enemies horses"/>
    	<underline method="pen" type="straight" language="EN" text="they rescued [the] fotemen"/>
    	<underline method="pen" type="straight" language="EN" text="of renuing the[m] selues three times, is almost"/>
    	<underline method="pen" type="straight" language="EN" text="to ouercome"/>
    	<underline method="pen" type="straight" language="EN" text="fortune must three times forsake"/>
    	<underline method="pen" type="straight" language="EN" text="to haue so much strength, that"/>
    	<underline method="pen" type="straight" language="EN" text="times he may ouercome"/>
    	<underline method="pen" type="straight" language="EN" text="not"/>
    	<underline method="pen" type="straight" language="EN" text="maner of renuing the[m] selues"/>
    	<underline method="pen" type="straight" language="EN" text="in rescuing the one the other"/>
    	<underline method="pen" type="straight" language="EN" text="not to retire the one order within the other, as the"/>
    	<underline method="pen" type="straight" language="EN" text="to enter the one man into the place"/>
    	<underline method="pen" type="straight" language="EN" text="foremoste sixe, mighte"/>
    	<underline method="pen" type="straight" language="EN" text="their Launces, the whiche"/>
    	<underline method="pen" type="straight" language="EN" text="Sarisse, were so long"/>
    	<underline method="pen" type="straight" language="EN" text="woundes fell"/>
    	<underline method="pen" type="straight" language="EN" text="the same man"/>
    	<underline method="pen" type="straight" language="EN" text="that was behinde in the seconde ranke, and in"/>
    	<underline method="pen" type="straight" language="EN" text="thirde, and thus"/>
    	
