    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="power of the Frenchemen, of [the] Tusca[n]s"/>
    	<underline method="pen" type="straight" language="EN" text="Umbries and Sannites"/>
    	<underline method="pen" type="straight" language="EN" text="a fewe men"/>
    	<underline method="pen" type="straight" language="EN" text="noised through all his armye, that"/>
    	<underline method="pen" type="straight" language="EN" text="daye to fighte the fielde with"/>
    	<underline method="pen" type="straight" language="EN" text="that certayne of the prisoners"/>
    	<underline method="pen" type="straight" language="EN" text="afore, had occasion to runne"/>
    	<underline method="pen" type="straight" language="EN" text="taken to fighte"/>
    	<underline method="pen" type="straight" language="EN" text="nexte"/>
    	<underline method="pen" type="straight" language="EN" text="afrayde to deminish"/>
    	<underline method="pen" type="straight" language="EN" text="serueth not"/>
    	<underline method="pen" type="straight" language="EN" text="deuide the force"/>
    	<underline method="pen" type="straight" language="EN" text="but to augmente a mans"/>
    	<underline method="pen" type="straight" language="EN" text="to deuide the enemies"/>
    	<underline method="pen" type="straight" language="EN" text="him to enter into their countrie"/>
    	<underline method="pen" type="straight" language="EN" text="him take many townes"/>
    	<underline method="pen" type="straight" language="EN" text="same garnisons"/>
    	<underline method="pen" type="straight" language="EN" text="might thereby diminishe"/>
    	<underline method="pen" type="straight" language="EN" text="this waye hauing made him"/>
    	<underline method="pen" type="straight" language="EN" text="assaulted and ouercome"/>
    	<underline method="pen" type="straight" language="EN" text="into one prouince"/>
    	<underline method="pen" type="straight" language="EN" text="woulde haue inuaded an other"/>
    	<underline method="pen" type="straight" language="EN" text="diligence, that sodainlye entring"/>
    	<underline method="pen" type="straight" language="EN" text="was not doubted that they woulde"/>
    	<underline method="pen" type="straight" language="EN" text="firste wonne it: before the enemie coulde haue"/>
    	<underline method="pen" type="straight" language="EN" text="not sure, whether"/>
    	<underline method="pen" type="straight" language="EN" text="tourne backe, to the"/>
    	<underline method="pen" type="straight" language="EN" text="not to forsake"/>
    	<underline method="pen" type="straight" language="EN" text="place, to succour the other"/>
    	<underline method="pen" type="straight" language="EN" text="defendeth neither the one"/>
    	<underline method="pen" type="straight" language="EN" text="nor the other"/>
    	<underline method="pen" type="straight" language="EN" text="growe sedition or discorde"/>
    	<underline method="pen" type="straight" language="EN" text="arte howe to extinguishe"/>
    	<underline method="pen" type="straight" language="EN" text="the heades of"/>
    	<underline method="pen" type="straight" language="EN" text="mayest first haue oppressed"/>
    	
    	<underline method="pen" type="straight" language="EN" text="staied his enemies"/>
    	<underline method="pen" type="straight" language="EN" text="to incou[n]ter a legion of"/>
    	<underline method="pen" type="straight" language="EN" text="to deuide his force"/>
    	<underline method="pen" type="straight" language="EN" text="enemies countrie"/>
    	<underline method="pen" type="straight" language="EN" text="he be avvare"/>
    	<underline method="pen" type="straight" language="EN" text="reforme sedicion and"/>
    	
