    	<underline method="pen" type="straight" language="EN" text="Romaines did besiege"/>
    	<underline method="pen" type="straight" language="EN" text="the qualitie of this army, and"/>
    	<underline method="pen" type="straight" language="EN" text="then certaine general rules"/>
    	<underline method="pen" type="straight" language="EN" text="you should haue moste familier"/>
