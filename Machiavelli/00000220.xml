    	<underline method="pen" type="straight" language="EN" text="incurre least perrills"/>
    	<underline method="pen" type="straight" language="EN" text="most of the victorie"/>
    	<underline method="pen" type="straight" language="EN" text="conducte thy men"/>
    	<underline method="pen" type="straight" language="EN" text="first"/>
    	<underline method="pen" type="straight" language="EN" text="not confirmed theyr mindes &amp; knowest them to"/>
    	<underline method="pen" type="straight" language="EN" text="without feare, &amp; to be in good order"/>
    	<underline method="pen" type="straight" language="EN" text="neuer to enterprise any daungerous thing"/>
    	<underline method="pen" type="straight" language="EN" text="thou seest, that they hope to ouercome"/>
    	
    	<underline method="pen" type="straight" language="EN" text="Honor got"/>
    	<underline method="pen" type="straight" language="EN" text="consta[n]cie"/>
    	<underline method="pen" type="straight" language="EN" text="Rules of vvarre."/>
    	
