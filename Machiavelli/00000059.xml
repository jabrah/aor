    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="pain to carry them"/>
    	<underline method="pen" type="straight" language="EN" text="with accustoming the bodye to diseases"/>
    	<underline method="pen" type="straight" language="EN" text="hardening it, to be able"/>
    	<underline method="pen" type="straight" language="EN" text="in thinges accustomed"/>
    	<underline method="pen" type="straight" language="EN" text="suffer no griefe"/>
    	<underline method="pen" type="straight" language="EN" text="co[n]straynd, to fight with footemen"/>
    	<underline method="pen" type="straight" language="EN" text="and with horse"/>
    	<underline method="pen" type="straight" language="EN" text="cannot either sustain the horses"/>
    	<underline method="pen" type="straight" language="EN" text="notwithstanding neede to feare the footemen, which be better armed"/>
    	<underline method="pen" type="straight" language="EN" text="in the Dutchemen"/>
    	<underline method="pen" type="straight" language="EN" text="to ouercome the horses"/>
    	<underline method="pen" type="straight" language="EN" text="disaduantage, when they fight with men"/>
    	<underline method="pen" type="straight" language="EN" text="ordeyned"/>
    	<underline method="pen" type="straight" language="EN" text="armed as"/>
    	<underline method="pen" type="straight" language="EN" text="Romanes could ouercome the men"/>
    	<underline method="pen" type="straight" language="EN" text="the horses"/>
    	<underline method="pen" type="straight" language="EN" text="Dutchemen onely the horses"/>
    	<underline method="pen" type="straight" language="EN" text="the Romane footemen"/>
    	<underline method="pen" type="straight" language="EN" text="innumerable horses"/>
    	<underline method="pen" type="straight" language="EN" text="haue beene ouercome of men on foote"/>
    	<underline method="pen" type="straight" language="EN" text="defaulte that"/>
    	<underline method="pen" type="straight" language="EN" text="vantage that the enemie hath had in"/>
    	<underline method="pen" type="straight" language="EN" text="defaulte"/>
    	<underline method="pen" type="straight" language="EN" text="the one of these twoo thinges"/>
    	<underline method="pen" type="straight" language="EN" text="suche, as shoulde arme them better"/>
    	<underline method="pen" type="straight" language="EN" text="haue taken the straungers maners, and shoulde haue lefte their owne"/>
    		

