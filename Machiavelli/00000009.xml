    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian">
    		<language ident="LA">
    			<position place="head" book_orientation="0">
    				<marginalia_text>
    					Imbelles Dama[e], quid nisi pra[e]da sumus?
    				</marginalia_text>
    				<X-ref person="Martial" book_title="Epigrams"/>
    			</position>
    		</language>
    		<translation>
    			Helpless deer, what are we but prey?
    		</translation>
    	</marginalia>

    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="he feleth himselfe naturally"/>
    	<underline method="pen" type="straight" language="EN" text="aduaunceme[n]t of a kingdome, or Common"/>
    	<underline method="pen" type="straight" language="EN" text="no one thing to be"/>
    	<underline method="pen" type="straight" language="EN" text="profitable, necessarye, or more honourable"/>
    	<underline method="pen" type="straight" language="EN" text="knowledge of seruice in warre, and deedes of armes;"/>
    	<underline method="pen" type="straight" language="EN" text="ambicion of the worlde"/>
    	<underline method="pen" type="straight" language="EN" text="the defence of the swoorde is not alwayes"/>
    	<underline method="pen" type="straight" language="EN" text="Greekes, being occupied about trifling matters"/>
    	<underline method="pen" type="straight" language="EN" text="pleasure in reciting of Comedies,"/>
    	<underline method="pen" type="straight" language="EN" text="neglecting Martial feates"/>
    	<underline method="pen" type="straight" language="EN" text="to Philip king of Macedonia, father of Alexander"/>
    	<underline method="pen" type="straight" language="EN" text="Great, to oppresse &amp; to bring them in seruitude"/>
    	    	
