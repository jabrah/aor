        <underline method="pen" type="straight" language="LA" text="quatuor cantredorum"/>
        <underline method="pen" type="straight" language="LA" text="Regi &amp; suis haeredibus cum"/>
        <underline method="pen" type="straight" language="LA" text="excepta Insula Angleseya"/>
        <underline method="pen" type="straight" language="LA" text="Insula enim Angleseya"/>
        <underline method="pen" type="double" language="LA" text="concessa est Principi"/>
        <underline method="pen" type="straight" language="LA" text="Et si Princeps sine haerede de corpore suo moreretur, Insula illa in regis Angliae possessione remaneret."/>
        <underline method="pen" type="straight" language="LA" text="quinque Baronum"/>
        <underline method="pen" type="straight" language="LA" text="quia se Principem"/>
        <underline method="pen" type="straight" language="LA" text="aliquos Barones"/>
        <underline method="pen" type="straight" language="LA" text="&amp; suis haeredibus in aeternum"/>
        <underline method="pen" type="straight" language="LA" text="Princeps"/>
        <underline method="pen" type="straight" language="LA" text="habuit enim tres fratres"/>
        <underline method="pen" type="straight" language="LA" text="Ovvenum &amp; Rodericum"/>
        <underline method="pen" type="straight" language="LA" text="Dauid fuga"/>
        <underline method="pen" type="double" language="LA" text="morem gentis"/>
        <underline method="pen" type="straight" language="LA" text="factus in ista"/>
