    <page filename="397.tif" reader="John Dee" pagination="377" signature="Kk.iij."/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="ducis Eboraci, creatur comes de Rutland.">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Dux Eboraci - comes Rutland.</marginalia_text>
                    <person name="Edward of Norwich, 2nd Duke of York"/> <!-- M.B. Note refer to one person, the king created the Earl of Rutland for the Duke of York. -->
                </position>
            </language>
            <translation>Duke of York - Earl of Rutland.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de Lifland noluerunt diutius expectare. Facti sunt christiani de gente de Lettovv">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lucomia est Lifland Lettovv est Lithuania</marginalia_text>
                    <person name=""/>
                </position>
            </language>
            <translation>Lucomia is Lifland, Livonia is Lithuania.</translation> <!-- MB. Livonia correspond to a region covering both modern day Latvia and Estonia. Aside from Lithuania these are all regions that no longer have an exact modern equivalent. -->
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Eode[m] anno in parliamento praescripto, dominus Iohannes dux Lancastriae factus est dux Aquitanniae per virgam &amp; pileu[m] praebitis manu regis"/>
        <underline method="pen" type="straight" language="LA" text="filius &amp; haeres ducis Eboraci"/>
        <underline method="pen" type="straight" language="LA" text="comes de Rutland"/>
        <underline method="pen" type="straight" language="LA" text="VVyot"/>
        <underline method="pen" type="straight" language="LA" text="regis de Letovve"/>
        <underline method="pen" type="straight" language="LA" text="de Poleyn"/>
        <underline method="pen" type="straight" language="LA" text="de Lifland"/>
       
        
    </annotation>
