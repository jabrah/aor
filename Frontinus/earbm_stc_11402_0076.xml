        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>so Maharbal,</marginalia_text>
                    <person name="Maharbal"/>
                    <emphasis method="chalk" type="straight"/>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>l.2.ca.5.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>wine, w[i]th mandrage. So Clisthenes.</marginalia_text>
                    <person name="Cleisthenes"/>
                    <emphasis method="pen" type="straight" text="wine"/>
                    <emphasis method="pen" type="straight" text="mandrage"/>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>l.3.c.7.</marginalia_text>
                </position>
