        <marginalia hand="Italian">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>On notable Stratagem in extremity; to solace, &amp; recreate [the] Army, with sum pleasant Conceits,
                    &amp; delectable deuises: euen to make them merry, in [their] deepist dumpes.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Huc. +</marginalia_text>
                </position>
            </language>
            <translation>Here.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="a flaake of fyre, that fell from heuen">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>A falke of fijer fro[m] heauen.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>+ Huc Ars ben&egrave; Jocandi:</marginalia_text>
                </position>
                <position place="intext" book_orientation="0">
                    <marginalia_text>pra[e]sertim ex</marginalia_text>
                </position>
