        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Naturall Philosophy studd thes 3. in speciall steade.</marginalia_text>
                    <emphasis method="pen" type="straight" text="Naturall"/>
                    <emphasis method="pen" type="straight" text="Philosophy"/>
                    <emphasis method="chalk" type="straight"/>
                </position>
