        <marginalia hand="Italian">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>Great marches, &amp; suddain surprizes, sumtime of woonderfull valour.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="head" book_orientation="0">
