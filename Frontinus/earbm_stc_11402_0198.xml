        <underline method="pen" type="straight" language="EN" text="not withstandyng he hadde the vpper hande"/>
        <underline method="pen" type="straight" language="EN" text="he forgyue the punyshement"/>
        <underline method="pen" type="straight" language="EN" text="contention or intercession and"/>
        <underline method="pen" type="straight" language="EN" text="requeste of the souldiours"/>
        <underline method="pen" type="straight" language="EN" text="Rome pursued"/>
        <underline method="pen" type="straight" language="EN" text="remytte the dredefull punyshment"/>
        <underline method="pen" type="straight" language="EN" text="Fabius with his father"/>
        <underline method="pen" type="straight" language="EN" text="downe at his knees"/>
        <underline method="pen" type="straight" language="EN" text="senate &amp; people made intercession"/>
