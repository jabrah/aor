        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Quid pro quo.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>Temporal Sypplyes of defects.</marginalia_text>
                </position>
            </language>
            <translation>Something for something.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="they clipped womens heare and made ropes therof">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>Ropes of womens</marginalia_text>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>heare.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Better pore shifts, then none at all.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ca[e]sars bred of grasse.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Georgieuiz roots &amp; herbes, with A little salt.
                    Bred of stones rather than faile.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Alexander of Macedon; extreme thyrst">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Alexandri admirabilis in extrema siti te[m]perantia.</marginalia_text>
                    <person name="Alexander the Great"/>
                    <emphasis method="pen" type="straight" text="in extrema siti"/>
                </position>
            </language>
            <translation>The admirable temperance of Alexander, in extreme thrist.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Alexander of Macedon">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Alexander cowld</marginalia_text>
                    <person name="Alexander the Great"/>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>make A</marginalia_text>
                </position>
                <position place="intext" book_orientation="0">
                    <marginalia_text>Vertue of Necessity. A worthy Example</marginalia_text>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>to his men.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Durate, et Vosmet</marginalia_text>
                    <X-ref person="Virgil" book_title="Aeneid" text="Durate, et Vosmet rebus servate secundis"/> 
                </position>
                <position place="intext" book_orientation="0">
                    <marginalia_text>rebus seruate</marginalia_text>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>secundis.</marginalia_text>
                </position>
            </language>
            <translation>'Carry on and preserve yourselves for better times.'</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>to make [the] comminalty to enuy the[m] [the] more, and to</marginalia_text>
                </position>
                <position place="tail" book_orientation="0">
                    <marginalia_text>haue them in</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>maoiri Odio.</marginalia_text>
                </position>
            </language>
            <translation>To make [the] comminalty to enuy the[m] [the] more, and to haue them in greater hate.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Regni diuisi contra Seipsum, ruina.
                    Syncretismo nihil firmius, aut magis inuictum.</marginalia_text>
                </position>
            </language>
            <translation>The collapse of divided royal power against itself. Nothing is more firm or more invincible than syncretism.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="90">
                    <marginalia_text>Necessitas, maxim&egrave; omniu[m] Artiu[m] Polymechana, et Polyda[e]dala.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>New masteries, &amp; Experiments.</marginalia_text>
                </position>
            </language>
            <translation>Necessity, especially the polymechanics and [polydaedala??] of all arts.</translation> <!--JG: How to translate polydaedala??-->
        </marginalia>
        
