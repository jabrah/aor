    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Ha[e]c Honoris, Gloria[e]q[ue] Messis: Julio, Augustoq[ue] sudore, atq[ue]
                    sanguine importanda.</marginalia_text>
                </position>
            </language>
            <translation>These months of honor and glory: July and August, with sweat and by causing blood.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Petrachs</marginalia_text>
                    <person name="Petrarch"/>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Triumph of fame: more worth, then all [the] other fiue, 
                    for vse in [the] world.</marginalia_text>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vt nobilibus factis, dictisue inclarescas.</marginalia_text>
                </position>
            </language>
            <translation>So that you are illuminated by noble deeds and sayings.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Martiall Axiomes.</marginalia_text>
                    <person name="Martial"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>s[upr]a.</marginalia_text>
                </position>
            </language>
            <translation>[See] above.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="That what so euer is profytable to the, is hurtefull to thyne aduersarie">
           <language ident="LA">
               <position place="right_margin" book_orientation="0">
                   <marginalia_text>quod tibi proficit; hosti officit: et contra.</marginalia_text>
                   <emphasis method="pen" type="straight" text="quod tibi"/>
                   <emphasis method="pen" type="straight" text="hosti officit"/>
               </position>
           </language>
           <translation>What benefits you, hinders the enemy, and the other way around.</translation>
       </marginalia> 
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Regulae belloru[m] generales &egrave; Flaujo Vegetjo.</marginalia_text>
                    <person name="Vegetius"/>
                </position>
            </language>
            <translation>General rules of war by Flavius Vegetius.</translation>
        </marginalia> 
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>s[upr]a.</marginalia_text>
                </position>
            </language>
            <translation>[See] above.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>&Egrave; libro 3. Vegetij de Re Militari: cap. 26.</marginalia_text>
                    <person name="Vegetius"/>
                    <book title="De re Militari"/>
                </position>
            </language>
            <translation>From book 3 of Vegetius' 'De re militari', chapter 26.</translation>
        </marginalia>
        
