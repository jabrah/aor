        <underline method="pen" type="straight" language="EN" text="campe, that"/>
        <underline method="pen" type="straight" language="EN" text="the water was in"/>
        <underline method="pen" type="straight" language="EN" text="theyr ennemys, with"/>
        <underline method="pen" type="straight" language="EN" text="his fynger"/>
        <underline method="pen" type="straight" language="EN" text="Yonder you must fetche"/>
        <underline method="pen" type="straight" language="EN" text="incitation, he so moued"/>
        <underline method="pen" type="straight" language="EN" text="order the battayle by constancy"/>
        <underline method="pen" type="straight" language="EN" text="Seruius Tullius"/>
        <underline method="pen" type="straight" language="EN" text="fought nothinge"/>
        <underline method="pen" type="straight" language="EN" text="toke the standarde, and violently"/>
        <underline method="pen" type="straight" language="EN" text="flange it among his ennemies"/>
        <underline method="pen" type="straight" language="EN" text="ardently foughte to"/>
        <underline method="pen" type="straight" language="EN" text="standarde, that they"/>
