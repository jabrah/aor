        <marginalia hand="English_secretary" anchor_text="Anniball; arrayed lyke Romaynes; spake also latyne">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Annibals men, apparrelid lyke Romanes. And spoke Latin.</marginalia_text>
                    <person name="Hannibal"/>
                    <emphasis method="pen" type="straight" text="apparrelid"/>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ut supr&agrave; Iugur. L2.c.4.</marginalia_text>
                    <person name="Jugurtha"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="EN">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Hannibal, and Jugurth[a] could speeke Lattin, to searue these purposes.</marginalia_text>
                    <person name="Hannibal"/>
                    <person name="Jugurtha"/>
                    <emphasis method="pen" type="straight" text="Hannibal"/>
                    <emphasis method="pen" type="straight" text="Jugurth[a]"/>
                    <emphasis method="pen" type="straight" text="speeke Lattin"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="recule agayne">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>recoile, or reare</marginalia_text>
                </position>
            </language>
        </marginalia>
        
