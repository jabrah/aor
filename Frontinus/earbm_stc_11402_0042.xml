        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>Videbant[ur] Carthaginenses naues, no[n] Laconica[e].</marginalia_text>
                    <location name="Carthage"/>
                    <location name="Sparta"/>
                </position>
            </language>
            <translation>The ships seem to be Carthaginian, not Spartan.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>False Letters.</marginalia_text>
                </position>
