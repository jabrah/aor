        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In publicis causis, omnes debent esse Amici, et dextras coniungere.</marginalia_text>
                </position>
            </language>
            <translation>In public cases, all ought to be friends, and to join hands.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
