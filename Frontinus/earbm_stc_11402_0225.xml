    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Milciadis Tropha[e]a, non sinebant Themistoclem dormire, neq[ue] cessare.</marginalia_text>
                    <person name="Miltiades"/>
                    <person name="Themistocles"/>
                </position>
            </language>
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>Why shoold not [the] valiant, and</marginalia_text>
                </position>
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>fortunate Aduentures of Captain Drake by Sea, &amp; Captain Norrice by land,
                    worke the same effect in many Noble Inglish Harts?</marginalia_text>
                    <person name="Francis Drake"/>
                    <person name="John Norris"/>
                </position>
            </language>
            <translation>Miltiades' trophies did not allow Themistocles to sleep or to be inactive.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Megarensians">
            <language ident="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Athenians, in Megarensians shippes.</marginalia_text>
                    <emphasis method="pen" type="straight" text="Athenians"/>
                    <emphasis method="pen" type="straight" text="Megarensians shippes"/>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EN">
                <position place="tail" book_orientation="0">
                    <marginalia_text>A mixt Stratagem/</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="aide of a citie"/>
        <underline method="pen" type="straight" language="EN" text="to inuade"/>
        <underline method="pen" type="straight" language="EN" text="had preuented them"/>
        <underline method="pen" type="straight" language="EN" text="replenysshed the walles with"/>
        <underline method="pen" type="straight" language="EN" text="battaylle"/>
        <underline method="pen" type="straight" language="EN" text="at Marathon was foughte"/>
