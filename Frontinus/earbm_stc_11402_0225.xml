        <underline method="pen" type="straight" language="EN" text="women of Athens"/>
        <underline method="pen" type="straight" language="EN" text="wel reuenged their"/>
        <underline method="pen" type="straight" language="EN" text="gret number of the sayd"/>
        <underline method="pen" type="straight" language="EN" text="the same shyppes"/>
        <underline method="pen" type="straight" language="EN" text="taken of theirs"/>
        <underline method="pen" type="straight" language="EN" text="with souldiours of Athens"/>
        <underline method="pen" type="straight" language="EN" text="settynge oute in syghte"/>
