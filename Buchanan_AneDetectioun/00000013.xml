    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="him that the king"/>
        <underline method="pen" type="straight" language="EN" text="deadly hatrit"/>
        <underline method="pen" type="straight" language="EN" text="tooke it so displeasantly"/>
        <underline method="pen" type="straight" language="EN" text="in her grace"/>
        <underline method="pen" type="straight" language="EN" text="determinit, so"/>
        <underline method="pen" type="straight" language="EN" text="opportunitie serued"/>
        <underline method="pen" type="straight" language="EN" text="to murder"/>
        <underline method="pen" type="straight" language="EN" text="him"/>
        <underline method="pen" type="straight" language="EN" text="he should choose"/>
        <underline method="pen" type="straight" language="EN" text="some yong gentlewoman, quhairof"/>
        <underline method="pen" type="straight" language="EN" text="thare was great store quhose"/>
        <underline method="pen" type="straight" language="EN" text="he might use in the meane time"/>
        <underline method="pen" type="straight" language="EN" text="baude to"/>
        
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="informing him that the
            king boyled in such deadly hatrit against him, and tooke it so displeasantly that he stood so hyely in
            her grace, that he was fully determinit, so soome as any possible opportunitie serued, to murder him."/>
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="he should
            choose him some yon gentlewoman, quhairf thare was great store, quhose companie he 
            might use in the meane time."/>
        
    </annotation>
