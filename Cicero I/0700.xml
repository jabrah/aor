        <underline method="pen" type="straight" language="LA" text="maxim&eacute;que illo libro augurali, quem"/>
        <underline method="pen" type="straight" language="LA" text="Nam cum teipsum, ex quo te[m]pore"/>
        <underline method="pen" type="straight" language="LA" text="coepisti, quotidie pluris feci, tum accesserunt etiam coniunctiones necessariorum"/>
        <underline method="pen" type="straight" language="LA" text="enim duarum aetatum"/>
        <underline method="pen" type="straight" language="LA" text="mediocre vinculum mihi quidem attulisse videtur ad voluntates nostras copula[n]das"/>
        <underline method="pen" type="straight" language="LA" text="ex illius"/>
        <underline method="pen" type="straight" language="LA" text="fide, commemoratio officiorum superuacanea est"/>
        <underline method="pen" type="straight" language="LA" text="Animaduerti enim, &amp; didici ex tuis"/>
        <underline method="pen" type="straight" language="LA" text=", &amp; praeparares"/>
        <underline method="pen" type="straight" language="LA" text="ratio facilior, &amp; solutior"/>
        <underline method="pen" type="straight" language="LA" text="vt ipse tu"/>
        <underline method="pen" type="straight" language="LA" text="neque enim obscuris personis, nec paruis in causis res agetur. Sed haec fieri, melius qu&agrave;m dici, aut scribi"/>
        <underline method="pen" type="straight" language="LA" text="in dubitationem videtur adducere, visurusne"/>
        <underline method="pen" type="straight" language="LA" text="veni in eum sermonem"/>
        <underline method="pen" type="straight" language="LA" text="&amp; perfore accommodatum"/>
        <underline method="pen" type="straight" language="LA" text="nauibus accessissem"/>
        <underline method="pen" type="straight" language="LA" text="fore ad meum aduentum"/>
        <underline method="pen" type="straight" language="LA" text="dum pecunia accipitur quae mihi ex publica premutatione debetur, co[m]morabor"/>
        <underline method="pen" type="straight" language="LA" text="quod commodo tuo fieri possit"/>
        <underline method="pen" type="straight" language="LA" text="Quam facultatem"/>
        <underline method="pen" type="straight" language="LA" text="fu&iacute;tque mecum familiariter triduum illud quod"/>
        
