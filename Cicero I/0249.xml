        <marginalia hand="English_secretary" anchor_text="Venio nun ad illud tuum, Non deieci">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        venio ad illud tuu[m] no[n] deieci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I come now to your one. I didn't drive him out.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="optio detur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        optione[m] dari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be offered the choice.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="verbis &amp; literis, &amp; (vt dici solet) summo iure contenditur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        verbis &amp; litteris co[n]tendere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To appeal to the word and letters.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aucupia verborum &amp; literaru[m] tendiculas">
