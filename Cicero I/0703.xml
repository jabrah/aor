    <annotation> 
        
        <underline method="pen" type="straight" language="LA" text="ex superiore &amp; ex aequo loco sermones habitos"/>
        <underline method="pen" type="straight" language="LA" text="egentissimarum"/>
        <underline method="pen" type="straight" language="LA" text="imminutione"/>
        <underline method="pen" type="straight" language="LA" text="sumptus discerni"/>
        <underline method="pen" type="straight" language="LA" text="cum soluendo ciuitates non essent"/>
        <underline method="pen" type="straight" language="LA" text="vrbanum"/>
        <underline method="pen" type="straight" language="LA" text="Claudium S.P.Q.R."/>
        <underline method="pen" type="straight" language="LA" text="meminisse vllum tempus"/>  
        <underline method="pen" type="straight" language="LA" text="me siqui suo sumptu functus esset officio: concessuru[m] si legitimo: non permissurum, si infinito."/>
        <underline method="pen" type="straight" language="LA" text="facere ii qui haec disputant"/>
        <underline method="pen" type="straight" language="LA" text="composui edictum"/>
        <underline method="pen" type="straight" language="LA" text="caput est"/>
        <underline method="pen" type="straight" language="LA" text="nova, salutaria"/>
        <underline method="pen" type="straight" language="LA" text="quo suspicio nata est"/>
        <underline method="pen" type="straight" language="LA" text="tralatitium est"/>
        <underline method="pen" type="straight" language="LA" text="tam despie[n]s"/>
        <underline method="pen" type="straight" language="LA" text="id est in senatu"/>
        <underline method="pen" type="straight" language="LA" text="iniussu meo"/>
        <underline method="pen" type="straight" language="LA" text="illaru[m] dioecesium"/>
        <underline method="pen" type="straight" language="LA" text="quae cis Taurum"/>
        <underline method="pen" type="straight" language="LA" text="coeperunt legare, quam"/>
        <underline method="pen" type="straight" language="LA" text="aut minuendo, aut remittendo"/>
        <underline method="pen" type="straight" language="LA" text="venditionem tributorum, &amp; illam acerbissima[m] exactionem"/>
        <underline method="pen" type="straight" language="LA" text="leuarem miseriis perditas ciuitates, &amp; perditas maxime per magistratus"/>
        <underline method="pen" type="straight" language="LA" text="sermones ad te delati de me sunt, non debuisti credere. Se aute[m] hoc genere delectaris, vt quae tibi in mentem veniant, aliis attributas"/>
        <underline method="pen" type="straight" language="LA" text="genus sermonis inducis in amicitiam minime liberale."/>
        <underline method="pen" type="straight" language="LA" text="neque ad praefectum fabrum"/>
        <underline method="pen" type="straight" language="LA" text="doctissimis hominibus authoribus"/>
        <underline method="pen" type="straight" language="LA" text="de amicitia gere[n]da"/>
        <underline method="pen" type="straight" language="LA" text="DISPUTABANT, EGO CONTRA DISSEREBAM. DICEBANT, EGO NEGABAM. An mihi"/>
        <underline method="pen" type="straight" language="LA" text="meus co[n]uentus erat Apameae"/>
        <underline method="pen" type="straight" language="LA" text="No[n] dicam plura, ne in quo te obiurgem, idipsum videar imitari."/>  
        
